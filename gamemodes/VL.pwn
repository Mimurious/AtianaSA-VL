cmd:eject(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/eject <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);

		new vid = GetPlayerVehicleID(playerid);
		if(GetPlayerState(playerid) != 2) return SendEngMessage(playerid, COLOR_ERROR, "Error: You are not driver."), SendPerMessage(playerid, COLOR_ERROR, "Shoma ranande nistid.");
		if(!IsPlayerInVehicle(giveplayerid, vid)) return SendEngMessage(playerid, COLOR_ERROR, "Player is not in your vehicle."), SendPerMessage(playerid, COLOR_ERROR, "In bazikon dar vasileye naghlie shoma nist.");

		format(gString, sizeof(gString), "You have ejected player %s from your vehicle.", GetName(giveplayerid));
		SendEngMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Shoma %s ra az vasileye naghlie khod biron andakhtid.", GetName(giveplayerid));
		SendPerMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "You have been ejected from vehicle by %s.", GetName(playerid));
		SendEngMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Shoma az vasileye naghlie %s biron andakhte shodid.", GetName(playerid));
		SendPerMessage(giveplayerid, COLOR_SYN2, gString);

		RemovePlayerFromVehicleEx(giveplayerid);
	}
	return 1;
}

function RemovePlayerFromVehicleEx(playerid)
{
	if(IsPlayerInAnyVehicle(playerid)) {
		new Float:cx, Float:cy, Float:cz, Float:ca, vehid[MAX_PLAYERS] = INVALID_VEHICLE_ID;
		vehid[playerid] = GetPlayerVehicleID(playerid);
		GetVehiclePos(vehid[playerid], cx, cy, cz);
		GetVehicleZAngle(vehid[playerid], ca);
		RemovePlayerFromVehicle(playerid);
		vtime[playerid] = SetTimerEx("RemovePlayerFromVehicleExEx", 2000, false, "iiffff", playerid, vehid[playerid], cx, cy, cz, ca);
	}
	return 1;
}

function RemovePlayerFromVehicleExEx(playerid, vehicleid, Float:cx, Float:cy, Float:cz, Float:ca)
{
	if(IsPlayerInAnyVehicle(playerid)) {
		RemovePlayerFromVehicle(playerid);
		SetPlayerPosEx(playerid, cx, cy, cz);
		SetVehiclePos(vehicleid, cx, cy, cz);
		SetVehicleZAngle(vehicleid, ca);
		vtime[playerid] = SetTimerEx("RemovePlayerFromVehicleEx", 2000, false, "i", playerid);
	}
}

