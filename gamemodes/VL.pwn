function MySQL_Init()
{
    new arrCoords[5][128];
	new strFromFile2[128];
	new File: file = fopen("database.ini", io_read);
	new hostname[255], port[255], username[255], password[255], dbname[255];
	if (file)
	{
		fread(file, strFromFile2);
		split(strFromFile2, arrCoords, '|');
		strmid(hostname, arrCoords[0], 0, strlen(arrCoords[0]), 255);
		strmid(port, arrCoords[1], 0, strlen(arrCoords[1]), 255);
		strmid(username, arrCoords[2], 0, strlen(arrCoords[2]), 255);
		strmid(password, arrCoords[3], 0, strlen(arrCoords[3]), 255);
		strmid(dbname, arrCoords[4], 0, strlen(arrCoords[4]), 255);
		fclose(file);
	}
	else
	{
		fopen("database.ini", io_write);
		fclose(file);
	}

	new MySQLOpt: option_id = mysql_init_options();
	mysql_set_option(option_id, AUTO_RECONNECT, true);
    mysql_set_option(option_id, SERVER_PORT, strval(port));

	Database = mysql_connect(hostname, username, password, dbname, option_id);

	if(Database == MYSQL_INVALID_HANDLE || mysql_errno(Database) != 0)
	{
		print("*------------------- Server SQL Error -------------------*");
		print("* Server has experienced an internal SQL error and cannot continue.");
		print("* SQL Error: 2002 - No connection could be made because the target machine actively refused it.");
		print("* Query: [READ] Unable to connect to MySQL server");
		print("*------------------- Server SQL Error -------------------*");
		SendRconCommand("gmx");
	}
}

