public OnRconLoginAttempt(ip[],password[],success)
{
    new Ip[16];
    foreach(Player, i)
    {
        GetPlayerIp(i,Ip,sizeof(Ip));
        if(!strcmp(ip,Ip))
        {
            if(PlayerInfo[i][pSQLID] != 1) {
                format(aString, sizeof(aString), "* %s has been kicked from the server (%s).", GetName(i), "Trying to RCON login");
				HBroadCast(COLOR_YELLOW, aString, 1);
				return BanEx(i, "RCON LOGIN ATTEMPTS");
			}
        }
    }
    return 1;
}

stock YoutubeStreamForPlayer(playerid, vlink[])
{
	new videoid[128],mp3link[128];
	strmid(videoid,vlink,strfind(vlink,"v=")+2,strfind(vlink,"v=")+15);
	format(mp3link,128,"http:");
	PlayAudioStreamForPlayersInCar(GetPlayerVehicleID(playerid),mp3link);
	return 1;
}

stock PlayAudioStreamForPlayersInCar(vehicleid,url[])
{
	foreach(Player,i)
	{
		if(IsPlayerInAnyVehicle(i))
		{
			if(GetPlayerVehicleID(i) == vehicleid)
			{
				PlayAudioStreamForPlayer(i,url);
			}
		}
	}
	return 1;
}


