cmd:unrentveh(playerid, params[]) {
	if(RentVehicle[playerid] == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_ERROR, "Error: You have not rented any vehicle.");
	if(IsPlayerInVehicle(playerid, RentVehicle[playerid])) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be out of the vehicle.");
	DestroyVehicleEx(RentVehicle[playerid], 24989);
	RentVehicle[playerid] = INVALID_VEHICLE_ID;
	RentTimer[playerid] = 0;
	RentKM[playerid] = 0;
	SendClientMessage(playerid, COLOR_SYN2, "You have unrented your rented vehicle sucessfully.");
	if(CheckPoint[playerid] == 1555) {
	    DisablePlayerCheckpoint(playerid);
		CheckPoint[playerid] = 0;
	}
	return 1;
}

cmd:findrentveh(playerid, params[]) {
	if(RentVehicle[playerid] == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_ERROR, "Error: You have not rented any vehicle.");
	if(IsPlayerInVehicle(playerid, RentVehicle[playerid])) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are already in your rented vehicle, there is no need to locate it.");
	new Float:x, Float:y, Float:z;
	GetVehiclePos(RentVehicle[playerid], x, y, z);
	SetPlayerCheckpointEx(playerid, x, y, z, 4.0);
	CheckPoint[playerid] = 1;
	SendClientMessage(playerid, COLOR_SYN2, "We have put a checkpoint on the location of your rented vehicle, please follow it to find your vehicle.");
	return 1;
}

cmd:givekey(playerid, params[]) {
    new giveplayerid, splayer[25];
    new id,giveplayer[30],sendername[30],string[128],idcar=-1;
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/givekey <Player ID/Name>");
	if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
	else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
	else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
	else giveplayerid = GetPlayers(splayer);
	if(playerid == giveplayerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You already have your keys.");
	if(HireCar[giveplayerid] == GetPlayerVehicleID(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: That player already has your keys.");
	for(new vv; vv < MAX_PERSONAL_VEHICLES; vv++)
	{
		if(PlayerInfo[playerid][pCarID][vv] == GetPlayerVehicleID(playerid)) {
			idcar = vv;
		}
	}
	if(idcar == -1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be inside your vehicle.");
	if(ProxDetectorS(5.0, playerid, giveplayerid)) {
		HireCar[giveplayerid] = GetPlayerVehicleID(playerid);
		GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
		GetPlayerName(playerid, sendername, sizeof(sendername));
		format(string, sizeof(string), "* %s takes out a set of keys, and tosses them to %s.", sendername ,giveplayer);
		ProxDetector(30.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
		SetVehicleParamsForPlayer(HireCar[id],giveplayerid,0,0);
	}
	else return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not near of this player.");
	return 1;
}
cmd:changelock(playerid, params[]) {
	new idcar = -1;
	for(new vv; vv < MAX_PERSONAL_VEHICLES; vv++)
	{
		if(PlayerInfo[playerid][pCarID][vv] == GetPlayerVehicleID(playerid)) {
			idcar = vv;
		}
	}
	if(idcar == -1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be inside your vehicle.");
	foreach(new i : Player)
	{
		if(PlayerInfo[playerid][pCarID][idcar] == HireCar[i]) {
			SetVehicleParamsForPlayer(HireCar[i],i,0,1);
			HireCar[i] = -1;
			SendClientMessage(i, COLOR_SYN3, "The lock of the vehicle you had the keys for have been changed.");
		}
	}
	SendClientMessage(playerid, COLOR_SYN3, "The lock of the vehicle you had the keys for have been changed, no one has keys to your vehicle now.");
	return 1;
}

cmd:removetunings(playerid, params[])
{
    new count,carid = GetPlayerVehicleID(playerid),Float:PosX,Float:PosY,Float:PosZ;
	if(IsPlayerConnected(playerid))
	{
		if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You aren't in any vehicle.");
		for(new v; v < MAX_PERSONAL_VEHICLES; v++)
		{
			if(PlayerInfo[playerid][pCarID][v] == carid) {
				PlayerInfo[playerid][pCarPaintJ][v] = 6;
				for(new s; s < 17; s++) {
					CarMod[playerid][v][s] = 0;
				}
				cUpdate(playerid, v, cModsx);
				cUpdate(playerid, v, cPaintJx);
				GetVehiclePos(PlayerInfo[playerid][pCarID][v], PosX, PosY, PosZ);
				SetVehicleToRespawnEx2(PlayerInfo[playerid][pCarID][v]);
				SetVehiclePos(PlayerInfo[playerid][pCarID][v], PosX, PosY, PosZ);
				count++;
			}
		}
		if(count == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You aren't in any vehicle.");
		else SendClientMessage(playerid, COLOR_SYN3, "Your vehicle has been respawned and all tuning parts have been removed.");
	}
	return 1;
}

