cmd:setnickname(playerid, params[])
{
	new giveplayerid, splayer[25], name[25], name2[25], nwt[25];
	if(PlayerInfo[playerid][pAdmin] < 7) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[25]s[25]", splayer, name)) return SendSyntaxMessage(playerid, "/setnickname <Player ID/Name> <New Nickname>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);

		strmid(nwt, name, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		format(name2, sizeof name2, "%s", name);
		if(strmatch(GetName(giveplayerid), "Mohammad")) return SendClientMessage(playerid, COLOR_ERROR, "Error: This name is reserved.");
		if(strmatch(name, "Mohammad")) return SendClientMessage(playerid, COLOR_ERROR, "Error: This name is reserved.");
		new clan = PlayerInfo[giveplayerid][pCMember] + PlayerInfo[giveplayerid][pCLeader];
		if(clan != 0)
		{
			if(ClanInfo[clan][cTagType] == 0) strins(name, ClanInfo[clan][cTag], 0);
			else strins(name, ClanInfo[clan][cTag], strlen(name));
			if(strlen(name) > 20) return SendClientMessage(playerid, COLOR_ERROR, "Name is to long with tag.");
		}
		if(strlen(name) > 20) return SendClientMessage(playerid, COLOR_ERROR, "Name is to long.");
		if(ExistName(name)) return SendClientMessage(playerid, COLOR_ERROR, "Name already exist.");
		format(gString, sizeof(gString), "%s has changed %s's nickname to %s.",GetName(playerid), GetName(giveplayerid), name);
		CMDRaport(gString, 1, 0);

		if(PlayerInfo[giveplayerid][pHouseKey] > 0)
		{
			strmid(HouseInfo[PlayerInfo[giveplayerid][pHouseKey]][hOwner], name2, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			hUpdate(PlayerInfo[giveplayerid][pHouseKey], hOwnerx);
			HouseUpdate(PlayerInfo[giveplayerid][pHouseKey]);
		}

		if(PlayerInfo[giveplayerid][pBizzKey] > 0)
		{
			strmid(BizzInfo[PlayerInfo[giveplayerid][pBizzKey]][bOwner], name2, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			bUpdate(PlayerInfo[giveplayerid][pBizzKey], bOwnerx);
			BizzUpdate(PlayerInfo[giveplayerid][pBizzKey]);
		}

		gQuery[0] = (EOS);
		mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Name`='%e' WHERE `ID`=%d", nwt, PlayerInfo[giveplayerid][pSQLID]);
		mysql_query(Database, gQuery);
		SetPlayerName(giveplayerid, name);
		format(PlayerInfo[giveplayerid][pUsername], 25, nwt);
		format(GetName(giveplayerid), 32, "%s", nwt);
		format(gString, sizeof(gString), "(%d) %s~n~SA.Atiana.IR", giveplayerid, GetName(giveplayerid));
		PlayerTextDrawSetString(giveplayerid, NameText[giveplayerid], gString);
		if(!TOG[6][giveplayerid]) PlayerTextDrawShow(giveplayerid, NameText[giveplayerid]);
	}
	return 1;
}

