		}

		SendEngMessage(playerid, COLOR_WHITE,"{FFFF45}Your report request has been sent to the admins."), SendPerMessage(playerid, COLOR_WHITE,"{FFFF45}Darkhaste gozareshe shoma baraye barresi be modiran ersal shod.");
