				SendEngMessage(i, COLOR_LIGHTRED, gString);
				format(gString,sizeof(gString),"Shoma modat zamane morede niaz baraye daryafte in PayCheck ra online naboodid.",PlayerInfo[i][pPayDay]);
				SendPerMessage(i, COLOR_LIGHTRED, gString);
				continue;
			}

			if(PlayerInfo[i][pPremiumAccount] > gettime()  || PlayerInfo[i][pPremiumAccount] == -1) { interest = 5; }
			else { interest = 3; }

			if(AFK[i] == 1) PlayerInfo[i][pPayCheck] /= 8;

			if(PlayerInfo[i][pPremiumAccount] > gettime() || PlayerInfo[i][pPremiumAccount] == -1) PlayerInfo[i][pPayCheck] = PlayerInfo[i][pPayCheck] + PlayerInfo[i][pPayCheck]/2;

			new house = PlayerInfo[i][pRentKey];
			new rent;
			if(house != 0)
			{
				rent = HouseInfo[house][hRent];
				HouseInfo[house][hSafe] += rent;
				hUpdate(house, hSafex);
			}

			new paycheck = PlayerInfo[i][pPayCheck] / 2;
			new bankacc = PlayerInfo[i][pAccount];
			new interestm = bankacc * interest/100000;
			if(AFK[i] == 1) interestm /= 6;
			new stateimposit;

			stateimposit += interestm/100;

			PlayerInfo[i][pAccount] = bankacc + paycheck + interestm - stateimposit- rent - Tax;
			Update(i,pAccountx);

			PlayerInfo[i][pTutVeh] += 1;
			Update(i,pTutVehx);

			format(gString, sizeof(gString), "|_______________________ [Paycheck Information] _______________________|");
			SendEngMessage(i, -1, gString);
			format(gString, sizeof(gString), "|_______________________ [Barresi va Pardakht] _______________________|");
			SendPerMessage(i, -1, gString);
			format(gString, sizeof(gString), "You received {00FF00}+$%s{BABABA} as your paycheck and {00FF00}+$%s{BABABA} as your bank account interest (0.00%d%s).", FormatNumber(paycheck), FormatNumber(interestm), interest, "%%");
			SendEngMessage(i, 0xBABABAFF, gString);
			format(gString, sizeof(gString), "Shoma {00FF00}+$%s{BABABA} be onvane paychecke khod va {00FF00}+$%s{BABABA} be onvane sude bankie khod (0.00%d%s) daryaft kardid.", FormatNumber(paycheck), FormatNumber(interestm), interest, "%%");
			SendPerMessage(i, 0xBABABAFF, gString);
