function ClearCrimes(playerid)
{
	strmid(PlayerCrimes[0][playerid], "None", 0, 32), PlayerCrimesW[0][playerid] = 0, UpdateCrime(playerid, 1);
	strmid(PlayerCrimes[1][playerid], "None", 0, 32), PlayerCrimesW[1][playerid] = 0, UpdateCrime(playerid, 2);
	strmid(PlayerCrimes[2][playerid], "None", 0, 32), PlayerCrimesW[2][playerid] = 0, UpdateCrime(playerid, 3);
	strmid(PlayerCrimes[3][playerid], "None", 0, 32), PlayerCrimesW[3][playerid] = 0, UpdateCrime(playerid, 4);
	return 1;
}

function AddCrime(playerid, reason[], wanted)
{
	if(wanted > PlayerCrimesW[0][playerid]) strmid(PlayerCrimes[0][playerid], reason, 0, 32), PlayerCrimesW[0][playerid] = wanted, UpdateCrime(playerid, 1);
	else if(wanted > PlayerCrimesW[1][playerid]) strmid(PlayerCrimes[1][playerid], reason, 0, 32), PlayerCrimesW[1][playerid] = wanted, UpdateCrime(playerid, 2);
	else if(wanted >= PlayerCrimesW[2][playerid]) strmid(PlayerCrimes[2][playerid], reason, 0, 32), PlayerCrimesW[2][playerid] = wanted, UpdateCrime(playerid, 3);
	else if(wanted >= PlayerCrimesW[3][playerid]) strmid(PlayerCrimes[3][playerid], reason, 0, 32), PlayerCrimesW[3][playerid] = wanted, UpdateCrime(playerid, 4);
	return 1;
}

