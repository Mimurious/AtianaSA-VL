	return 1;
}

cmd:charity(playerid, params[])
{
	new charity;
	if(sscanf(params,"i", charity)) return SendSyntaxMessage(playerid, "/charity <Amount ($+)>");
	{
		if(MoneyACheck(playerid, charity, 1, 1000000)) return 1;
		if(GetPlayerCash(playerid) < charity) return SendEngMessage(playerid, COLOR_ERROR, "Error: You don't have enough money."), SendPerMessage(playerid, COLOR_ERROR, "Error: Shoma poole kaafi nadarid.");
		format(gString, sizeof(gString), "Thank you for your donation of $%s.", FormatNumber(charity));
		SendEngMessage(playerid,COLOR_SYN2,gString);
		format(gString, sizeof(gString), "Az $%s komake malie shoma moteshakerim.", FormatNumber(charity));
		SendPerMessage(playerid,COLOR_SYN2,gString);
		GivePlayerCash(playerid, -charity);
		CharityMoney += charity;
		gQuery[0] = (EOS);
		mysql_format(Database, gQuery, sizeof(gQuery),"UPDATE `others` SET `Charity`=%d WHERE `ID`='1'", CharityMoney);
		mysql_query(Database, gQuery);
	}
	return 1;
}

cmd:settax(playerid, params[])
{
	new taxx;
	if(PlayerInfo[playerid][pLeader] != 3)  return SendEngMessage(playerid, COLOR_ERROR, "Error: You need to be mayor."), SendPerMessage(playerid, COLOR_ERROR, "Shoma mayor nistid.");
	if(sscanf(params,"i", taxx)) return SendSyntaxMessage(playerid, "/settax <Amount ($+)>");
	{
		if(MoneyACheck(playerid, taxx, 1, 10)) return 1;
		format(gString, sizeof(gString), "Tax is now $%s.", FormatNumber(taxx));
		SendEngMessage(playerid,COLOR_SYN2,gString);
		format(gString, sizeof(gString), "Maliyat bar ruye $%s tanzim shod.", FormatNumber(taxx));
		SendPerMessage(playerid,COLOR_SYN2,gString);
		Tax = taxx;
		gQuery[0] = (EOS);
		mysql_format(Database, gQuery, sizeof(gQuery),"UPDATE `others` SET `Tax`=%d WHERE `ID`='1'", Tax);
		mysql_query(Database, gQuery);
	}
	return 1;
}

