public OnPlayerFinishedDownloading(playerid, virtualworld)
{
	if(!gLogged[playerid]) {
	    GameTextForPlayer(playerid, "~y~LOADING ACCOUNT INFORMATION...", 1500, 6);
		gQuery[0] = (EOS);
		mysql_format(Database, gQuery, sizeof(gQuery), "SELECT `Status`, `Name` FROM `users` WHERE `Name` = '%s' AND `Status` = 0", GetName(playerid));
		mysql_tquery(Database, gQuery, "WhenOnlineCheck", "i", playerid);
		
		CancelSelectTextDraw(playerid);
		TogglePlayerSpectating(playerid, false);
	}
    return 1;
}

