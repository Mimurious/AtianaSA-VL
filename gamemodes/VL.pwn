cmd:getjob(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid)) return SendEngMessage(playerid, COLOR_ERROR, "Error: You must be on foot to use this command."), SendPerMessage(playerid, COLOR_ERROR, "Khata: Shoma nabayad savare vasileye naghliyeii bashid.");
    if(PlayerInfo[playerid][pJob] != 0)
	{
	    PlayerInfo[playerid][pJob] = 0;
		Update(playerid,pJobx);
		if(JobWorking[playerid] == 1)
		{
			JobWorking[playerid] = 0;
			if(JobVehicle[playerid] != 0) { DestroyVehicleEx(JobVehicle[playerid], 13354); JobVehicle[playerid] = 0; }
			DisablePlayerCheckpoint(playerid);
			JobVehicle[playerid] = 0;
			CheckPoint[playerid] = 0;
			CCP[playerid] = false;
			HideMowerObjects(playerid);
			HideFarmerObjects(playerid);
			HideDealerObjects(playerid);
			DestroyVehicleEx(FarmTrailer[playerid], 13362);
			if(Trailer[playerid] != 0) { DestroyVehicleEx(Trailer[playerid], 13363); Trailer[playerid] = 0; TruckerRoute[playerid] = 15; }
			PlayerTextDrawHide(playerid, TruckTD[playerid]);
			DetectivWorking[playerid] = 0;
			DestroyDynamicPickup(DetectivObj[playerid]);
			PlayerTextDrawHide(playerid, DetectivTD[playerid]);
		}
		DisablePlayerCheckpoint(playerid);
		CheckPoint[playerid] = 0;
	}
	for(new i = 0; i < MAX_JOBS; i++)
	{
		if(PlayerToPoint(2.0, playerid, JobInfo[i][jPosx], JobInfo[i][jPosy], JobInfo[i][jPosz]))
		{
			if(PlayerInfo[playerid][pLevel] < JobInfo[i][jLevel]) return SendEngMessage(playerid, COLOR_ERROR, "Error: Your level isn't high enough to get this job."), SendPerMessage(playerid, COLOR_ERROR, "Khata: Sathe shoma baraye gereftane in shoghl kaafi nist.");
			PlayerInfo[playerid][pJob] = JobInfo[i][jID];
			Update(playerid,pJobx);
			DisablePlayerCheckpoint(playerid);
			UseFind[playerid] = 0;
			Finding[playerid] = INVALID_PLAYER_ID;
			CheckPoint[playerid] = 0;
			CCP[playerid] = false;
			if(PlayerInfo[playerid][pLanguage] == 0)
			{
				format(gString, sizeof(gString), "* You are now a(n) %s.", JobInfo[i][jName]);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				SendClientMessage(playerid, COLOR_SYN2, "* Type /help to see the job available commands.");
			}
			else if(PlayerInfo[playerid][pLanguage] == 1)
			{
				format(gString, sizeof(gString), "* Shoma ham aknun shoghle %s ra entekhab kardid.", JobInfo[i][jFName]);
				SendClientMessage(playerid, COLOR_SYN2, gString);
				SendClientMessage(playerid, COLOR_SYN2, "* Baraye didane dasturate in shoghl az /help estefade konid.");
			}
			break;
		}
	}
	return 1;
}
