function UpdateFPS(playerid)
{
	new string[128];
	if(HUD[2][playerid] == 1)
	{
		new drunk2 = GetPlayerDrunkLevel(playerid);
		if(drunk2 < 100) SetPlayerDrunkLevel(playerid,2000);
		else
		{
			if(LastDrunkLevel[playerid] != drunk2)
			{
				new fps = LastDrunkLevel[playerid] - drunk2;

				if((fps > 0) && (fps < 200)) FPS2[playerid] = fps;
				LastDrunkLevel[playerid] = drunk2;
			}
		}
		format(string,sizeof(string),"%d",FPS2[playerid]-1);
		PlayerTextDrawSetString(playerid, HUD4[playerid] ,string);
		PlayerTextDrawShow(playerid, HUD4[playerid]);
		if(FPS2[playerid]-1 < 20)
		{
			format(string,sizeof(string),"~r~%d",FPS2[playerid]-1);
			PlayerTextDrawSetString(playerid, HUD4[playerid] ,string);
			PlayerTextDrawShow(playerid, HUD4[playerid]);
		}
	}
	if(HUD[0][playerid] == 1)
	{
		new Float:hp;
		GetPlayerHealthEx(playerid, hp);
		format(string,sizeof(string)," %.0f",hp);
		PlayerTextDrawSetString(playerid, HUD1[playerid] ,string);
		PlayerTextDrawShow(playerid, HUD1[playerid]);
	}
	if(HUD[1][playerid] == 1)
	{
		new Float:ap;
		GetPlayerArmourEx(playerid, ap);
		format(string,sizeof(string)," %.0f",ap);
		PlayerTextDrawSetString(playerid, HUD3[playerid] ,string);
		PlayerTextDrawShow(playerid, HUD3[playerid]);
	}
	return 1;
}

