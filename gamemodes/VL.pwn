cmd:sms(playerid, params[])
{
	new
	smstext[126],
	tnum;
	if(PlayerInfo[playerid][pPhoneNumber] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have cell phone.");
	if(PlayerInfo[playerid][pMuted] == 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot speak, you have been silenced.");
	if(!IsPlayerSpawned(playerid)) {
	    SendEngMessage(playerid, COLOR_LIGHTRED, "You cannot do any action when you are are dead.");
		SendPerMessage(playerid, COLOR_LIGHTRED, "Shoma dar hengami ke mordid tavanayie anjam dadane kari ra nadarid.");
		return 0;
	}
	if(sscanf(params, "is[126]", tnum,smstext)) return SendSyntaxMessage(playerid, "/sms <Phone Number> <Text>");

	format(gString, sizeof(gString), "* %s takes out a cellphone.", GetName(playerid));
	ProxDetector(20.0, playerid, gString,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
	new sender = PlayerInfo[playerid][pPhoneNumber];

	new found = -1;
	foreach(new i : Player)
	{
		if(PlayerInfo[i][pPhoneNumber] == tnum && tnum != 0)
		{
			found = i;
		}
	}
	if(found >= 0)
	{
	    if(FindAdvertise(smstext) == 2) return BanAdPlayer(playerid);
	    if(FindAdvertise(smstext))
		{
			format(gString, sizeof(gString), "{00FF00}[AD]{FFFFFF} %s (SMS to %s): {FF0000}%s", GetName(playerid), GetName(found), smstext);
			ABroadCast(COLOR_LIGHTRED, gString, 1);
		}
		ChangeSwear(smstext);

		if(OnOff[found] == 0 || SpectatorID[found] != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_ERROR, "Player's cellphone is turned off.");
		if(GetPlayerCash(playerid) < 10) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");

		format(gString, sizeof(gString), "SMS: {ffffff}%s{FFFF00}, from: %s (%s)", smstext,GetName(playerid),PhoneFormat(sender));
		SendClientMessage(found, COLOR_YELLOW, gString);
		PlayerPlaySound(found, 1052, 0.0, 0.0, 0.0);
		format(gString, sizeof(gString), "SMS: {ffffff}%s{FFFF00}, to: %s (%s)", smstext,GetName(found),PhoneFormat(tnum));
		SendClientMessage(playerid, COLOR_YELLOW, gString);
		LastSMS[found] = playerid;

		format(gString, sizeof(gString), "SMS from %s to %s: {ffffff}%s", GetName(playerid), GetName(found), smstext);
		foreach(new i : Player)
		{
			if(!ShowSMS[i])
				continue;

			SendClientMessage(i, COLOR_YELLOW, gString);
		}

		PhoneLog(playerid, found, smstext, 1);
		GivePlayerCash(playerid, -5*BizzInfo[8][bItemPrice]);
		format(gString, sizeof(gString), "~r~-$%d", 5*BizzInfo[8][bItemPrice]);
		GameTextForPlayer(playerid, gString, 5000, 1);

		BizzInfo[8][bSafe] += 5*BizzInfo[8][bItemPrice];
		bUpdate(8, bSafex);
		BizzPayLog(playerid, 8, 5*BizzInfo[8][bItemPrice]);
	}
	else
	{
		SendClientMessage(playerid, COLOR_ERROR, "Message delivery failed.");
		return 1;
	}
	return 1;
}

