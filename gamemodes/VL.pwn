	if(PlayerInfo[playerid][pAdmin] < 1) return NotEnoughPermission(playerid);
	if(sscanf(params,"f",carid)) return SendSyntaxMessage(playerid, "/front <Amount (Float)>");
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			new Float:px, Float:py, Float:pz, Float:pa;
			new vehicleid = GetPlayerVehicleID(playerid);
			GetVehicleZAngle(vehicleid, pa);
			if(pa >= 0.0 && pa <= 22.5) //
			{
				GetVehiclePos(vehicleid, px, py, pz);
				SetVehiclePos(vehicleid, px, py+carid, pz);
			}
			if(pa >= 22.5 && pa <= 67.5) //nw
			{
				GetVehiclePos(vehicleid, px, py, pz);
				SetVehiclePos(vehicleid, px - (carid / 2), py + (carid / 2), pz);
			}
			if(pa >= 67.5 && pa <= 112.5) //w
			{
				GetVehiclePos(vehicleid, px, py, pz);
				SetVehiclePos(vehicleid, px - carid, py, pz);
			}
			if(pa >= 112.5 && pa <= 157.5) //sw
