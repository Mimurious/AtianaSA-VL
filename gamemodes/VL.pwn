	ClanWarT2 = 0;

	ClanWarS1 = 0;
	ClanWarS2 = 0;

	ClanWarP1 = 0;
	ClanWarP2 = 0;
}

function SpeedUp(playerid) {
	if(!boom[playerid]) return KillTimer(turboTimer[playerid]);
	new car = GetPlayerVehicleID(playerid);
	new Speed;
	if(GetVehicleType(car) == 5 || GetVehicleType(car) == 9) Speed = GetPlayerSpeed(playerid,1);
	else Speed = GetPlayerSpeed(playerid,0);
	if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && Speed > GetVehicleMaxSpeed(GetVehicleModel(car)) - 60)
	{
		new Float:vx,Float:vy,Float:vz;
		GetVehicleVelocity(car,vx,vy,vz);
		if(Speed < GetVehicleMaxSpeed(GetVehicleModel(car))*1.101) SetVehicleVelocity(car, vx * 1.065, vy *1.065, vz * 1.066);
		else if(Speed > GetVehicleMaxSpeed(GetVehicleModel(car))*1.16) SetVehicleVelocity(car, vx / 1.025, vy / 1.025, vz / 1.026);
	}
	return 1;
}

function SpeedDown(playerid) {
    if(!boom[playerid]) return 0;
	new car = GetPlayerVehicleID(playerid);
	if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		new Float:vx,Float:vy,Float:vz;
		GetVehicleVelocity(car,vx,vy,vz);
	 	SetVehicleVelocity(car, vx / 1.165, vy / 1.165, vz / 1.165);
	}
	return 0;
}

