	if(vehicleid == INVALID_VEHICLE_ID || playerid == INVALID_PLAYER_ID) return 0;
	
	if(TOG[13][GetDriver(vehicleid)] == 1)
	{
		new Float:px, Float:py, Float:pz, Float:pa, carid = 3;
