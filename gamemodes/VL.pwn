cmd:givetax(playerid, params[])
{
	if(PlayerInfo[playerid][pLeader] != 3) return SendEngMessage(playerid, COLOR_ERROR, "Error: You need to be mayor."), SendPerMessage(playerid, COLOR_ERROR, "Shoma mayor nistid.");
	new players = 0, charitygive, rpgive;
	foreach(new i : Player) if(PlayerInfo[i][pLevel] > 1 && PlayerInfo[i][pLevel] < 9) players ++;
	if(players < 5) return SendEngMessage(playerid, COLOR_ERROR, "Need to be minimum 5 player with level between 2 and 8."), SendPerMessage(playerid, COLOR_ERROR, "Bayad hadeaghal 5 bazikon ba sathe 2 ta 8 hozur dashte bashand.");
	if(CharityMoney < 50000) return SendEngMessage(playerid, COLOR_ERROR, "Error: There are not enough funds in the Charity box."), SendPerMessage(playerid, COLOR_ERROR, "In meghdar dar sandoghe shahrdari mojud nist.");
	charitygive = 10000 + random(4001);
	CharityMoney -= charitygive;
	rpgive = 1 + random(3);
	charitygive /= players;

	foreach(new i : Player) if(PlayerInfo[i][pLevel] > 1 && PlayerInfo[i][pLevel] < 9) {
		GivePlayerCash(i, charitygive);
		PlayerInfo[i][pRespect] += rpgive;
		Update(i, pRespectx);
	}
	format(gString, sizeof(gString), "Mayor %s given to all players with level between 2 and 8 $%s and %d experience points.",GetName(playerid),FormatNumber(charitygive),rpgive);
	SendEngMessageToAll(COLOR_LIGHTRED, gString);
	format(gString, sizeof(gString), "Mayor %s be tamame bazikonan beyne levele 2 ta 8 $%s va %d emtiyaze tajrobe ehda kard.",GetName(playerid),FormatNumber(charitygive),rpgive);
	SendPerMessageToAll(COLOR_LIGHTRED, gString);
	gQuery[0] = '\0';
	format(gQuery, sizeof(gQuery), "UPDATE `others` SET `Charity`=%d WHERE `ID`=1", CharityMoney);
	mysql_query(Database, gQuery);
	return 1;
}

