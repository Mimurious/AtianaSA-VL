cmd:vehsticker(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in any vehicle.");
	new carid = GetPlayerVehicleID(playerid),idcar=-1;
	for(new vv; vv < MAX_PERSONAL_VEHICLES; vv++)
	{
		if(carid == PlayerInfo[playerid][pCarID][vv])
		{
			idcar = vv;
		}
	}
	if(idcar == -1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You can only place stickers on premium vehicles.");
	if(IsPlayerConnected(playerid))
	{
		if(carid == PlayerInfo[playerid][pCarID][idcar])
		{
			if(PlayerInfo[playerid][pCarVIP][idcar] > 0) {
				new query[256],text[20], color;
				if(sscanf(params, "is[20]", color, text)) return SendSyntaxMessage(playerid, "/vehsticker <ColorID (/stickercolors)> <Text>");
				if(color < 0 || color > 12) return SendClientMessage(playerid, COLOR_ERROR, "Error: Invalid color id entered.");
				if(strlen(text) > 20) return SendClientMessage(playerid, COLOR_ERROR, "Error: Maximum 20 characters are allowed.");
				if(strlen(text) == 0) return 1;
				format(CarVipName[playerid][idcar], 20, text);
				PlayerInfo[playerid][pCarVipColor][idcar] = color;
				cUpdate(playerid, idcar, cCarVipColorx);
				SetVipText(playerid, idcar);
				mysql_format(Database, query, sizeof(query), "UPDATE `cars` SET `VipText`='%s' WHERE `ID`='%d'", text, PlayerInfo[playerid][pCar][idcar]);
				mysql_tquery(Database, query, "", "");
				SendClientMessage(playerid, COLOR_SYN3, "The vehicle sticker has been placed on your vehicle.");
			}
			else return SendClientMessage(playerid, COLOR_ERROR, "Error: You can only use this command on premium vehicles.");
		}
		else return SendClientMessage(playerid, COLOR_ERROR, "Error: You can only use this command on premium vehicles.");
	}
	return 1;
}

