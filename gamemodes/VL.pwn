cmd:createclan(playerid, params[])
{
	new name[64], tag[24], color[24], tagtype, slots;
	if(PlayerInfo[playerid][pAdmin] < 7) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[24]iis[64]s[24]",tag,tagtype,slots,name, color)) return SendSyntaxMessage(playerid, "/createclan <Tag> <TagType (0 = Before / 1 = After)> <Slots> <Name> <Color (Hex color without #)>");
	{
		if(ExistClan(tag)) return SendClientMessage(playerid, COLOR_ERROR, "Clan already exist.");
		if(strlen(tag) > 10) return SendClientMessage(playerid, COLOR_ERROR, "Clan tag is to long.");
		if(strlen(name) > 32) return SendClientMessage(playerid, COLOR_ERROR, "Clan name is to long.");
		if(strlen(color) != 6) return SendClientMessage(playerid, COLOR_ERROR, "Color amount must be a 6-char hexadecimal number.");

		TotalClans ++;

		strmid(ClanInfo[TotalClans][cName], name, 0, 32, 32);
		strmid(ClanInfo[TotalClans][cTag], tag, 0, 10, 10);
		strmid(ClanInfo[TotalClans][cColor], color, 0, 10, 10);
		ClanInfo[TotalClans][cTagType] = tagtype;
		ClanInfo[TotalClans][cSlots] = slots;

		gQuery[0] = (EOS);
		mysql_format(Database, gQuery, sizeof(gQuery) ,"INSERT INTO `clans` (`Name`,`Tag`,`TagType`,`Slots`,`Color`, `CreateDate`) VALUES ('%e','%e',%d,%d,'%e',UNIX_TIMESTAMP())",
			ClanInfo[TotalClans][cName],
			ClanInfo[TotalClans][cTag],
			ClanInfo[TotalClans][cTagType],
			ClanInfo[TotalClans][cSlots],
			ClanInfo[TotalClans][cColor]);
		mysql_query(Database ,gQuery);


		format(gString,sizeof(gString),"Clan - %s - (ID:%d) with tag: %s and %d slots was created.", ClanInfo[TotalClans][cName], TotalClans, ClanInfo[TotalClans][cTag], ClanInfo[TotalClans][cSlots]);
		SendClientMessage(playerid, COLOR_SYN2,gString);

		format(gString, sizeof(gString), "%s has created a clan: %s - (ID:%d) with tag: %s and %d slots.",GetName(playerid),ClanInfo[TotalClans][cName], TotalClans, ClanInfo[TotalClans][cTag], ClanInfo[TotalClans][cSlots]);
