cmd:buygun(playerid, params[])
{
	if(IsPlayerInRangeOfPoint(playerid, 2, 2119.1157, -2001.7606, 7.9844)) // Black Market
	{
		new gunname[24], ammo;
		new GunPrice[6][1] = { {2}, {3}, {4}, {5}, {6}, {10} };
		if(sscanf(params,"s[24]i",gunname,ammo)){
			SendSyntaxMessage(playerid, "/buygun <Name> <Ammo>");
			SendClientMessage(playerid, COLOR_SERVER,"_______________________________________");
			SendClientMessage(playerid, COLOR_WHITE,"*** Guns & Ammo ***");
			format(gString, sizeof(gString), "Weapons: Deagle MP5 Ak47 M4 Shotgun Rifle");
			SendClientMessage(playerid, COLOR_GRAD3, gString);
			return 1;
		}
		if(strmatch(gunname,"deagle"))
		{
			if(ammo < 1 || ammo > 500) return SendClientMessage(playerid, COLOR_ERROR, "Ammo must be between 1 and 500.");
			new price = (ammo*GunPrice[0][0])/1;
			if(GetPlayerCash(playerid) < price) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");
			format(gString, sizeof(gString), "You have purchased Deagle with %d ammo for $%s",ammo,FormatNumber(price));
			SendClientMessage(playerid, COLOR_GRAD3, gString);
			GivePlayerCash(playerid, -price);
			GivePlayerWeaponEx(playerid, 24, ammo);
			BizzInfo[1][bSafe] += price;
			bUpdate(1,bSafex);
			BizzPayLog(playerid, 1, price);
		}
		else if(strmatch(gunname,"mp5"))
		{
