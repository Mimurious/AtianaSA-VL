cmd:open(playerid, params[])
{
	for(new i = 1; i < sizeof(HouseInfo); i++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 2.0, HouseInfo[i][hEntranceX], HouseInfo[i][hEntranceY], HouseInfo[i][hEntranceZ]) || IsPlayerInRangeOfPoint(playerid, 2.0, HouseInfo[i][hExitX], HouseInfo[i][hExitY], HouseInfo[i][hExitZ]) && HouseEntered[playerid] == i)
		{
			if(PlayerInfo[playerid][pHouseKey] == i)
			{
				if(HouseInfo[i][hLock] == 1)
				{
					HouseInfo[i][hLock] = 0;
					hUpdate(i, hLockx);
					if(PlayerInfo[i][pLanguage] == 0)GameTextForPlayer(playerid, "~w~Door ~g~Open", 5000, 6);
					if(PlayerInfo[i][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Dar ~g~Baz", 5000, 6);
					PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					return 1;
				}
				if(HouseInfo[i][hLock] == 0)
				{
					HouseInfo[i][hLock] = 1;
					hUpdate(i, hLockx);
					if(PlayerInfo[i][pLanguage] == 0)GameTextForPlayer(playerid, "~w~Door ~r~Closed", 5000, 6);
					if(PlayerInfo[i][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Dar ~r~Baste", 5000, 6);
					PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					return 1;
				}
			}
			else SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have keys of this house.");
		}
	}
	for(new i = 1; i <= businesses_count; i++)
	{
		if (IsPlayerInRangeOfPoint(playerid, 2.0, BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ]) || IsPlayerInRangeOfPoint(playerid, 2.0, BizzInfo[i][bExitX], BizzInfo[i][bExitY], BizzInfo[i][bExitZ]) && BizzEntered[playerid] == i)
		{
			if(BizzInfo[i][bSBiz] == 1 && i != 19 && i != 25) return 1;
			if(PlayerInfo[playerid][pBizzKey] == i)
			{
				if(BizzInfo[i][bLocked] == 1)
				{
					BizzInfo[i][bLocked] = 0;
					bUpdate(i, bLockedx);
					GameTextForPlayer(playerid, "~w~Bussiness ~g~Open", 5000, 6);
					if(PlayerInfo[i][pLanguage] == 0)GameTextForPlayer(playerid, "~w~Bussiness ~g~Open", 5000, 6);
					if(PlayerInfo[i][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Tejarat ~g~Baz", 5000, 6);
					PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					return 1;
				}
				if(BizzInfo[i][bLocked] == 0)
				{
					BizzInfo[i][bLocked] = 1;
					bUpdate(i, bLockedx);
					if(PlayerInfo[i][pLanguage] == 0)GameTextForPlayer(playerid, "~w~Bussiness ~g~Closed", 5000, 6);
					if(PlayerInfo[i][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Tejarat ~g~Baste", 5000, 6);
					PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					return 1;
				}
			}
			else SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have keys of this business.");
		}
	}
	return 1;
}

