Dialog:sellhousetostate(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		if(PlayerInfo[playerid][pHouseKey] == 0) return SendEngMessage(playerid, COLOR_ERROR, "Error: You don't own a house."),SendPerMessage(playerid, COLOR_ERROR, "Khata: Shoma khane nadarid.");
		new house = PlayerInfo[playerid][pHouseKey];

		HouseInfo[house][hLock] = 1;
		hUpdate(house, hLockx);
		HouseInfo[house][hOwned] = 0;
		hUpdate(house, hOwnedx);

		strmid(HouseInfo[house][hOwner], "The State", 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		hUpdate(house, hOwnerx);

		GivePlayerCash(playerid,HouseInfo[house][hValue] / 3);

		format(gString, sizeof(gString), "You sold house for $%s.", FormatNumber(HouseInfo[house][hValue] / 3));
		if(PlayerInfo[playerid][pLanguage] == 0)GameTextForPlayer(playerid, gString, 5000, 3);
		format(gString, sizeof(gString), "Shoma khaneye khod ra baraye $%s forokhtid.", FormatNumber(HouseInfo[house][hValue] / 3));
		if(PlayerInfo[playerid][pLanguage] == 1)GameTextForPlayer(playerid, gString, 5000, 3);
		SellLog(playerid, house, 0, 0, HouseInfo[house][hValue] / 3);
		PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		PlayerInfo[playerid][pHouseKey] = 0;
		Update(playerid,pHouseKeyx);

		if(HouseEntered[playerid] == house)
		{
			SetPlayerInterior(playerid,0);
			SetPlayerVirtualWorld(playerid,0);
			SetPlayerPosEx(playerid,HouseInfo[house][hEntranceX],HouseInfo[house][hEntranceY],HouseInfo[house][hEntranceZ]);
			HouseEntered[playerid] = 0;
			SetPlayerFacingAngle(playerid, HouseInfo[house][hEntranceA]);
			SetCameraBehindPlayer(playerid);
		}
		PlayerInfo[playerid][pSpawnChange] = 1;
		Update(playerid, pSpawnChangex);

		for(new i = 1; i <= TotalClans; i++)
		{
			if(ClanInfo[i][cHouseHQ] == house)
			{
				ClanInfo[i][cHQType] = 0;
				clanUpdate(i, cHQTypex);
				ClanInfo[i][cHouseHQ] = 0;
				clanUpdate(i, cHouseHQx);
			}
		}
		HouseUpdate(house);
	}
	return 1;
}

Dialog:sellbiztostate(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		if(PlayerInfo[playerid][pBizzKey] == 0) return SendEngMessage(playerid, COLOR_ERROR, "Error: You don't own a business."), SendPerMessage(playerid,COLOR_ERROR,"Khata: Shoma tejarati nadarid.");
		new bizz = PlayerInfo[playerid][pBizzKey];

		BizzInfo[bizz][bLocked] = 1;
		bUpdate(bizz,bLockedx);
		BizzInfo[bizz][bOwned] = 0;
		bUpdate(bizz,bOwnedx);
		strmid(BizzInfo[bizz][bOwner], "The State", 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		bUpdate(bizz,bOwnerx);

		new moneyback = (3*BizzInfo[bizz][bPrice])/6 + BizzInfo[bizz][bSafe];
		GivePlayerCash(playerid, moneyback);

		PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
		if(PlayerInfo[playerid][pLanguage] == 0)
		{
			format(gString, sizeof(gString), "You have sold your business for ~n~~g~$%s", FormatNumber(moneyback));
			GameTextForPlayer(playerid, gString, 10000, 3);
		}
		if(PlayerInfo[playerid][pLanguage] == 1)
		{
			format(gString, sizeof(gString), "Shoma tejarate khod ra baraye ~n~~g~$%s forokhtid", FormatNumber(moneyback));
			GameTextForPlayer(playerid, gString, 10000, 3);
		}
		SellLog(playerid, 0, bizz, 0, moneyback);

		BizzInfo[bizz][bSafe] = 0;
		bUpdate(bizz,bSafex);

		BizzInfo[bizz][bEntranceCost] = 1;
		bUpdate(bizz, bEntranceCostx);

		if(BizzInfo[bizz][bItemPrice] > 0)
		{
			BizzInfo[bizz][bItemPrice] = 1;
			bUpdate(bizz, bItemPricex);
		}

		PlayerInfo[playerid][pBizzKey] = 0;
		Update(playerid,pBizzKeyx);

		BizzUpdate(bizz);
	}
	return 1;
}

