			format(gString,128,"%s...",gString);
			ABroadCast(COLOR_AC, gString, 1);

			format(string2,128,"...%s",string2);
			ABroadCast(COLOR_AC, string2, 1);
		}
		else
		{
			ABroadCast(COLOR_AC, gString, 1);
		}

		if(strfind(ReportInfo[reportid][rReason], "hack", true) != -1 || strfind(ReportInfo[reportid][rReason], "cheat", true) != -1 || strfind(ReportInfo[reportid][rReason], "tabli", true) != -1) {
			foreach(new i : Player)
			{
				if(PlayerInfo[i][pAdmin]) GameTextForPlayer(i, "~y~~h~Priority Report", 4000, 1);
