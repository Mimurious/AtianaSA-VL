	if (!params[0] || params[0] == ' ' || strlen(params) < 4) return SendSyntaxMessage(playerid, "/checkban <Player Name/IP>");
	new bool:nameEntered;
	for (new i; params[i] != EOS; i++)
	{
		if (params[i] == '.' || params[i] >= '0' && params[i] <= '9')
		{
			continue;
		}

		nameEntered = true;
		break;
	}
	new string[1024];
	if (nameEntered) format(string, sizeof string, "SELECT * FROM `bans` WHERE `name` = '%s' LIMIT 1", params);
	else
	{
		if(DebugMode == false) if (!Ban_GetLongIP(params)) return SendClientMessage(playerid, COLOR_ERROR, "Invalid IP Address entered.");

		format(string, sizeof string, "SELECT * FROM `bans` WHERE (`longip` != 0 AND (`longip` & %i) = %i) LIMIT 1", BAN_MASK, (Ban_GetLongIP(params) & BAN_MASK));
	}
	mysql_tquery(Database, string, "CheckBan", "i", playerid);
	return 1;
}

