stock ShowResults(playerid, option[])
{
	new string[128];
	SendEngMessage(playerid, COLOR_WHITE, "{FFFFCC}Too many results for this search. Try one of these:");
	SendPerMessage(playerid, COLOR_WHITE, "{FFFFCC}Natayeje bishtari yaft shod. Yeki az an ha ra entekhab konid:");
	foreach(Player, i)
	{
		if(strfind(GetName(i), option, true) != -1)
		{
			format(string, sizeof(string), "{FFFFCC}(%d) %s", i, GetName(i));
			SendClientMessage(playerid, COLOR_WHITE, string);
		}
	}
	return 1;
}

