cmd:materials(playerid, params[])
{
	new act[24], amount;
	if(PlayerInfo[playerid][pJob] != 9) return SendEngMessage(playerid, COLOR_ERROR, "Error: You don't have arms dealer job."),SendPerMessage(playerid, COLOR_ERROR, "Shoghle shoma arms delaer nist.");
	if(sscanf(params,"s[24]I(0)", act, amount)) { SendSyntaxMessage(playerid, "/materials <Name>"); SendClientMessage(playerid, -1, "Available names: Get Deliver"); return 1; }
	{
		if(strmatch(act,"get"))
		{
			if(amount == 0) return SendSyntaxMessage(playerid, "/materials get <Amount (1-10)>");
			if(!IsPlayerInRangeOfPoint(playerid,2.0,2595.1450,2790.6736,10.8203)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not at the Materials Package Building.");
			if(AmountCheck(playerid, amount, 1, 10)) return 1;
			if(GetPlayerCash(playerid) < 50*amount) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");
			if(MatsHolding[playerid] >= 10) return SendClientMessage(playerid, COLOR_ERROR, "Error: You already have 10 packets of materials.");
			format(gString, sizeof(gString), "You took %d packets of materials for $%s. Go to the checkpoint and process it.", amount, FormatNumber(50*amount));
			SendClientMessage(playerid, COLOR_SYN2, gString);
			UseFind[playerid] = 0;
			Finding[playerid] = INVALID_PLAYER_ID;
			IsTowing[playerid] = 0;
			SetPlayerCheckpointEx(playerid, 1707.5345,1025.6398,10.8203, 2.0);
			CheckPoint[playerid] = 1995;
			GivePlayerCash(playerid, -50*amount);
			MatsHolding[playerid] += amount;
		}
		else if(strmatch(act,"deliver"))
		{
			if(!IsPlayerInRangeOfPoint(playerid,2.0,1707.5345,1025.6398,10.8203)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not at the Materials Process Building.");
			if(MatsHolding[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have any packet of materials.");
			
			if(PlayerInfo[playerid][pQuest1] == 5)
			{
				if(PlayerInfo[playerid][pQuestNeed1] > PlayerInfo[playerid][pQuestProgress1])
				{
					PlayerInfo[playerid][pQuestProgress1] += MatsHolding[playerid] * 10;
					Update(playerid, pQuestProgress1x);
					if(PlayerInfo[playerid][pQuestProgress1] >= PlayerInfo[playerid][pQuestNeed1])
					{
						FinishQuest1(playerid, 5);
					}
					else
					{
						new str[128];
						if(PlayerInfo[playerid][pLanguage] == 0)
						{
							format(str, sizeof(str),"Quest [Collect %d materials] progress: %d/%d",PlayerInfo[playerid][pQuestNeed1],PlayerInfo[playerid][pQuestProgress1],PlayerInfo[playerid][pQuestNeed1]);
							SendClientMessage(playerid, COLOR_YELLOW, str);
						}
						else
						{
							format(str, sizeof(str),"Pishraviye queste [Jam avarie %d mavade avaliye]: %d/%d",PlayerInfo[playerid][pQuestNeed1],PlayerInfo[playerid][pQuestProgress1],PlayerInfo[playerid][pQuestNeed1]);
							SendClientMessage(playerid, COLOR_YELLOW, str);
						}
					}
				}
			}
			
			format(gString, sizeof(gString), "You have got %s Materials for the %d packages.", FormatNumber(10*MatsHolding[playerid]), MatsHolding[playerid]);
			SendClientMessage(playerid, COLOR_SYN2, gString);
			PlayerInfo[playerid][pMats] += 10*MatsHolding[playerid];
			Update(playerid, pMatsx);
			MatsHolding[playerid] = 0;
		}
	}
	return 1;
}

cmd:selldrugs(playerid,params[])
{
	if(IsPlayerConnected(playerid))
	{
		if(PlayerInfo[playerid][pJob] != 2) return SendClientMessage(playerid, COLOR_YELLOW2, "Error: You are not 'drug dealer'.");
		new playa,needed,string[128],giveplayer[25],sendername[25],money;
		if(sscanf(params, "uii",playa,needed,money)) return SendSyntaxMessage(playerid, "/selldrugs <Name/Playerid> <Drugs> <Price ($+)>");
		if(needed < 1 || needed > 50) return SendClientMessage(playerid, COLOR_YELLOW2, "Error: The grams of drugs must be between 1 and 50 grams!");
		if(money < 1 || money > 15000) return SendClientMessage(playerid, COLOR_YELLOW2, "Error: Price must be between $1 and $15,000!");
		if(needed > PlayerInfo[playerid][pDrugs]) return SendClientMessage(playerid, COLOR_WHITE,"{FFF8C6}You dont have that much Drugs with you.");
		if(PlayerInfo[playerid][pPinLogged] == 0 && strlen(PlayerInfo[playerid][pPin]) != 1) return SendEngMessage(playerid, COLOR_YELLOW2, PINEN), SendPerMessage(playerid, COLOR_YELLOW2, PINFA);
		if(IsPlayerConnected(playa))
		{
			if(playa != INVALID_PLAYER_ID)
			{
				if (ProxDetectorS(8.0, playerid, playa))
				{
					if(playa == playerid) return 1;
					GetPlayerName(playa, giveplayer, sizeof(giveplayer));
					GetPlayerName(playerid, sendername, sizeof(sendername));
					format(string, sizeof(string), "{B8FFDB}* You've offered to %s, %d grams of drugs for %s$.", giveplayer, needed, FormatNumber(money));
					SendClientMessage(playerid, COLOR_WHITE, string);
					format(string, sizeof(string), "{B8FFDB}* %s offered you %d grams of drugs for %s$, (type (/acc)ept drugs %d) to transaction.", sendername, needed, FormatNumber(money),playerid);
					SendClientMessage(playa, COLOR_WHITE, string);
					DrugOffer[playa] = playerid;
					DrugPrice[playa] = money;
					DrugGram[playa] = needed;
				}
				else return SendClientMessage(playerid, COLOR_YELLOW2, "Error: That player is not near you.");
			}
		}
	}
	return 1;
}

