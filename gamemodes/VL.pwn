	{
		format(gString, sizeof(gString), "Locked: %s EnterFee: $%s ItemPrice: $%s Safe: $%s", locktext, FormatNumber(BizzInfo[bizz][bEntranceCost]), FormatNumber(BizzInfo[bizz][bItemPrice]), FormatNumber(BizzInfo[bizz][bSafe]));
