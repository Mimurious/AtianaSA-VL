			    KillTimer(FindTimer[playerid]);
				UseFind[playerid] = 0;
				Finding[playerid] = INVALID_PLAYER_ID;
				FindPoints[playerid] = 0;
				CheckPoint[playerid] = 0;
				CCP[playerid] = false;
				DisablePlayerCheckpoint(playerid);
				SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
				SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
			}
			else if(CheckPoint[playerid] != 0)
			{
				DisablePlayerCheckpoint(playerid);
				SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
				SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
				CheckPoint[playerid] = 0;
				CCP[playerid] = false;
			}
			else {
				SendEngMessage(playerid, COLOR_ERROR, "Error: You don't have any checkpoints.");
				SendPerMessage(playerid, COLOR_ERROR, "Khata: Shoma hich checkpointi nadarid.");
			}
		}
		else if(strmatch(name,"refill"))
		{
			foreach(new i : Player)
			{
				if(RefillPrice[i][playerid] != 0) RefillPrice[i][playerid] = 0;
				if(RefillPrice[playerid][i] != 0) RefillPrice[playerid][i] = 0;
			}
			SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
			SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
		}
		else if(strmatch(name,"cinvite"))
		{
			foreach(new i : Player)
			{
				if(CInviteOffer[i][playerid] != 0) CInviteOffer[i][playerid] = 0;
				if(CInviteOffer[playerid][i] != 0) CInviteOffer[playerid][i] = 0;
			}
			SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
			SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
		}
		else if(strmatch(name,"ticket"))
		{
			foreach(new i : Player)
			{
				if(TicketMoney[i][playerid] != 0) TicketMoney[i][playerid] = 0;
