cmd:setfreq(playerid, params[])
{
	new freq[6];
	if(PlayerInfo[playerid][pWalkieTalkie] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have a Walkie Talkie.");
	if(sscanf(params, "s[6]",freq)) return SendSyntaxMessage(playerid, "/setfreq <Frequency (89.5 - 98.5)>");
	{
		if(strlen(freq) != 1 && strlen(freq) != 4) return SendSyntaxMessage(playerid, "/setfreq <Frequency (89.5 - 98.5)>");
		if(floatstr(freq) != 0 && floatstr(freq) < 89.5 || floatstr(freq) > 98.5 && floatstr(freq) != 0) return SendSyntaxMessage(playerid, "/setfreq <Frequency (89.5 - 98.5)>");
		if(floatstr(freq) == floatstr(WTChannel[playerid])) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are already on this channel.");
		if(floatstr(freq) > 0)
		{
			format(gString, sizeof(gString), "You set your Walkie Talkie frequency to %s.", freq);
			SendClientMessage(playerid, COLOR_SYN2, gString);
			format(WTChannel[playerid],24,"%s",freq);
		}
		else SendClientMessage(playerid, COLOR_SYN2, "You turned your Walkie Talkie off.");
	}
	return 1;
}

cmd:wt(playerid, params[])
{
	new msg[128];
	if(PlayerInfo[playerid][pMuted] == 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot speak, you have been silenced.");
	if(!IsPlayerSpawned(playerid)) {
	    SendEngMessage(playerid, COLOR_LIGHTRED, "You cannot do any action when you are are dead.");
		SendPerMessage(playerid, COLOR_LIGHTRED, "Shoma dar hengami ke mordid tavanayie anjam dadane kari ra nadarid.");
		return 0;
	}
	if(PlayerInfo[playerid][pWalkieTalkie] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have a Walkie Talkie.");
	if(sscanf(params, "s[128]", msg)) return SendSyntaxMessage(playerid, "/wt <Text>");
	if(floatstr(WTChannel[playerid]) == 0) return SendClientMessage(playerid,COLOR_ERROR,"Your walkie talkie is off.");

	foreach(new i : Player)
	{
		if(floatstr(WTChannel[i]) != 0 && floatstr(WTChannel[i]) == floatstr(WTChannel[playerid]))
		{
			format(gString, sizeof(gString), "In Frequency [%s]: %s: {FFFFFF}%s",WTChannel[playerid],GetName(playerid), msg);
			SendClientMessage(i, 0x54BF9CFF, gString);
		}
	}
	return 1;
}


cmd:buyhouse(playerid, params[])
{
	if(IsInWar1(playerid) || IsInWar2(playerid))
		return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot enter in this enter.");
	if(PlayerInfo[playerid][pHouseKey] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You already own a house, type /sellhousetostate if you want to buy this one.");
	new h = 0;
	for(new i = 1; i < sizeof(HouseInfo); i++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 2.0, HouseInfo[i][hEntranceX], HouseInfo[i][hEntranceY], HouseInfo[i][hEntranceZ]))
		{
			h = i;
			break;
		}
	}
	if(!h) return 1;

	new bool: success = false, price = 0;

	if(!HouseInfo[h][hOwned])
	{
		if(PlayerInfo[playerid][pLevel] < HouseInfo[h][hLevel])
		{
			format(gString, sizeof(gString), "You need level %d to purchase this house.", HouseInfo[h][hLevel]);
			SendClientMessage(playerid, COLOR_ERROR, gString);
			return 1;
		}
		success = true;
		price = HouseInfo[h][hValue];
	}
	else if(HouseInfo[h][hOwned] && HouseInfo[h][hSellPrice] > 0)
	{
		if(PlayerInfo[playerid][pLevel] < HouseInfo[h][hLevel])
		{
			format(gString, sizeof(gString), "You need level %d to purchase this house.", HouseInfo[h][hLevel]);
			SendClientMessage(playerid, COLOR_ERROR, gString);
			return 1;
		}
		success = true;
		price = HouseInfo[h][hSellPrice];
	}
	else if(HouseInfo[h][hOwned] && !HouseInfo[h][hSellPrice])
	{
		success = false;
		return SendClientMessage(playerid, COLOR_ERROR, "Error: This house is not for sale.");
	}

	if(success)
	{
		if(GetPlayerCash(playerid) < price) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");
		PlayerInfo[playerid][pHouseKey] = h;
		Update(playerid,pHouseKeyx);
		PlayerInfo[playerid][pRentKey] = 0;
		Update(playerid,pRentKeyx);

		if(HouseInfo[h][hOwned] && HouseInfo[h][hSellPrice])
		{
			new id = GetPlayerID(HouseInfo[h][hOwner]);
			if(id != INVALID_PLAYER_ID)
			{
				GivePlayerCash(id, HouseInfo[h][hSellPrice] - HouseInfo[h][hLevel]);
				SendClientMessage(id, COLOR_YELLOW, "Your house has been sold.");
				PlayerInfo[id][pHouseKey] = 0;
				Update(id,pHouseKeyx);
			}
			else
			{
				if(ExistName(HouseInfo[h][hOwner]))
				{
					gQuery[0] = EOS;
					mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Cash`='Cash'+'%d',`HouseKey`='0' WHERE Name='%e'", price - HouseInfo[h][hLevel]*10000, HouseInfo[h][hOwner]);
					mysql_query(Database, gQuery);
				}
			}
		}

		HouseInfo[h][hOwned] = 1;
		hUpdate(h, hOwnedx);
		strmid(HouseInfo[h][hOwner], PlayerInfo[playerid][pUsername], 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
		hUpdate(h, hOwnerx);
		HouseInfo[h][hSellPrice] = 0;
		hUpdate(h, hSellPricex);


		GivePlayerCash(playerid, -price);
		SetPlayerInterior(playerid,HouseInfo[h][hInt]);
		SetPlayerVirtualWorld(playerid,HouseInfo[h][hWorld]);
		SetPlayerPosEx(playerid,HouseInfo[h][hExitX],HouseInfo[h][hExitY],HouseInfo[h][hExitZ]);
		HouseEntered[playerid] = h;
		SetPlayerFacingAngle(playerid, HouseInfo[h][hExitA]);
		SetCameraBehindPlayer(playerid);
		PlayerInfo[playerid][pSpawnChange] = 2;
		Update(playerid, pSpawnChangex);

		if(PlayerInfo[playerid][pLanguage] == 0)GameTextForPlayer(playerid, "~w~Welcome Home!!!", 5000, 3);
		if(PlayerInfo[playerid][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Be khane khosh amadid!!!", 5000, 3);
		SendClientMessage(playerid, COLOR_SYN2, "Congratulations, for your new purchase !");
		SendClientMessage(playerid, COLOR_SYN2, "Type /househelp to review the new property help section !");
		HouseUpdate(h);
	}
	return 1;
}

cmd:houseprice(playerid, params[])
