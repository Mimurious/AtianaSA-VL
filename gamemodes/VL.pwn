function PaintballEnded()
{
	KillTimer(painttimer);
	foreach(Player,i)
	{
		if(PlayerPaintballing[i] != 0)
		{
			if(!IsPlayerConnected(PaintballWinner))
			{
				PaintballWinner = 999;
				PaintballWinnerScore = 0;
				foreach(Player,j)
				{
					if(PlayerPaintballing[j] != 0)
					{
						if(PlayerPaintballScore[j] > PaintballWinnerScore) PaintballWinner = j;
					}
				}
			}

			if(IsPlayerConnected(PaintballWinner))
			{
				format(gString,sizeof(gString), "** %s won the Paintball Match with %d kills **",GetName(PaintballWinner),PaintballWinnerScore);
				SendEngMessage(i, COLOR_WHITE, gString);
				format(gString,sizeof(gString),"** Your score is %d and you have %d kills and %d deaths **",(KillP[i]-DeathP[i]),KillP[i],DeathP[i]);
				SendEngMessage(i, COLOR_WHITE, gString);

				format(gString,sizeof(gString), "** %s ba %d kills barandeye Paintball Match shod **",GetName(PaintballWinner),PaintballWinnerScore);
				SendPerMessage(i, COLOR_WHITE, gString);
				format(gString,sizeof(gString),"** Emtiaze shoma %d bood va shoma %d kills va %d deaths dashtid **",(KillP[i]-DeathP[i]),KillP[i],DeathP[i]);
				SendPerMessage(i, COLOR_WHITE, gString);

				if(i == PaintballWinner)
				{
					new chance = random(10);
					if(chance == 5)
					{
						SendEngMessage(PaintballWinner, COLOR_WHITE, "You win 2 experience points at paintball match.");
						SendPerMessage(PaintballWinner, COLOR_WHITE, "Shoma 2 emtiyaze tajrobe dar paintball match bordid.");
						PlayerInfo[PaintballWinner][pRespect] += 2;
						Update(PaintballWinner, pRespectx);
					}
					else
					{
						SendEngMessage(PaintballWinner, COLOR_WHITE, "You win $100 at paintball match.");
						SendPerMessage(PaintballWinner, COLOR_WHITE, "Shoma $100 dar paintball match bordid.");
						GivePlayerCash(PaintballWinner, 100);
					}
				}
			}

			ResetPlayerWeapons(i);
			SetPlayerArmourEx(i, 0);
			SetPlayerHealthEx(i, 100);
			PlayerPaintballing[i] = 0;
			PlayerPaintballScore[i] = 0;
			SetPlayerPosEx(i, BizzInfo[19][bEntranceX],BizzInfo[19][bEntranceY],BizzInfo[19][bEntranceZ]);
			SetPlayerInterior(i,0);
			SetPlayerVirtualWorld(i,0);
			KillP[i] = 0;
			DeathP[i] = 0;
			for(new m = 0; m < 5; m++) SendDeathMessageToPlayer(i, 1001, 1001, 200);
		}
	}
	PaintballRound = 0;
	PaintballWinner = 999;
	PaintballWinnerScore = 0;
	StartingPaintballRound = 0;
	return 1;
}

function DestroyWarPickups(war)
{
	for(new i = 0; i <= CountDynamicPickups(); i++)
