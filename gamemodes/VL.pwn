function CheckCar()
{
	foreach(new i : Player)
	{
		if(CCP[i] || RCP[i])
		{
			new str[64], Float:fDistance = GetPlayerDistanceFromPoint(i, CheckpointPos[i][0], CheckpointPos[i][1], CheckpointPos[i][2]);
			if(PlayerInfo[i][pLanguage] == 0) format(str, sizeof(str), "Distance: %s%0.1fm", (fDistance <= 300) ? "~g~~h~" : "~y~", fDistance);
			else if(PlayerInfo[i][pLanguage] == 1) format(str, sizeof(str), "Fasele: %s%0.1fm", (fDistance <= 300) ? "~g~~h~" : "~y~", fDistance);
			PlayerTextDrawSetString(i, CpTDD[i], str);
			PlayerTextDrawShow(i, CpTDD[i]);
		}
		else PlayerTextDrawHide(i, CpTDD[i]);

		if(GetPlayerState(i) == PLAYER_STATE_DRIVER && SpeedLimiter[i] > 0 && GetPlayerSpeed(i, 0) > SpeedLimiter[i])
		{
			new newspeed = GetPlayerSpeed(i, 0) - SpeedLimiter[i];
			ModifyVehicleSpeed(GetPlayerVehicleID(i), - newspeed);
		}

		if(IsPlayerInAnyVehicle(i))
		{
			new car = GetPlayerVehicleID(i);

			new Speed;
			new btx[64] = "";
			if(GetVehicleType(car) == 5 || GetVehicleType(car) == 9) Speed = GetPlayerSpeed(i,1);
			else Speed = GetPlayerSpeed(i,0);
			new SpeedTD[128];
			new mxsp = GetVehicleMaxSpeed(GetVehicleModel(car))+1;
			if(boom[i]) mxsp += 50;
			if(boom[i]) btx = "Turbo-Charge ";
			if(Speed > mxsp-35) SpeedTD = "~r~";
			else SpeedTD = "~y~";
			if(PlayerInfo[i][pLanguage] == 0) format(gString, sizeof(gString), "%s%d", SpeedTD, Speed);
			else if(PlayerInfo[i][pLanguage] == 1) format(gString, sizeof(gString), "%s%d", SpeedTD, Speed);
			PlayerTextDrawSetString(i, Speedo[1][i], gString);
			if(Fuel[car] < 20) SpeedTD = "~r~";
			else SpeedTD = "~y~";
			if(PlayerInfo[i][pLanguage] == 0) format(gString, sizeof(gString), "Fuel: %s%d%", SpeedTD, Fuel[car]);
			else if(PlayerInfo[i][pLanguage] == 1) format(gString, sizeof(gString), "Benzin: %s%d%", SpeedTD, Fuel[car]);
			PlayerTextDrawSetString(i, Speedo[0][i], gString);
			if(car == RentVehicle[i]) {
			    new engine, lights, alarm, doors, bonnet, boot, objective;
				GetVehicleParamsEx(RentVehicle[i], engine, lights, alarm, doors, bonnet, boot, objective);
				if(PlayerInfo[i][pLanguage] == 0) format(gString, sizeof(gString), "%s", (doors == VEHICLE_PARAMS_ON ? "~r~Locked" : "~g~Unlocked"));
				else if(PlayerInfo[i][pLanguage] == 1) format(gString, sizeof(gString), "%s", (doors == VEHICLE_PARAMS_ON ? "~r~Ghofl" : "~g~Baz"));
				PlayerTextDrawSetString(i, Speedo[3][i], gString);

				if(PlayerInfo[i][pLanguage] == 0) format(gString, sizeof(gString), "Odometer: ~y~%0.0fKM", RentKM[i]);
				else if(PlayerInfo[i][pLanguage] == 1) format(gString, sizeof(gString), "Karkard: ~y~%0.0fKM", RentKM[i]);
				PlayerTextDrawSetString(i, Speedo[2][i], gString);
			}
			foreach(new s : Player) {
				for(new v; v < MAX_PERSONAL_VEHICLES; v++)
				{
					if(PlayerInfo[s][pCarID][v] == car) {
						if(PlayerInfo[i][pLanguage] == 0) format(gString, sizeof(gString), "Odometer: ~y~%0.0fKM", PlayerInfo[s][pCarKM][v]);
						else if(PlayerInfo[i][pLanguage] == 1) format(gString, sizeof(gString), "Karkard: ~y~%0.0fKM", PlayerInfo[s][pCarKM][v]);
						PlayerTextDrawSetString(i, Speedo[2][i], gString);

						if(PlayerInfo[i][pLanguage] == 0) format(gString, sizeof(gString), "%s", (PlayerInfo[s][pCarLock][v] == 1 ? "~r~Locked" : "~g~Unlocked"));
						else if(PlayerInfo[i][pLanguage] == 1) format(gString, sizeof(gString), "%s", (PlayerInfo[s][pCarLock][v] == 1 ? "~r~Ghofl" : "~g~Baz"));
						PlayerTextDrawSetString(i, Speedo[3][i], gString);
					}
				}
			}
		}
		if(SpectatorID[i] != INVALID_PLAYER_ID)
		{
			if(AntiCheatGetInterior(i) != AntiCheatGetInterior(SpectatorID[i])) SetPlayerInterior(i, AntiCheatGetInterior(SpectatorID[i]));
			if(GetPlayerVirtualWorld(i) != GetPlayerVirtualWorld(SpectatorID[i])) SetPlayerVirtualWorld(i, GetPlayerVirtualWorld(SpectatorID[i]));

			if((GetPlayerState(SpectatorID[i]) == PLAYER_STATE_DRIVER || GetPlayerState(SpectatorID[i]) == PLAYER_STATE_PASSENGER) && OldState[SpectatorID[i]] != GetPlayerState(SpectatorID[i])) PlayerSpectateVehicle(i, GetPlayerVehicleID(SpectatorID[i]));
			else if(GetPlayerState(SpectatorID[i]) == PLAYER_STATE_ONFOOT && OldState[SpectatorID[i]] != GetPlayerState(SpectatorID[i])) PlayerSpectatePlayer(i, SpectatorID[i]);
			OldState[SpectatorID[i]] = GetPlayerState(SpectatorID[i]);

			new specstring[210], Float:shp, Float:sap, Float:vhp, temp2[70], temp3[60];
			GetPlayerHealthEx(SpectatorID[i], shp);
			GetPlayerArmourEx(SpectatorID[i], sap);
			format(specstring, sizeof(specstring), "~r~%s(%d)~w~ (%.1fHP/%.1fAP)", GetName(SpectatorID[i]), SpectatorID[i], shp, sap);
			if(AntiCheatGetWeapon(SpectatorID[i]) > 0 && GetPlayerAmmo(SpectatorID[i]) > 0) { format(temp2, sizeof(temp2), "~n~Weapon: ~r~%d ~w~Ammo: ~r~%d", AntiCheatGetWeapon(SpectatorID[i]), GetPlayerAmmo(SpectatorID[i])); strcat(specstring, temp2); }
			if(IsPlayerInAnyVehicle(SpectatorID[i])) { GetVehicleHealth(GetPlayerVehicleID(SpectatorID[i]), vhp); format(temp3, sizeof(temp3), "~n~~w~Vehicle(%d): ~r~%.1f HP ~w~Speed: ~r~%dkm/h", GetPlayerVehicleID(SpectatorID[i]), vhp, GetPlayerSpeed(SpectatorID[i], 0)); strcat(specstring, temp3); }
			PlayerTextDrawSetString(i, SpecText[i], specstring);
		}
	}
	return 1;
}

function CheckFuel()
{
	foreach(new i : Player)
	{
	    if(IsPlayerInRangeOfFire(i, 3.5) && !IsPlayerInRangeOfFire(i, 1.5))
	    {
			new Float: HP;
			GetPlayerHealthEx(i, HP);
			if(HP != 100) {
				if(HP < 95) SetPlayerHealthEx(i, HP+5);
				else SetPlayerHealthEx(i, 100);
				ShowGainScreen(i, 20);
			}
	    }
		if(GetPlayerState(i) != PLAYER_STATE_DRIVER) continue;
		if(IsAFK[i]) continue;

		new vehicle = GetPlayerVehicleID(i);
		if(Fuel[vehicle] > 0)
		{
			new Speed = GetPlayerSpeed(i, 0);
            new engine,lights,alarm,doors,bonnet,boot,objective;
			GetVehicleParamsEx(vehicle,engine,lights,alarm,doors,bonnet,boot,objective);
			if(engine == VEHICLE_PARAMS_ON && Speed > 10 && IsAFK[i] == 0)
			{
				if(GetVehicleType(vehicle) == 2 || GetVehicleType(vehicle) == 4 || GetVehicleType(vehicle) == 5 || GetVehicleType(vehicle) == 9 || IsADMVCar(vehicle) || IsAPizzaCar(vehicle) || IsAMechanicCar(vehicle))
				{
					Fuel[vehicle]++;
				}
				Fuel[vehicle]--;
			}
		}
		else
		{
            new lights,alarm,doors,bonnet,boot,objective;
			SetVehicleParamsEx(vehicle,VEHICLE_PARAMS_OFF,lights,alarm,doors,bonnet,boot,objective);
		}

		if(IsAnOwnableCar(vehicle))
        {
			foreach(new s : Player) {
				for(new v; v < MAX_PERSONAL_VEHICLES; v++)
				{
					if(PlayerInfo[s][pCarID][v] == vehicle)
					{
						if(GetPlayerState(i) == PLAYER_STATE_DRIVER && GetPlayerVehicleID(i) == vehicle)
						{
							new Float:carhealth;
							GetVehicleHealth(vehicle, carhealth);
							GetVehicleDamageStatus(vehicle, PlayerInfo[s][pCarDamage1][v], PlayerInfo[s][pCarDamage2][v], PlayerInfo[s][pCarDamage3][v], PlayerInfo[s][pCarDamage4][v]);
							PlayerInfo[s][pCarHP][v] = carhealth;
							PlayerInfo[s][pCarGas][v] = Fuel[vehicle];
							cUpdate(s, v, cHPx);
							cUpdate(s, v, cDamage1x);
							cUpdate(s, v, cDamage2x);
							cUpdate(s, v, cDamage3x);
							cUpdate(s, v, cDamage4x);
							cUpdate(s, v, cGasx);
							cUpdate(s, v, cKMx);
						}
					}
				}
			}
		}
	}
	return 1;
}

function DespawnCar(playerid) {
	for(new v; v < MAX_PERSONAL_VEHICLES; v++)
	{
		if(PlayerInfo[playerid][pCarID][v] != 0)
		{
			if(strlen(CarVipName[playerid][v]) > 0) {
				DestroyObject(vObject[PlayerInfo[playerid][pCarID][v]]);
				vObject[PlayerInfo[playerid][pCarID][v]] = 0;
			}
			DestroyObject(vObject[PlayerInfo[playerid][pCarID][v]]);
			vObject[PlayerInfo[playerid][pCarID][v]] = 0;
			DestroyVehicleEx(PlayerInfo[playerid][pCarID][v], 38833);
			PlayerInfo[playerid][pCarID][v] = 0;
		}
	}
	return 1;
}

