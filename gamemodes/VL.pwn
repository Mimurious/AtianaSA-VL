public OnPlayerExitVehicle(playerid, vehicleid)
{
	if(boom[playerid]) boom[playerid] = 0, KillTimer(turboTimer[playerid]);
    StopAudioStreamForPlayer(playerid);
	if(IsAnOwnableCar(vehicleid))
	{
		foreach(new i : Player)
		{
			for(new v; v < MAX_PERSONAL_VEHICLES; v++) {
				if(PlayerInfo[i][pCarID][v] == vehicleid) {
					GetVehiclePos(vehicleid, PlayerInfo[i][pCarLLocX][v], PlayerInfo[i][pCarLLocY][v], PlayerInfo[i][pCarLLocZ][v]);
					GetVehicleZAngle(vehicleid, PlayerInfo[i][pCarAngle2][v]);
					cUpdate(i, v, cLocationx2x);
					cUpdate(i, v, cLocationy2x);
					cUpdate(i, v, cLocationz2x);
					cUpdate(i, v, cAngle2x);
				}
			}
		}
		for(new v; v < MAX_PERSONAL_VEHICLES; v++)
		{
			if(PlayerInfo[playerid][pCarID][v] == vehicleid) {
				SetVehicleParamsForPlayer(vehicleid,playerid,0,0);
				if(PlayerInfo[playerid][pCarVIP][v] == 1) {
				    KillTimer(turboTimer[playerid]);
					boom[playerid] = 0;
				}
			}
		}
	}

	if(RentVehicle[playerid] == vehicleid) {
		RentTimer[playerid] = 300;
	}

    VehDriver[vehicleid] = INVALID_PLAYER_ID;

	if(boom[playerid] == 1) {
		boom[playerid] = 0;
		KillTimer(turboTimer[playerid]);
	}
	KillTimer(vtime[playerid]);
	if(JobWorking[playerid] == 1)
	{
		JobWorking[playerid] = 0;
		if(JobVehicle[playerid] != 0) { DestroyVehicleEx(JobVehicle[playerid], 11654); JobVehicle[playerid] = 0; }
		DisablePlayerCheckpoint(playerid);
		JobVehicle[playerid] = 0;
		CheckPoint[playerid] = 0;
		CCP[playerid] = false;
		HideMowerObjects(playerid);
		HideFarmerObjects(playerid);
		HideDealerObjects(playerid);
		DestroyVehicleEx(FarmTrailer[playerid], 11662);
		if(Trailer[playerid] != 0) { DestroyVehicleEx(Trailer[playerid], 11663); Trailer[playerid] = 0; TruckerRoute[playerid] = 15; }
		PlayerTextDrawHide(playerid, TruckTD[playerid]);
		SendEngMessage(playerid,COLOR_ERROR,"Mission failed, you got out of your vehicle.");
		SendPerMessage(playerid,COLOR_ERROR,"Shoma az vasileye naghliyeye khod piyade shodid va mamoriyat laghv shod.");
	}
	if(InRace[playerid])
		PutPlayerInVehicleEx(playerid, vehicleid, 0);
	else if(DrivingTest[playerid] == 1 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		if(DMVVeh[playerid]) DestroyVehicleEx(DMVVeh[playerid], 11672);
		RadioCH[DMVVeh[playerid]] = 0;
		DMVCar[DMVVeh[playerid]] = 0;
		DMVVeh[playerid] = 0;
		DCheck[playerid] = 0;
		DrivingTest[playerid] = 0;
		DisablePlayerRaceCheckpoint(playerid);
		SendEngMessage(playerid, COLOR_LIGHTRED, "You failed the test. Go back and start it again.");
		SendPerMessage(playerid, COLOR_LIGHTRED, "Shoma dar test rad shodid. Lotfan mojadadan talash konid.");
		PlayerTextDrawHide(playerid, DMVText[playerid]);
		SetPlayerVirtualWorld(playerid, 0);
	}
	else if(FlyingTest[playerid] == 1 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		FCheck[playerid] = 0;
		FlyingTest[playerid] = 0;
		DisablePlayerRaceCheckpoint(playerid);
		SendEngMessage(playerid, COLOR_LIGHTRED, "You failed the test. Go back and start it again.");
		SendPerMessage(playerid, COLOR_LIGHTRED, "Shoma dar test rad shodid. Lotfan mojadadan talash konid.");
		SetVehicleToRespawn(vehicleid);
		PlayerTextDrawHide(playerid, DMVText[playerid]);
	}
	else if(SailingTest[playerid] == 1 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		SCheck[playerid] = 0;
		SailingTest[playerid] = 0;
		DisablePlayerRaceCheckpoint(playerid);
		SendEngMessage(playerid, COLOR_LIGHTRED, "You failed the test. Go back and start it again.");
		SendPerMessage(playerid, COLOR_LIGHTRED, "Shoma dar test rad shodid. Lotfan mojadadan talash konid.");
		SetVehicleToRespawn(vehicleid);
		PlayerTextDrawHide(playerid, DMVText[playerid]);
	}
	return 1;
}

