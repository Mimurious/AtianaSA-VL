function StartGunTest(playerid)
{
	GunTest[playerid] = true;
	ResetPlayerWeapons(playerid);
	GivePlayerWeapon(playerid,24,9999);
	SetPlayerPosEx(playerid, 2461.7842, -1897.6841, 13.5469);

	for(new i = 0; i < sizeof(gobj); i++)
	{
		new Float:x = getRand(2471.13281, 2477.46216);
		new Float:z = getRand(12.91844, 14.02351);
		gobj[i] = CreateDynamicObject(1600, x, -1891.94641, z, 0.00000, 0.00000, 0.00000);
	}

	for(new i = 0; i < sizeof(gobj); i++)
	{
		if(!IsValidDynamicObject(gobj[i])) continue;
		new Float:x, Float:y, Float:z;
		GetDynamicObjectPos(gobj[i], x, y, z);
		new speed = 3 + random(3);
		MoveDynamicObject(gobj[i], x, -1902.19995, z, speed);
	}
	GunO = 1;
	SetTimer("ResetGun", 300000, 0);
	return 1;
}

function ResetGun()
{
	foreach(new i : Player)
	{
		if(GunTest[i] == true)
		{
			for(new o = 0; o < sizeof(gobj); o++)
			{
				if(!IsValidDynamicObject(gobj[o])) continue;
				DestroyDynamicObject(gobj[o]);
			}
			GunTest[i] = false;
			GunScore[i] = 0;
			ResetPlayerWeapons(i);
			SetPlayerPosEx(i, 2457.6025,-1901.6090,13.5469);
		}
	}
	GunO = 0;
	return 1;
}

