				SetPlayerPosEx(playerid, px - (carid / 2), py + (carid / 2), pz);
			}
			if(pa >= 67.5 && pa <= 112.5) //w
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px - carid, py, pz);
			}
			if(pa >= 112.5 && pa <= 157.5) //sw
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px - (carid / 2), py - (carid / 2), pz);
			}
			if(pa >= 157.5 && pa <= 202.5) //s
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px, py - carid, pz);
			}
			if(pa >= 202.5 && pa <= 247.5)//se
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px, py - (carid / 2), pz);
			}
			if(pa >= 247.5 && pa <= 292.5)//e
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px + carid, py, pz);
			}
			if(pa >= 292.5 && pa <= 332.5)//e
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px + (carid / 2), py + (carid / 2), pz);
			}
			if(pa >= 332.5 && pa < 360.0) //n2
			{
				GetPlayerPos(playerid, px, py, pz);
				SetPlayerPosEx(playerid, px, py + (carid / 2), pz);
			}
		}

		format(gString, sizeof(gString), "%s has teleported %s by %.2f.", GetName(playerid), (carid >= 0 ? "front" : "rear"), (carid >= 0 ? carid : -carid));
