cmd:veh(playerid, params[])
{
	new modelv[32], col1, col2, vsplate[18], Float:ox, Float:oy, Float:oz, Float:of;
	new Float:X,Float:Y,Float:Z,Float:Angle;
	new Float:redundancyX = 0, Float:redundancyY = 0, Float:redundancyZ = 0;
	if(PlayerInfo[playerid][pAdmin] < 4) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[32]I(-1)I(-1)F(0)F(0)F(0)F(0)",modelv,col1,col2, ox, oy, oz, of)) return SendSyntaxMessage(playerid, "/veh <ModelID> <Color1> <Color2> <RedundancyX> <RedundancyY> <RedundancyZ> <RedundancyFront>");
	{
		new model;
		if(IsNumeric(modelv))
		{
			model = strval(modelv);
			if(DebugMode == false) if(model < 400 || model > 611) return SendClientMessage(playerid, COLOR_ERROR, "Model must be between 400 and 611.");
		}
		else
		{
			for(new i; i < 212; i++)
			{
				if(strfind(VehicleNames[i], modelv, true) != -1)
				{
					model = 400+i;
				}
			}
			if(model == 0) return SendClientMessage(playerid, COLOR_ERROR, "Invalid model name.");
		}
		format(gString, sizeof(gString), "%s has spawned a(n) %s.",GetName(playerid), VehicleNames[model - 400]);
		CMDRaport(gString, 1, 0);

		GetPlayerPos(playerid, X,Y,Z);
		GetPlayerFacingAngle(playerid, Angle);
		if(ox) redundancyX += ox;
		if(oy) redundancyY += oy;
		if(oz) redundancyZ += oz;
		if(of)
		{
			if(Angle >= 0.0 && Angle <= 22.5) redundancyY += of;
			if(Angle >= 332.5 && Angle < 0.0) redundancyY += of/2;
			if(Angle >= 22.5 && Angle <= 67.5)
			{
				redundancyX += of/2;
				redundancyY += of/2;
			}
			if(Angle >= 67.5 && Angle <= 112.5) redundancyX -= of;
			if(Angle >= 112.5 && Angle <= 157.5)
			{
				redundancyX -= of/2;
				redundancyY -= of/2;
			}
			if(Angle >= 157.5 && Angle <= 202.5) redundancyY -= of;
			if(Angle >= 202.5 && Angle <= 247.5) redundancyY -= of;
			if(Angle >= 247.5 && Angle <= 292.5) redundancyX += of;
			if(Angle >= 292.5 && Angle <= 332.5)
			{
				redundancyX += of/2;
				redundancyY += of/2;
			}
		}

		VehToDestroy[counter] = CreateVehicleEx(model, X + redundancyX, Y + redundancyY, Z + redundancyZ, Angle, col1, col2, 1800, 0);
		format(gString, sizeof gString, "X: %.2f | Y : %.2f | Z: %.2f ||| rX: %.2f | rY : %.2f | rZ: %.2f ||| tX: %.2f | tY : %.2f | tZ: %.2f", X, Y, Z, redundancyX, redundancyY, redundancyZ, X + redundancyX, Y + redundancyY, Z + redundancyZ);
		if(DebugMode == true) SendClientMessage(playerid, -1, gString);
        new alarm,doors,bonnet,boot,objective;
		SetVehicleParamsEx(VehToDestroy[counter],VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
		Fuel[VehToDestroy[counter]] = 100;
		SetVehicleHealth(VehToDestroy[counter], 999.0);
		LinkVehicleToInterior(VehToDestroy[counter], AntiCheatGetInterior(playerid));
		SetVehicleVirtualWorld(VehToDestroy[counter], GetPlayerVirtualWorld(playerid));

		VehCreator[VehToDestroy[counter]] = PlayerInfo[playerid][pSQLID];

		if(PlayerInfo[playerid][pAdmin] == 8) {
			if(counter < 9) format(vsplate, sizeof(vsplate), "{FF0000}MV 00%d", counter+1);
			else format(vsplate, sizeof(vsplate), "{FF0000}MV 0%d", counter+1);
		}
		else {
			if(counter < 9) format(vsplate, sizeof(vsplate), "{E04343}AV 00%d", counter+1);
			else format(vsplate, sizeof(vsplate), "{E04343}AV 0%d", counter+1);
		}

		SetVehicleNumberPlate(VehToDestroy[counter], vsplate);
		PutPlayerInVehicleEx(playerid, VehToDestroy[counter], 0);
		format(gString, sizeof(gString), "Vehicle spawned. [ID: %d] [CID: %d]", counter+1, VehToDestroy[counter]);
		SendClientMessage(playerid, COLOR_SYN2, gString);
		counter ++;
	}
	return 1;
}

