cmd:news(playerid, params[])
{
	new msg[128];
	if(PlayerInfo[playerid][pLeader] + PlayerInfo[playerid][pMember] != 4) return SendClientMessage(playerid, COLOR_ERROR, "Error: you are not part of News Reporters.");
	if(sscanf(params,"s[128]",msg)) return SendSyntaxMessage(playerid, "/news <text>");
	{
		new vid = GetPlayerVehicleID(playerid);
		if(!IsANRCar(vid))
		{
			SendEngMessage(playerid, COLOR_ERROR, "Error: You are not in a News Van/Chopper.");
			SendPerMessage(playerid, COLOR_ERROR, "Shoma savare News Van/Chopper nistid.");
			return 1;
		}
		if(NewsWait > 0)
		{
			format(gString, sizeof(gString), "You need to wait %d seconds to place another news.", NewsWait);
			SendClientMessage(playerid, COLOR_ERROR, gString);
			return 1;
		}

		format(gString, sizeof(gString), "[DEMO-NEWS] News placed by %s (%d): %s", GetName(playerid), playerid, msg);
		new string2[256];
		if(strlen(gString) > 134)
		{
			strmid(string2, gString, 124, strlen(gString));
			strdel(gString, 124, strlen(gString));

			format(gString,256,"%s ...",gString);
			HBroadCast(COLOR_YELLOW, gString, 1);

			format(string2,256,"... %s",string2);
			HBroadCast(COLOR_YELLOW, string2, 1);
		}
		else
		{
			HBroadCast(COLOR_YELLOW, gString, 1);
		}

		News[playerid] = SetTimerEx("DoNews", 20000, false, "i", playerid);
		format(NewsText[playerid], sizeof(NewsText), msg);

		NewsWait += 20;
        LastNews = playerid;
		format(gString, sizeof(gString), "{B8DBFF}Your news will be shown in %d seconds.", NewsWait);
		SendClientMessage(playerid, COLOR_SYN, gString);
	}
	return 1;
}

function DoNews(playerid) {
	new string2[256];

	format(gString, sizeof gString, "NR %s: %s", GetName(playerid), NewsText[playerid]);
	if(strlen(gString) > 120)
	{
		strmid(string2, gString, 120, 256);
