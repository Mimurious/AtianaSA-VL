function LoadEvents()
{
	new arrCoords[4][128];
	new strFromFile2[128];
	new File: file = fopen("events.ini", io_read);
	if (file)
	{
		fread(file, strFromFile2);
		split(strFromFile2, arrCoords, '|');
		strmid(event1, arrCoords[0], 0, strlen(arrCoords[0]), 255);
		strmid(event2, arrCoords[1], 0, strlen(arrCoords[1]), 255);
		strmid(event3, arrCoords[2], 0, strlen(arrCoords[2]), 255);
		strmid(event4, arrCoords[3], 0, strlen(arrCoords[3]), 255);
		fclose(file);
	}
	else
	{
		fopen("events.ini", io_write);
		SaveEvents();
		fclose(file);
	}
	return 1;
}

function SaveEvents()
{
	new coordsstring[128];
	format(coordsstring, sizeof(coordsstring), "%s|%s|%s|%s",event1,event2,event3,event4);
	new File: file2 = fopen("events.ini", io_write);
	fwrite(file2, coordsstring);
	fclose(file2);
}

function split(const strsrc[], strdest[][], delimiter)
{
	new i, li;
	new aNum;
	new len;
	while(i <= strlen(strsrc)){
		if(strsrc[i]==delimiter || i==strlen(strsrc)){
			len = strmid(strdest[aNum], strsrc, li, i, 128);
			strdest[aNum][len] = 0;
			li = i+1;
			aNum++;
		}
		i++;
	}
	return 1;
}

cmd:agl(playerid, params[])
{
	new giveplayerid, splayer[25], license[24], hours;
	if(PlayerInfo[playerid][pAdmin] < 3) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[25]s[24]i", splayer, license, hours)) return SendSyntaxMessage(playerid, "/agl <Player ID/Name> <Name> <Hours>"), SendClientMessage(playerid, COLOR_GRAD4, "Available names: Driving Flying Sailing Weapon Pack");
	{
		if(hours > 1000) return SendClientMessage(playerid, COLOR_ERROR, "Invalid hours.");
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[playerid][pAdmin] == 6 && giveplayerid != playerid) return NotEnoughPermission(playerid);
		if(strmatch(license,"driving"))
		{
			PlayerInfo[giveplayerid][pDrivingLic] = hours;
			Update(giveplayerid, pDrivingLicx);
		}
		else if(strmatch(license,"flying"))
		{
			PlayerInfo[giveplayerid][pFlyingLic] = hours;
			Update(giveplayerid, pFlyingLicx);
		}
		else if(strmatch(license,"sailing"))
		{
			PlayerInfo[giveplayerid][pSailLic] = hours;
			Update(giveplayerid, pSailLicx);
		}
		else if(strmatch(license,"weapon"))
		{
			if(PlayerInfo[giveplayerid][pGunLic] < 0) ResetPlayerWeapons(giveplayerid);
			PlayerInfo[giveplayerid][pGunLic] = hours;
			Update(giveplayerid, pGunLicx);
		}
		else if(strmatch(license,"fishing"))
		{
			PlayerInfo[giveplayerid][pFishingLic] = 1;
			Update(giveplayerid, pFishingLicx);
		}
		else if(strmatch(license,"pack"))
		{
			PlayerInfo[giveplayerid][pDrivingLic] = hours;
			Update(giveplayerid, pDrivingLicx);
			PlayerInfo[giveplayerid][pFlyingLic] = hours;
			Update(giveplayerid, pFlyingLicx);
			PlayerInfo[giveplayerid][pSailLic] = hours;
			Update(giveplayerid, pSailLicx);
			if(PlayerInfo[giveplayerid][pGunLic] < 0) ResetPlayerWeapons(giveplayerid);
			PlayerInfo[giveplayerid][pGunLic] = hours;
			Update(giveplayerid, pGunLicx);
			PlayerInfo[giveplayerid][pFishingLic] = 1;
			Update(giveplayerid, pFishingLicx);
		}
		else return SendSyntaxMessage(playerid, "/agl <Player ID/Name> <Name> <Hours>"), SendClientMessage(playerid, COLOR_GRAD4, "Available names: Driving Flying Sailing Weapon Pack");
		format(gString, sizeof(gString), "You've given a %s license to %s.",license,GetName(giveplayerid));
		SendEngMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Admin %s has given you a %s license.",GetName(playerid),license);
		SendEngMessage(giveplayerid, COLOR_SYN2, gString);

		format(gString, sizeof(gString), "Shoma mojaveze %s be %s dadid.",license,GetName(giveplayerid));
		SendPerMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Admin %s be shoma yek mojaveze %s dad.",GetName(playerid),license);
		SendPerMessage(giveplayerid, COLOR_SYN2, gString);

		format(gString, sizeof(gString), "%s has given a %s license to %s.",GetName(playerid),license,GetName(giveplayerid));
		CMDRaport(gString, 1, 0);
	}
	return 1;
}

