stock ChangeSwear(Text[], Symbol = '*')
{
	new iWords = sizeof(BadWords), i;
	new xl = 0;
	for (new BadWord; BadWord != iWords; ++BadWord)
	{
	    while ((i = strfind(Text, BadWords[BadWord], true)) != -1)
	    {
	        for (new x = (i + strlen(BadWords[BadWord])); i != x; ++i)
			{
				Text[i] = Symbol;
				xl++;
			}
		}
	}
	return true;
}

