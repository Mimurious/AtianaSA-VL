			SendPerMessageToAll(COLOR_LIGHTGREEN, gString);
			ChatLog(playerid, text, 2);
		}
	}
	else
	{
		if(Undercover[playerid] == 1)
		{
			format(gString, sizeof(gString), "Unknown said: %s", text);
			ProxDetectorEn(20.0, playerid, gString, COLOR_FADE1, COLOR_FADE2, COLOR_FADE3, COLOR_FADE4, COLOR_FADE5);
			format(gString, sizeof(gString), "Unknown goft: %s", text);
