stock GetZoneName(Float:x , Float: y, zone[], len)
{
	for(new i = 0; i != sizeof(SAZones); i++ )
	{
		if(x >= SAZones[i][SAZONE_AREA][0] && x <= SAZones[i][SAZONE_AREA][3] && y >= SAZones[i][SAZONE_AREA][1] && y <= SAZones[i][SAZONE_AREA][4])
		{
			return format(zone, len, SAZones[i][SAZONE_NAME], 0);
		}
	}
	return 0;
}

public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ)
{
	if(PlayerInfo[playerid][pAdmin] >= 1 || PlayerInfo[playerid][pHelper] >= 1)
	{
		new Float:health;
		GetPlayerHealthEx(playerid,health);
		if(health < 20) return SendClientMessage(playerid,COLOR_ERROR,"Error: You cannot teleport because you need at least 20 HP.");
		if(JobWorking[playerid] == 1 && PlayerInfo[playerid][pSQLID] != 2) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are already working in a job.");
		if(IsInWar1(playerid) || IsInWar2(playerid)) return SendClientMessage(playerid,COLOR_ERROR,"Error: You are in a war.");
		MapAndreas_FindZ_For2DCoord(fX, fY, fZ);
		if (GetPlayerState(playerid) == 2)
		{
			new gcar = GetPlayerVehicleID(playerid);
			SetVehiclePos(gcar, fX, fY, fZ + 2);
			new zone[256];
			GetZoneName(fX , fY, zone, sizeof zone);
			format(gString, sizeof(gString), "%s has teleported to %s [%.2f, %.2f, %.2f] by vehicle.",GetName(playerid), zone, fX, fY, fZ);
			if(PlayerInfo[playerid][pAdmin]) CMDRaport(gString, 1, 0);
			else CMDRaport(gString, 0, 1);
		}
		else
		{
			SetPlayerPosFindZ(playerid, fX, fY, fZ + 2);
			new zone[256];
			GetZoneName(fX , fY, zone, sizeof zone);
			format(gString, sizeof(gString), "%s has teleported to %s [%.2f, %.2f, %.2f].",GetName(playerid), zone, fX, fY, fZ);
			if(PlayerInfo[playerid][pAdmin]) CMDRaport(gString, 1, 0);
			else CMDRaport(gString, 0, 1);
		}
		SetPlayerInterior(playerid, 0);
		SetPlayerVirtualWorld(playerid, 0);
		HouseEntered[playerid] = 0;
		BizzEntered[playerid] = 0;
	}
	return 1;
}

public OnDynamicObjectMoved(objectid)
{
	for(new i = 0; i < sizeof(gobj); i++)
	{
		if(!IsValidDynamicObject(gobj[i])) continue;
		if(objectid == gobj[i])
		{
			new Float:x, Float:y, Float:z;
			GetDynamicObjectPos(gobj[i], x, y, z);
			if(y == -1902.19995)
			{
				new speed = 2 + random(3);
				MoveDynamicObject(gobj[i], x, -1891.94641, z, speed);
			}

			if(y == -1891.94641)
			{
				new speed = 2 + random(3);
				MoveDynamicObject(gobj[i], x, -1902.19995, z, speed);
			}
		}
	}
	return 1;
}

public OnPlayerEnterDynamicArea(playerid, areaid)
{
	if(areaid == PartyArea)
	{
		if(strlen(PartyMusic) > 1) CustomStream(playerid, PartyMusic);
	}
	else
	{
		if(IsInWar1(playerid))
		{
			new zone = GetPlayerTurf(playerid);
			foreach(new i : Player)
			{
				if(zone != -1 && TakeON[zone] == 1 && IsInWar1(i) && IsPlayerInTurf(i, zone) && IsPlayerInDynamicArea(i, areaid))
				{
					SetPlayerMarkerForPlayer( playerid, i, GetPlayerColor( i ) );
					SetPlayerMarkerForPlayer( i, playerid, GetPlayerColor( playerid ) );
				}
				else
				{
					SetPlayerMarkerForPlayer( playerid, i, ( GetPlayerColor( i ) & 0xFFFFFF00 ) );
					SetPlayerMarkerForPlayer( i, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
				}
			}
		}
		if(IsInWar2(playerid))
		{
			new zone = GetPlayerTurf(playerid);
			foreach(new i : Player)
			{
				if(zone != -1 && TakeON[zone] == 2 && IsInWar2(i) && IsPlayerInTurf(i, zone) && IsPlayerInDynamicArea(i, areaid))
				{
					SetPlayerMarkerForPlayer( playerid, i, GetPlayerColor( i ) );
					SetPlayerMarkerForPlayer( i, playerid, GetPlayerColor( playerid ) );
				}
				else
				{
					SetPlayerMarkerForPlayer( playerid, i, ( GetPlayerColor( i ) & 0xFFFFFF00 ) );
					SetPlayerMarkerForPlayer( i, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
				}
			}
		}
	}
	return 1;
}

public OnVehicleStreamIn(vehicleid, forplayerid)
{
	ChangeVehicleColor(vehicleid, vcolor1[vehicleid], vcolor2[vehicleid]);
	for(new v; v < MAX_PERSONAL_VEHICLES; v++)
	{
		if(PlayerInfo[forplayerid][pCarID][v] == vehicleid)
		{
			SetVehicleParamsForPlayer(vehicleid,forplayerid,0,0);
		}
	}
	for(new vv; vv < MAX_VEHICLES; vv++)
	{
		if(vv == HireCar[forplayerid])
		{
			SetVehicleParamsForPlayer(vv,forplayerid,0,0);
		}
	}
	for(new vv; vv < MAX_VEHICLES; vv++)
	{
		if(vv == RentVehicle[forplayerid])
		{
			SetVehicleParamsForPlayer(vv,forplayerid,0,0);
		}
	}
	return 1;
}

public OnVehicleStreamOut(vehicleid, forplayerid)
{
	ChangeVehicleColor(vehicleid, vcolor1[vehicleid], vcolor2[vehicleid]);
	return 1;
}

public OnUnoccupiedVehicleUpdate(vehicleid, playerid, passenger_seat, Float:new_x, Float:new_y, Float:new_z, Float:vel_x, Float:vel_y, Float:vel_z)
{
	ChangeVehicleColor(vehicleid, vcolor1[vehicleid], vcolor2[vehicleid]);
	if(GetVehicleDistanceFromPoint(vehicleid, new_x, new_y, new_z) > 50) return 0;
	return 1;
}

public OnPlayerLeaveDynamicArea(playerid, areaid)
{
	if(areaid == PartyArea)
	{
		StopAudioStreamForPlayer(playerid);
	}
	foreach(new i : Player)
	{
		if(IsPlayerInDynamicArea(i, areaid))
		{
			SetPlayerMarkerForPlayer( playerid, i, ( GetPlayerColor( i ) & 0xFFFFFF00 ) );
			SetPlayerMarkerForPlayer( i, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
		}
	}
	return 1;
}

public OnPlayerStreamIn(playerid, forplayerid)
{
	if(Undercover[playerid] == 1)
	{
		TeamColor(playerid);
		SetPlayerMarkerForPlayer( playerid, forplayerid, ( GetPlayerColor( forplayerid ) & 0xFFFFFF00 ) );
		SetPlayerMarkerForPlayer( forplayerid, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
		return 1;
	}
	if(IsInWar1(playerid) && IsInWar1(forplayerid) || IsInWar2(playerid) && IsInWar2(forplayerid))
	{
		new zone = GetPlayerTurf(playerid);
		if(zone == -1 || TakeON[zone] == 0 || !IsPlayerInTurf(forplayerid, zone))
		{
			SetPlayerMarkerForPlayer( playerid, forplayerid, ( GetPlayerColor( forplayerid ) & 0xFFFFFF00 ) );
			SetPlayerMarkerForPlayer( forplayerid, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
		}
	}
	else
	{
		if(PlayerPaintballing[playerid] && PlayerPaintballing[forplayerid] || InClanWar[playerid] && InClanWar[forplayerid])
		{
			SetPlayerMarkerForPlayer( playerid, forplayerid, GetPlayerColor( forplayerid ) );
			SetPlayerMarkerForPlayer( forplayerid, playerid, GetPlayerColor( playerid ) );
		}
		else
		{
			SetPlayerMarkerForPlayer( playerid, forplayerid, ( GetPlayerColor( forplayerid ) & 0xFFFFFF00 ) );
			SetPlayerMarkerForPlayer( forplayerid, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
		}
	}
	return 1;
}

public OnPlayerStreamOut(playerid, forplayerid)
{
	if(IsPlayerCompat(forplayerid))
	{
		if(GetPlayerCustomSkin(playerid) != 0) SetPlayerSkinForPlayer(playerid, forplayerid, GetPlayerSkin(playerid));
	}

	if(!IsInWar1(playerid) && !IsInWar2(playerid) && !IsInWar1(forplayerid) && !IsInWar2(forplayerid))
	{
		SetPlayerMarkerForPlayer( playerid, forplayerid, ( GetPlayerColor( forplayerid ) & 0xFFFFFF00 ) );
		SetPlayerMarkerForPlayer( forplayerid, playerid, ( GetPlayerColor( playerid ) & 0xFFFFFF00 ) );
	}
	return 1;
}

GetSQLIDByPlayerName(name[25])
{
	new id = -1;
    if(!ExistName(name)) return -1;
	gQuery[0] = (EOS);
	mysql_format(Database, gQuery, sizeof(gQuery), "SELECT `ID` FROM `users` WHERE `Name` = '%s'", name);
	new Cache:result = mysql_query(Database, gQuery);
	if(cache_num_rows()) cache_get_value_name_int(0, "ID", id);
	cache_delete(result);

	return id;
}

GetPlayerNameBySQLID(iD)
{
	new name[25] = "Unknown";
	if(iD == -1) return name;
	gQuery[0] = (EOS);
	mysql_format(Database, gQuery, sizeof(gQuery), "SELECT `Name` FROM `users` WHERE `ID` = '%d'", iD);
	new Cache:result = mysql_query(Database, gQuery);
	if(cache_num_rows()) cache_get_value_name(0, "Name", name);
	cache_delete(result);

	return name;
}

public OnVehicleMod(playerid, vehicleid, componentid)
{
	new vehicleide = GetVehicleModel(vehicleid);
	new modok = islegalcarmod(vehicleide, componentid);
	if (!modok) {
        SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due getting out of sync or using mods/abnormal clients.");
        format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} Using illegal vehicle components detected on %s (%d) and got kicked.", GetName(playerid), playerid);
		ABroadCast(COLOR_LIGHTRED, gString, 1);
		return Kick(playerid);
	}

	ChangeVehicleColor(vehicleid, vcolor1[vehicleid], vcolor2[vehicleid]);
	SetVehicleHealth(vehicleid, ModShopH[vehicleid]);
	if(IsAnOwnableCar(vehicleid))
	{
		foreach(new i : Player)
		{
			for(new v; v < MAX_PERSONAL_VEHICLES; v++)
			{
				if(PlayerInfo[i][pCarID][v] == vehicleid)
					cUpdate(playerid, v, cModsx);
			}
		}
		SaveComponent(vehicleid, componentid);
	}

	return 1;
}

