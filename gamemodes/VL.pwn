			format(gString, sizeof(gString), "{00FF00}[AD]{FFFFFF} %s (whisper to %s): {FF0000}%s", GetName(playerid), GetName(giveplayerid), msg);
			ABroadCast(COLOR_LIGHTRED, gString, 1);
		}
		ChangeSwear(msg);

		format(gString, sizeof(gString), "%s whispers: {FFFFFF}%s", GetName(playerid), msg);
		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
			strdel(gString, 110, 256);

			format(gString,128,"%s...",gString);
			SendClientMessage(giveplayerid, COLOR_YELLOW, gString);

			format(string2,128,"...%s",string2);
			SendClientMessage(giveplayerid, -1, string2);
		}
		else
		{
			SendClientMessage(giveplayerid, COLOR_YELLOW, gString);
		}

		format(gString, sizeof(gString), "You whisper to %s : {FFFFFF}%s", GetName(giveplayerid), msg);
		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
			strdel(gString, 110, 256);

			format(gString,128,"%s...",gString);
			SendClientMessage(playerid,  COLOR_YELLOW, gString);

			format(string2,128,"...%s",string2);
			SendClientMessage(playerid,  -1, string2);
		}
		else
		{
			SendClientMessage(playerid,  COLOR_YELLOW, gString);
		}

		format(gString, sizeof(gString), "%s whispers to %s: %s", GetName(playerid), GetName(giveplayerid), msg);
		foreach(new i : Player)
		{
			if(!ShowWhisper[i])
				continue;

			if(strlen(gString) > 120)
			{
				strmid(string2, gString, 110, 256);
				strdel(gString, 110, 256);

				format(gString,128,"%s...",gString);
