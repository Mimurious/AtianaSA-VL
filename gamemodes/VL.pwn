			format(gString,sizeof(gString),"Basic payoff: $%s", FormatNumber(salar));
			SendClientMessage(playerid, COLOR_GREY, gString);

			if(PlayerInfo[playerid][pPremiumAccount] > gettime())
			{
				format(gString,sizeof(gString),"Premium account bonus (25%s): $%s", "%%", FormatNumber(salar * 25 / 100));
