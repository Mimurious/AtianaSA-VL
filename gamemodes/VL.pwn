function InsertCar(playerid, dealerid, randspawn, premium) {
    new string[256],sendername[25],total,idcar,premiump;
    new model, value;
    if(dealerid == 1) model = D1Info[DealerID[playerid]][d1Model], value = D1Info[DealerID[playerid]][d1Price];
    else if(dealerid == 2) model = D2Info[DealerID[playerid]][d2Model], value = D2Info[DealerID[playerid]][d2Price];
    else if(dealerid == 3) model = D3Info[DealerID[playerid]][d3Model], value = D3Info[DealerID[playerid]][d3Price];
    else if(dealerid == 4) model = D4Info[DealerID[playerid]][d4Model], value = D4Info[DealerID[playerid]][d4Price];
	else return -1;
	
    GetPlayerName(playerid,sendername,sizeof(sendername));
    BuyCar[playerid] = -1;
    if(GetPlayerCash(playerid) < value && premium == 0) return SendClientMessage(playerid,COLOR_ERROR,"You don't have enough money to purchase this vehicle.");
	for(new v; v < MAX_PERSONAL_VEHICLES; v++)
	{
		if(PlayerInfo[playerid][pCar][v] != -1) total++;
	}
	if(total+1 > PlayerInfo[playerid][pCarSlots])
	{
		SendClientMessage(playerid, COLOR_GREY, "You cannot buy any more cars. Sell one of your vehicles if you want to buy a new one or buy a vehicle slot from the shop.");
		return 1;
	}
	for(new v; v < MAX_PERSONAL_VEHICLES; v++)
	{
		if(PlayerInfo[playerid][pCar][v] == -1)
		{
			idcar = v;
			break;
		}
	}

	mysql_format(Database, string, sizeof(string),"INSERT INTO cars (Model) VALUES (%d)", model);
    new Cache:ids = mysql_query(Database,string);
	new idd = cache_insert_id();
