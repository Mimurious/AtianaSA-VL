cmd:invite(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(PlayerInfo[playerid][pLeader] == 0) return NotEnoughPermission(playerid);
	if(PlayerInfo[playerid][pAdmin] == 6) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/invite <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(DebugMode == false) if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot invite yourself.");
		if(PlayerInfo[giveplayerid][pMember] + PlayerInfo[giveplayerid][pLeader] != 0 || PlayerInfo[giveplayerid][pFPunish] > 0) return SendClientMessage(playerid, COLOR_ERROR, "Player is already in a faction/has faction punish.");
		if(FactionMembers(PlayerInfo[playerid][pLeader]) >= SafeInfo[PlayerInfo[playerid][pLeader]][sSlot]) return SendClientMessage(playerid, COLOR_ERROR, "Error: The faction has reached its members limitation.");
		if(PlayerInfo[giveplayerid][pLevel] < SafeInfo[PlayerInfo[playerid][pLeader]][sMinLvl]) return SendClientMessage(playerid, COLOR_ERROR, "Error: The player doesn't have the required faction level.");

		InviteOffer[giveplayerid][playerid] = PlayerInfo[playerid][pLeader];

		format(gString, sizeof(gString), "You invited %s to join in your faction.",GetName(giveplayerid));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Leader %s invited you to join in %s, type ((/acc)ept invite %d) to join in.",GetName(playerid),FactionName[PlayerInfo[playerid][pLeader]],playerid);
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
	}
	return 1;
}

cmd:uninvite(playerid, params[])
{
	new giveplayerid, splayer[25], punish;
	if(PlayerInfo[playerid][pPinLogged] == 0 && strlen(PlayerInfo[playerid][pPin]) != 1) return SendEngMessage(playerid, COLOR_YELLOW2, PINEN), SendPerMessage(playerid, COLOR_YELLOW2, PINFA);
	if(PlayerInfo[playerid][pLeader] == 0) return NotEnoughPermission(playerid);
	if(PlayerInfo[playerid][pAdmin] == 6) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[25]i", splayer, punish)) return SendSyntaxMessage(playerid, "/uninvite <Player ID/Name> <Punishment (+)>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(DebugMode == false) if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot uninvite yourself.");
		if(PlayerInfo[giveplayerid][pMember] != PlayerInfo[playerid][pLeader]) return SendClientMessage(playerid, COLOR_ERROR, "Player is not in your faction.");

		PlayerInfo[giveplayerid][pMember] = 0;
