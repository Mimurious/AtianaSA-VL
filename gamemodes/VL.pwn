cmd:fish(playerid, params[])
{
	if (IsPlayerConnected(playerid))
	{
		new vehicle_id = GetNearestVehicle(playerid, 9.0);

		if (IsAtFishPlace(playerid) || (vehicle_id != INVALID_VEHICLE_ID && IsABoat(vehicle_id)))
		{
			if (!PlayerInfo[playerid][pFishingLic])
				return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have a fishing license.");

			if (IsPlayerInWater(playerid) || Fishing[playerid])
				return SendClientMessage(playerid, COLOR_ERROR, "Error: You are already fishing.");

			if (PlayerFishes[playerid][0] > 0 && PlayerFishes[playerid][1] > 0 && PlayerFishes[playerid][2] > 0 && PlayerFishes[playerid][3] > 0 && PlayerFishes[playerid][4] > 0)
				return SendClientMessage(playerid, COLOR_ERROR, "Error: You have already caught 5 fishes, let them go or sell them.");

			new
			string[144];

			if (IsAtFishPlace(playerid))
			{
				format(string, sizeof(string), "* %s throws his rod into the water as he is fishing from the jetty.", GetName(playerid));
				MeAction(playerid, string);
			}

			else {
				if (!IsAtFishBoatPlace(playerid))
				{
					format(string, sizeof(string), "* %s throws his rod into the water as he is fishing from the %s.", GetName(playerid), VehicleNames[GetVehicleModel(vehicle_id) - 400]);
					MeAction(playerid, string);
				}

				else return SendClientMessage(playerid, COLOR_ERROR, "Please drive away with the boat.");
			}

			ApplyAnimation(playerid, "SWORD", "sword_block", 4.0, 0, 0, 0, 0, 0, 1);
			SetPlayerAttachedObject(playerid, 7, 18632, 6, 0.071, 0.007, 0.011, 0.0, -167.9, 0.0, 1.0, 1.0, 1.0);
			TogglePlayerControllable(playerid, false);
			Fishing[playerid] = true;
			stopfishing[playerid] = SetTimerEx("StopFishing", randomEx(9500, 16000), false, "i", playerid);
		}
		else return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in a boat, at the Big Wheel rods or at any fishing place.");
	}
	return 1;
}

