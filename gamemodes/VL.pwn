{
	new Float:Vx, Float:Vy, Float:Vz, Float:DV, Float:multiple;
	GetVehicleVelocity(vehicleid, Vx, Vy, Vz);
	DV = floatsqroot(Vx*Vx + Vy*Vy + Vz*Vz);
