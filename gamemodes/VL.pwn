}

cmd:cancel(playerid, params[])
{
	new name[64];
	if(sscanf(params,"s[64]", name)) { SendSyntaxMessage(playerid, "/cancel <Name>"); SendClientMessage(playerid, -1, "Available names: Ad, News, Taxi, Repair, Refill, Invite, Ticket, Live, Craps, Checkpoint"); return 1; }
	{
	    if(strmatch(name,"ad"))
	    {
	        if(LastAd != playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: There is no active advertisement for you.");
	    	KillTimer(Advertise[playerid]);
	    	AddWait = 0;
	    	SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
			SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
	    }
	    else if(strmatch(name,"news"))
	    {
	        if(LastNews != playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: There is no active news for you.");
	    	KillTimer(News[playerid]);
	    	NewsWait = 0;
	    	SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
			SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
	    }
		else if(strmatch(name,"drugs"))
		{
			if(DrugOffer[playerid] == INVALID_PLAYER_ID) return SendClientMessage(playerid,COLOR_YELLOW2, "Error: Nobody offered you any drugs.");
			DrugOffer[playerid] = INVALID_PLAYER_ID;
			DrugPrice[playerid] = 0;
			DrugGram[playerid] = 0;
			SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
			SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
		}
		else if(strmatch(name,"repair"))
		{
			foreach(new i : Player)
			{
				if(RepairPrice[i][playerid] != 0) RepairPrice[i][playerid] = 0;
				if(RepairPrice[playerid][i] != 0) RepairPrice[playerid][i] = 0;
			}
			SendEngMessage(playerid, COLOR_SYN2, "You have canceled this operation.");
			SendPerMessage(playerid, COLOR_SYN2, "Shoma in amaliyat ra laghv kardid.");
		}
		else if(strmatch(name,"checkpoint"))
		{
			if(CheckPoint[playerid] == 200 || CheckPoint[playerid] == 210 || CheckPoint[playerid] == 220) return SendClientMessage(playerid,COLOR_YELLOW2, "Error: This checkpoint is for rob.");
			if(IsTowing[playerid] == 1) return SendClientMessage(playerid,COLOR_YELLOW2,"Error: This checkpoints is for tow");
			if(DrivingTest[playerid] == 1) return SendClientMessage(playerid,COLOR_YELLOW2,"Error: This checkpoint is for DMV.");
			if(JobWorking[playerid] == 1) return SendClientMessage(playerid,COLOR_YELLOW2, "Error: Get down first from vehicle!");
