		format(gString, sizeof(gString), "Locked: %s EnterFee: $%s Safe: $%s", locktext, FormatNumber(BizzInfo[bizz][bEntranceCost]), FormatNumber(BizzInfo[bizz][bSafe]));
		SendClientMessage(playerid, COLOR_GRAD1,gString);
	}
	SendClientMessage(playerid, COLOR_SERVER,"_______________________________________");
	return 1;
}

