function IsPlayerInTurf(playerid, zoneid)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:x, Float:y, Float:z;
		GetPlayerPos(playerid, x, y, z);
		if(x >= ZoneInfo[zoneid][zMinX] && x < ZoneInfo[zoneid][zMaxX] && y >= ZoneInfo[zoneid][zMinY] && y < ZoneInfo[zoneid][zMaxY])
		{
			return 1;
		}
	}
	return 0;
}

function GetPlayerTurf(playerid)
{
	for(new i = 1; i < sizeof(ZoneInfo); i++)
	{
		if(IsPlayerInTurf(playerid, i) && AntiCheatGetInterior(playerid) == 0)
		{
			return i;
		}
	}
	return -1;
}

function GetPlayersInTurf(zoneid, teamid)
{
	new count;
	foreach(new i : Player)
	{
		new faction = PlayerInfo[i][pMember] + PlayerInfo[i][pLeader];
		if(IsPlayerConnected(i) && faction == teamid && IsPlayerInTurf(i, zoneid))
		{
			count++;
		}
	}
	return count;
}

