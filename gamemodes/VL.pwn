function OnCheatDetected(playerid, ip_address[], type, code)
{
    if(IsForgeinIP(ip_address)) return 0;
	if(type) {
		format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} %s detected on IP: %s.", chCode[code], ip_address);
		ABroadCast(COLOR_LIGHTRED, gString, 1);
	}
    else
    {
    	HackLog(playerid, chCode[code]);
    	if(AFK[playerid])
    	{
    		TogglePlayerControllable(playerid, false);
    		return 1;
    	}
        switch(code)
        {
        	case 5, 6, 11, 22:
        	{
        		format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} %s detected on %s (%d), he may well be suspected.", chCode[code], GetName(playerid), playerid, playerid);
				ABroadCast(COLOR_LIGHTRED, gString, 1);
        	}
            case 14:
            {
                return 1;
            }
            case 32:
            {
                new Float:x, Float:y, Float:z;
                GetPlayerPos(playerid, x, y, z);
                SetPlayerPosEx(playerid, x, y, z);
            }
            case 15, 18:
            {
            	if(Desync[playerid]) return 0;
        		SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due getting out of sync or using mods/abnormal clients.");
                format(gString, sizeof(gString), "> %s has been kicked by AdmBot, reason: Desync or using mods/abnormal clients [%s].", GetName(playerid), chCode[code]);
				SendEngMessageToAll(COLOR_LIGHTRED, gString);
				format(gString, sizeof(gString), "> %s tavasote AdmBot kick shod, dalil: Desync or using mods/abnormal clients [%s].", GetName(playerid), chCode[code]);
				SendPerMessageToAll(COLOR_LIGHTRED, gString);
				Desync[playerid] = true;
				AntiCheatKickWithDesync(playerid, code);
            }
            case 41: SendClientMessage(playerid, COLOR_LIGHTRED, "Your client version is invalid, make sure you installed a right client. Check: [https://atiana.ir/upgradeasa]");
            default:
            {
                if(code == 9 && FlyMode[playerid]) return 0;

            	if(chdet[playerid] > 5 && !Desync[playerid]) {
	                SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due getting out of sync or using mods/abnormal clients.");
	                SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due getting out of sync or using mods/abnormal clients.");
	                format(gString, sizeof(gString), "> %s has been kicked by AdmBot, reason: Desync or using mods/abnormal clients [%s].", GetName(playerid), chCode[code]);
					SendEngMessageToAll(COLOR_LIGHTRED, gString);
					format(gString, sizeof(gString), "> %s tavasote AdmBot kick shod, dalil: Desync or using mods/abnormal clients [%s].", GetName(playerid), chCode[code]);
					SendPerMessageToAll(COLOR_LIGHTRED, gString);
					Desync[playerid] = true;
					AntiCheatKickWithDesync(playerid, code);
				}
				else {
					format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} %s detected on %s (%d), keep eyes on him with /spec %d.", chCode[code], GetName(playerid), playerid, playerid);
					ABroadCast(COLOR_LIGHTRED, gString, 1);
					chdet[playerid]++;
				}
            }
        }
    }
	return 1;
}

stock IntToBase(number, const base) {
    new str[32];
    if(1 < base < 37) {
        new
            m = 1,
            depth = 0;
        while (m <= number) {
            m *= base;
            depth++;
        }
        for ( ; (--depth) != -1; ) {
            str[depth] = (number % base);
            number = ((number - str[depth]) / base);
            if(str[depth] > 9) str[depth] += 'A'; else str[depth] += '0';
        }
    }
    return str;
}

