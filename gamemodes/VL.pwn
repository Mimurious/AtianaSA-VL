cmd:sellhidden(playerid, params[])
{
    new giveplayerid, splayer[25];
    new id,price,sendername[30],giveplayer[30],string[500],other[128];
	if(sscanf(params, "s[25]i", id,price,other)) return SendSyntaxMessage(playerid, "/sellhidden <Player ID/Name> <Price>");
	if(PlayerInfo[playerid][pPremiumPoints] <= 19) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough premium points to do this.");
	if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
	else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
	else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
	else giveplayerid = GetPlayers(splayer);

	if(SellTradeTimer[playerid] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You already have sent an offer. The offer will expire in 2 minutes.");
	if(MoneyACheck(playerid, price, 1, 10000000)) return 1;
	if(GetPlayerCash(giveplayerid) < price) return SendClientMessage(playerid, COLOR_ERROR, "Error: This player don't have enough money.");
	GetPlayerName(playerid, sendername, sizeof(sendername));
	GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
	if(IsPlayerConnected(giveplayerid))
	{
	    if(IsPlayerInRangeOfPlayer(playerid, giveplayerid, 5.0))
	    {
	    	SellHiddenID[playerid] = giveplayerid;
	    	SellHiddenPrice[playerid] = price;
			format(string, sizeof(string), "Are you sure you want to pay 20 premium points to sell a hidden color to %s?\nYou will receive $%s and %s will receive a hidden color.", giveplayer, FormatNumber(price), giveplayer);
			Dialog_Show(playerid, SELLHIDDEN, DIALOG_STYLE_MSGBOX, "Are you sure?", string, "Yes", "No");
		}
		else return SendClientMessage(playerid, COLOR_ERROR, "Error: This player is not near you.");
	}
	return 1;
}

