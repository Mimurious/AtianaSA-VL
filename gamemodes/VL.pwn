cmd:attack(playerid, params[])
{
	if(!IsAMafiaMember(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not a mafia member.");
	if(!CanStartWar) return SendClientMessage(playerid, COLOR_ERROR, "Wars are disabled now.");
	if(PlayerInfo[playerid][pRank] < 5) return NotEnoughPermission(playerid);

	new zone = GetPlayerTurf(playerid);
	new member = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader];
	if(zone == -1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not on a zone.");
	if(ZoneInfo[zone][zTeam] < 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be in a zone to attack.");
	if(WarPause[member] > gettime())
	{
		format(gString, sizeof(gString), "You need to wait %d seconds.", WarPause[member]-gettime());
		SendClientMessage(playerid, COLOR_ERROR, gString);
		return 1;
	}
	if(ZoneInfo[zone][zHours] > 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot attack this zone now.");
	if(ZoneInfo[zone][zTeam] == member) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot attack your zone.");
	if(TakeON[zone]) return SendClientMessage(playerid, COLOR_ERROR, "Error: This zone is already attacked.");
	if(InWar[ZoneInfo[zone][zTeam]] == 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: This mafia has already a war.");
	if(InWar[member] == 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: Your mafia has already a war.");
	if(War == 1 && War2 == 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: There is already a war in progress.");

	if(War == 0)
	{
		Attackers = member;
		Defenders = ZoneInfo[zone][zTeam];

		format(gString, sizeof(gString), "%s (%s) attacked your turf %s",FactionName[Attackers],GetName(playerid),ZoneInfo[zone][zName]);
		SendFactionMessage(Defenders, COLOR_WAR, gString);

		format(gString, sizeof(gString), "Your mafia (%s) attacked turf %s owned by %s",GetName(playerid),ZoneInfo[zone][zName],FactionName[Defenders]);
		SendFactionMessage(Attackers, COLOR_WAR, gString);

		ScoreAtt = 0.0;
		ScoreDef = 0.0;

		War = 1;

		TakeON[zone] = 1;

		ZoneFlashForAllEx(Zone[zone], GetZoneColor(Attackers));

		new sec = 1000;
		WarTime[0] = 900;
		SetTimerEx("StopWar", sec*WarTime[0], 0, "ii", zone, 1);

		format(gString, sizeof(gString), "Turf %s, Time remaining: 15:00",ZoneInfo[zone][zName]);
		SendFactionMessage(Defenders, COLOR_WAR, gString);
		SendFactionMessage(Attackers, COLOR_WAR, gString);
		format(gString, sizeof(gString), "Turf: %s",ZoneInfo[zone][zName]);
		PlayerTextDrawSetString(playerid, PlayerTextDrawWar[playerid][1], gString);
		InWar[Attackers] = 1;
		InWar[Defenders] = 1;
		//for(new i = GroveStreet[0]; i <= rifa[7]; i++) SetVehicleToRespawn(i);
		format(gString, sizeof(gString), "%s (A) - (D) %s",WarFactionName[Attackers], WarFactionName[Defenders]);
		PlayerTextDrawSetString(playerid, PlayerTextDrawWar[playerid][2], gString);

		format(gString, sizeof(gString), "On turf: %s %d-%d %s",WarFactionName[Attackers],GetPlayersInTurf(zone, Attackers),GetPlayersInTurf(zone, Defenders),WarFactionName[Defenders]);
		PlayerTextDrawSetString(playerid, PlayerTextDrawWar[playerid][4], gString);
		return 1;
	}
	if(War2 == 0)
	{
		Attackers2 = member;
		Defenders2 = ZoneInfo[zone][zTeam];

		format(gString, sizeof(gString), "%s (%s) attacked your turf %s",FactionName[Attackers2],GetName(playerid),ZoneInfo[zone][zName]);
		SendFactionMessage(Defenders2, COLOR_WAR, gString);

		format(gString, sizeof(gString), "Your mafia (%s) attacked turf %s owned by %s",GetName(playerid),ZoneInfo[zone][zName],FactionName[Defenders2]);
		SendFactionMessage(Attackers2, COLOR_WAR, gString);

		ScoreAtt2 = 0.0;
		ScoreDef2 = 0.0;

		War2 = 1;

		TakeON[zone] = 2;

		ZoneFlashForAllEx(Zone[zone], GetZoneColor(Attackers2));

		new sec = 1000;
		WarTime[1] = 900;
		SetTimerEx("StopWar", sec*WarTime[1], 0, "ii", zone, 2);

		format(gString, sizeof(gString), "Turf %s, Time remaining: 15:00",ZoneInfo[zone][zName]);
		SendFactionMessage(Defenders2, COLOR_WAR, gString);
		SendFactionMessage(Attackers2, COLOR_WAR, gString);
		format(gString, sizeof(gString), "Turf: %s",ZoneInfo[zone][zName]);
		PlayerTextDrawSetString(playerid, PlayerTextDrawWar[playerid][1], gString);
		InWar[Attackers2] = 1;
		InWar[Defenders2] = 1;
		//for(new i = GroveStreet[0]; i <= rifa[7]; i++) SetVehicleToRespawn(i);
		format(gString, sizeof(gString), "%s (A) - (D) %s",WarFactionName[Attackers2], WarFactionName[Defenders2]);
		PlayerTextDrawSetString(playerid, PlayerTextDrawWar[playerid][2], gString);
		format(gString, sizeof(gString), "On turf: %s %d-%d %s",WarFactionName[Attackers2],GetPlayersInTurf(zone, Attackers2),GetPlayersInTurf(zone, Defenders2),WarFactionName[Defenders2]);
		PlayerTextDrawSetString(playerid, PlayerTextDrawWar[playerid][4], gString);
		return 1;
	}
	return 1;
}

