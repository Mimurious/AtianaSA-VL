function AfterSpray(playerid, id)
{
	if(IsPlayerConnected(playerid))
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			SetVehicleVirtualWorld(GetPlayerVehicleID(playerid), 0);
			SetPlayerVirtualWorld(playerid, 0);
			foreach(new i : Player)
			{
				if(IsPlayerInVehicle(i, GetPlayerVehicleID(playerid))) SetPlayerVirtualWorld(i, 0);
			}
			if(id == 1)
			{
				SetVehiclePos(GetPlayerVehicleID(playerid), 1024.8258, -1032.1052, 31.5072);
				SetVehicleZAngle(GetPlayerVehicleID(playerid), 90.0);
				TogglePlayerControllableEx(playerid, 1);
			}
			else if(id == 2)
			{
				SetVehiclePos(GetPlayerVehicleID(playerid), 2074.9380, -1831.1598, 13.1756);
				SetVehicleZAngle(GetPlayerVehicleID(playerid), 180.0);
				TogglePlayerControllableEx(playerid, 1);
			}
			else if(id == 3)
			{
				SetVehiclePos(GetPlayerVehicleID(playerid), 488.6419, -1731.3525, 10.8720);
				SetVehicleZAngle(GetPlayerVehicleID(playerid), 260.0);
				TogglePlayerControllableEx(playerid, 1);
			}
			else if(id == 4)
			{
				SetVehiclePos(GetPlayerVehicleID(playerid), -1909.2337, 276.3349, 41.0468);
				SetVehicleZAngle(GetPlayerVehicleID(playerid), 270.0);
				TogglePlayerControllableEx(playerid, 1);
			}
		}
	}
	return 1;
}

function LoggedPlayers()
{
	new count;
	foreach(new i : Player) if(gLogged[i] == 1) count ++;
	return count;
}

function CheckEvent()
{
	if(LoggedPlayers() >= EPlayers)
	{
		format(gString,sizeof(gString),"The server reached %d players online.",LoggedPlayers());
		SendEngMessageToAll(0xFFB870FF, gString);
		format(gString,sizeof(gString),"All players received $%s and %d experience points.",FormatNumber(EMoney),ERespect);
		SendEngMessageToAll(0xFFB870FF, gString);
		format(gString,sizeof(gString),"Tedade bazikonane server be %d nafar resid.",LoggedPlayers());
		SendPerMessageToAll(0xFFB870FF, gString);
		format(gString,sizeof(gString),"Tamamiye bazikonan $%s va %d emtiyaze tajrobe daryaft kardand.",FormatNumber(EMoney),ERespect);
		SendPerMessageToAll(0xFFB870FF, gString);

		foreach(new i : Player)
		{
			if(gLogged[i] == 1)
			{
				GivePlayerCash(i, EMoney);
				PlayerInfo[i][pRespect] += ERespect;
				Update(i,pRespectx);
				BuyLevelMsg(i);
			}
		}
		EPlayers = 1000;
		EMoney = 0;
		ERespect = 0;
	}
	return 1;
}

stock IsPlayerSpawned(playerid)
{
    return 0 <= playerid < MAX_PLAYERS && GetPlayerState(playerid) != PLAYER_STATE_NONE && GetPlayerState(playerid) != PLAYER_STATE_WASTED && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING;
}

function LoginKick(playerid)
{
    format(aString, sizeof(aString), "* %s has been kicked from the server (%s).", GetName(playerid), "Overing login deadline");
	HBroadCast(COLOR_YELLOW, aString, 1);
	Kick(playerid);
	SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due failing to login.");
	return 1;
}

function NumIp(ip[], playerid)
{
	new ips = 0;
	foreach(new i : Player)
	{
		if(i == playerid) continue;
		new playerip[16];
		GetPlayerIp(i,playerip,sizeof(playerip));
		if(strmatch(ip, playerip)) ips++;
	}
	return ips;
}

function IsPremiumOnIP(ip[])
{
	foreach(new i : Player)
	{
		new playerip[16];
		GetPlayerIp(i,playerip,sizeof(playerip));
		if(strmatch(ip, playerip) && PlayerInfo[i][pPremiumAccount] > gettime()) return 1;
	}
	return 0;
}

function IsAdminOnIP(ip[])
{
	foreach(new i : Player)
	{
		new playerip[16];
		GetPlayerIp(i,playerip,sizeof(playerip));
		if(strmatch(ip, playerip) && PlayerInfo[i][pAdmin] > 0) return 1;
	}
	return 0;
}

function IsBotOnIP(ip[])
{
	foreach(new i : Player)
	{
		new playerip[16];
		GetPlayerIp(i,playerip,sizeof(playerip));
		if(strmatch(ip, playerip) && IsBot[i] > 0) return 1;
	}
	return 0;
}

function IsLocalIP(ip[])
{
	if(strmatch(ip, "127.0.0.1")) return 1;
	else return 0;
}

function IsForgeinIP(ip[])
{
	if(IsLocalIP(ip)) return 1;
	else if(strmatch(ip, "178.22.121.87")) return 1;
	else if(strmatch(ip, "10.201.72.82")) return 1;
	else return 0;
}

function CountD(playerid, range)
{
	new string[64];
	foreach(new i : Player)
	{
		if(ProxDetectorS(range, playerid, i))
		{
			if(CountDown == 0)
			{
				TogglePlayerControllableEx(i, 1);
				format(string, sizeof(string), "~r~%s", CountMSG);
				GameTextForPlayer(i, string, 1000, 6);
				KillTimer(counttimer);
			}
			else
			{
				format(string, sizeof(string), "~y~%d", CountDown);
				GameTextForPlayer(i, string, 1000, 6);
				if(CountFreeze) TogglePlayerControllableEx(i, 0);
			}
		}
	}
	if(CountDown > 0) CountDown --;
	if(CountFreeze) CountFreeze = 0;
	return 1;
}

function VerifyPickup(pickup)
{
	for(new i = 0; i < sizeof(Pickups); i++) if(Pickups[i] == pickup) return i+1;
	return 0;
}

function ValidPickup(playerid, pickup)
{
	new pickupv[75];
	sscanf(PlayerInfo[playerid][pPlayerPickups], "p<|>e<iiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiii>", pickupv);
	for(new i = 0; i < sizeof(pickupv); i++) if(pickupv[i] == pickup) return 1;
	return 0;
}

function RemovePickup(playerid, pickup)
{
	new pickupv[75];
	sscanf(PlayerInfo[playerid][pPlayerPickups], "p<|>e<iiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiii>", pickupv);
	for(new i = 0; i < sizeof(pickupv); i++) if(pickupv[i] == pickup) pickupv[i] = 0;
	new strn[16];
	format(PlayerInfo[playerid][pPlayerPickups], 16, "%d", pickupv[0]);
	for(new i = 1; i < sizeof(pickupv); i++)
	{
		format(strn, sizeof(strn), "%d|", pickupv[i]);
		strins(PlayerInfo[playerid][pPlayerPickups], strn, 0);
	}
	Update(playerid, pPlayerPickupsx);

	new remainpickups = 0;
	for(new i = 0; i < sizeof(pickupv); i++) if(pickupv[i] != 0) remainpickups++;
	if(remainpickups)
	{
		format(gString, sizeof(gString), "You need to find others %d bundles.", remainpickups);
