cmd:safe(playerid, params[])
{
	new name[128],action[128],amount;
	if(!IsNearSafe(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not near faction's safe.");
	new id = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader];
	if(sscanf(params,"s[128]S(s)[128]I(0)", name,action,amount)) {
		SendSyntaxMessage(playerid, "/safe <Name> <Action> <Amount (+)>");
		if(id == 8 || id == 9 || id == 10 || id == 11) SendClientMessage(playerid, -1, "Available names: Money Drugs Mats");
		else SendClientMessage(playerid, -1, "Available names: Money");
		return 1;
	}
	{
		if(strmatch(name,"money"))
		{
			if(strlen(action) < 2) { SendSyntaxMessage(playerid, "/safe money <Action> <Amount (+)>"); SendClientMessage(playerid, -1, "Available actions: Deposit Withdraw Balance"); return 1; }
			if(strmatch(action,"deposit"))
			{
				if(MoneyACheck(playerid, amount, 1, 10000)) return 1;

				if(GetPlayerCash(playerid) < amount) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");
				format(gString, sizeof(gString), "Old Balance: $%s. You deposited $%s.", FormatNumber(SafeInfo[id][sMoney]),FormatNumber(amount));
				SendClientMessage(playerid, COLOR_YELLOW, gString);
				SafeInfo[id][sMoney] += amount;
				sUpdate(id,sMoneyx);
				GivePlayerCash(playerid,-amount);
				format(gString, sizeof(gString), "New Balance: $%s", FormatNumber(SafeInfo[id][sMoney]));
				SendClientMessage(playerid, COLOR_YELLOW, gString);
				SafeMoneyLog(playerid, "deposited", amount, id, PlayerInfo[playerid][pLeader]);
			}
			else if(strmatch(action,"withdraw"))
			{
				if(PlayerInfo[playerid][pLeader] == 0) return NotEnoughPermission(playerid);
				if(amount <= 0 || amount > 9999999) return SendSyntaxMessage(playerid, "/safe money withdraw <Amount (+)>");

				if(SafeInfo[id][sMoney] < amount) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have that amount of money in your faction safe.");
				SafeInfo[id][sMoney] -= amount;
				sUpdate(id,sMoneyx);
				GivePlayerCash(playerid,amount);
				format(gString, sizeof(gString), "You have withdrawn $%s from the faction safe.Now you have $%s in the faction safe.", FormatNumber(amount),FormatNumber(SafeInfo[id][sMoney]));
				SendClientMessage(playerid, COLOR_YELLOW, gString);
				SafeMoneyLog(playerid, "withdrawn", amount, id, PlayerInfo[playerid][pLeader]);
			}
			else if(strmatch(action,"balance"))
			{
				format(gString, sizeof(gString), "You have $%s in your faction's safe.", FormatNumber(SafeInfo[id][sMoney]));
				SendClientMessage(playerid, COLOR_YELLOW, gString);
			}
		}
		else if(strmatch(name,"drugs") && (id == 8 || id == 9 || id == 10 || id == 11))
		{
			if(!IsAMafiaMember(playerid) && !IsACop(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not a mafia member or a cop.");
			if(strlen(action) < 2) { SendSyntaxMessage(playerid, "/safe drugs <Action> <Amount (+)>"); SendClientMessage(playerid, -1, "Available actions: Put Take Balance"); return 1; }
