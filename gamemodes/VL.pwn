{
	if(HouseEntered[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in your own/rented house.");
	if(HouseEntered[playerid] != PlayerInfo[playerid][pHouseKey] && HouseEntered[playerid] != PlayerInfo[playerid][pRentKey]) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in your own/rented house.");
	new cnt = 0, skins[100], fac = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader], rank;
	if(PlayerInfo[playerid][pLeader] != 0) rank = 7;
	else if(PlayerInfo[playerid][pMember] != 0) rank = PlayerInfo[playerid][pRank];
	if(fac > 0)
	{
		for(new x = 0; x < sizeof(fskins); x++)
		{
			if(fskins[x][0] != fac) continue;
			if(fskins[x][1] > rank) continue;
			skins[cnt] = fskins[x][2];
			cnt++;
		}
	}
	else {
		for(new i = 1; i <= pskins; i++)
		{
			if(SkinInfo[i][sOwner] != PlayerInfo[playerid][pSQLID]) continue;
			skins[cnt] = SkinInfo[i][sModel];
			cnt++;
		}
	}
	if(cnt == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: Your closet is empty, buy clothes from skin stores.");
	ShowModelSelectionMenuEx(playerid, skins, cnt, "Clothes Selection", 66669);

	return 1;
}
	
