cmd:withdraw(playerid, params[])
{
	new money;
	if(BizzEntered[playerid] != 2 && BizzEntered[playerid] != 32 && BizzEntered[playerid] != 33) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be in Bank to withdraw money.");
	if(sscanf(params,"i", money)) return SendSyntaxMessage(playerid, "/withdraw <Amount ($+)>");
	{
		if(money < 1 || PlayerInfo[playerid][pAccount] < money) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money in bank.");

		GivePlayerCash(playerid,money);
		PlayerInfo[playerid][pAccount] -= money;
		Update(playerid,pAccountx);
		format(gString, sizeof(gString), "You Have Withdrawn $%s from your account Total: $%s ", FormatNumber(money),FormatNumber(PlayerInfo[playerid][pAccount]));
		SendClientMessage(playerid, COLOR_YELLOW, gString);
	}
	return 1;
}

cmd:deposit(playerid, params[])
{
	new money;
	if(BizzEntered[playerid] != 2 && BizzEntered[playerid] != 32 && BizzEntered[playerid] != 33) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be in Bank to deposit money.");
	if(sscanf(params,"i", money)) return SendSyntaxMessage(playerid, "/deposit <Amount ($+)>");
	{
		if(money < 1 || GetPlayerCash(playerid) < money) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");

		GivePlayerCash(playerid, -money);
		new old = PlayerInfo[playerid][pAccount];
		PlayerInfo[playerid][pAccount] += money;
		Update(playerid,pAccountx);
		SendClientMessage(playerid, COLOR_WHITE, "|___ BANK STATMENT ___|");
		format(gString, sizeof(gString), "Old Balance: $%s", FormatNumber(old));
		SendClientMessage(playerid, COLOR_GRAD2, gString);
		format(gString, sizeof(gString), "Deposit: $%s",FormatNumber(money));
		SendClientMessage(playerid, COLOR_GRAD4, gString);
		SendClientMessage(playerid, COLOR_GRAD6, "|-----------------------------------------|");
		format(gString, sizeof(gString), "New Balance: $%s", FormatNumber(PlayerInfo[playerid][pAccount]));
		SendClientMessage(playerid, COLOR_WHITE, gString);
	}
	return 1;
}

