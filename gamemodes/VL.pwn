cmd:rac(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 3) return NotEnoughPermission(playerid);
	new bool:occupied[MAX_VEHICLES];
	foreach(new i : Player)
	{
		if(IsPlayerInAnyVehicle(i))
		{
			if(IsTrailerAttachedToVehicle(GetPlayerVehicleID(i))) occupied[GetVehicleTrailer(GetPlayerVehicleID(i))] = true;
			occupied[GetPlayerVehicleID(i)] = true;
		}
	}
	for(new i = 1; i < MAX_VEHICLES; i++)
	{
		if(i == INVALID_VEHICLE_ID) continue;
		if(occupied[i] == true) continue;
		SetVehicleToRespawn(i);
	}
	format(gString, sizeof(gString), "All unoccupied vehicles have been respawned by %s.",GetName(playerid));
	CMDRaport(gString, 1, 0);
	return 1;
}

