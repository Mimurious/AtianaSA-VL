CreatePlaneSeatActor(modelid, Float:X, Float:Y, Float:Z, Float:RX, Float:RY, Float:RZ, type = 0)
{
    CreateObject(modelid, X, Y, Z, RX, RY, RZ);
    if(type == 0) return 1;
    airplane_actors[airplane_actors_num] = CreateActor(AirPlane_skins[random(sizeof(AirPlane_skins))], X, Y-0.5, Z, 180.0);
    SetActorVirtualWorld(airplane_actors[airplane_actors_num], 1);
    airplane_actors_num++;
    return 1;
}

