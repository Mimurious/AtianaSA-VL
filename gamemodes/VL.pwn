		for(new m = 0; m < 5; m++) SendDeathMessageToPlayer(i, 1001, 1001, 200);
	}
	ClanWarPause[ClanWarT1] = gettime() + 120;
	ClanWarPause[ClanWarT2] = gettime() + 120;
