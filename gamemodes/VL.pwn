cmd:buybiz(playerid, params[])
{
	if(IsInWar1(playerid) || IsInWar2(playerid))
		return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot enter in this enter.");
	if(PlayerInfo[playerid][pBizzKey] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You already own a business, type /sellbiz if you want to buy this one.");
	for(new b = 1; b <= businesses_count; b++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 2.0, BizzInfo[b][bEntranceX], BizzInfo[b][bEntranceY], BizzInfo[b][bEntranceZ]) && BizzInfo[b][bOwned] == 0)
		{
			if(PlayerInfo[playerid][pLevel] < BizzInfo[b][bLevel])
			{
				format(gString, sizeof(gString), "You need level %d to purchase this business.", BizzInfo[b][bLevel]);
				SendClientMessage(playerid, COLOR_ERROR, gString);
				return 1;
			}
			if(GetPlayerCash(playerid) < BizzInfo[b][bPrice]) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");

			PlayerInfo[playerid][pBizzKey] = b;
			Update(playerid,pBizzKeyx);

			BizzInfo[b][bOwned] = 1;
			bUpdate(b,bOwnedx);

			strmid(BizzInfo[b][bOwner], GetName(playerid), 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			bUpdate(b,bOwnerx);

			BizzInfo[b][bSafe] = 0;
			bUpdate(b,bSafex);




			GivePlayerCash(playerid,-BizzInfo[b][bPrice]);
			if(BizzInfo[b][bSBiz] == 0)
			{
				SetPlayerInterior(playerid,BizzInfo[b][bInt]);
				SetPlayerPosEx(playerid,BizzInfo[b][bExitX],BizzInfo[b][bExitY],BizzInfo[b][bExitZ]);
				BizzEntered[playerid] = b;
				if(PlayerInfo[playerid][pLanguage] == 0)GameTextForPlayer(playerid, "~w~Welcome", 5000, 3);
				if(PlayerInfo[playerid][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Khosh amadid", 5000, 3);
			}
			SendClientMessage(playerid, COLOR_SYN2, "Congratulations, for your new purchase.");
			SendClientMessage(playerid, COLOR_SYN2, "Type /help to review the new business help section.");
			BizzUpdate(b);
		}
	}
	return 1;
}

