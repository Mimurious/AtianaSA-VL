	if(PlayerInfo[playerid][pLeader] == 0) return NotEnoughPermission(playerid);
	if(PlayerInfo[playerid][pAdmin] == 6) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/fwarn <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(DebugMode == false) if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot faction warn yourself.");
		if(PlayerInfo[giveplayerid][pMember] != PlayerInfo[playerid][pLeader]) return SendClientMessage(playerid, COLOR_ERROR, "Player is not in your faction.");

		PlayerInfo[giveplayerid][pFWarns] += 1;
		Update(giveplayerid,pFWarnsx);
		format(gString, sizeof(gString), "You have been faction warned by leader %s", GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "You have faction warned %s.", GetName(giveplayerid));
		SendClientMessage(playerid, COLOR_SYN2, gString);

		if(PlayerInfo[giveplayerid][pFWarns] >= 3)
		{
			PlayerInfo[giveplayerid][pMember] = 0;
			Update(giveplayerid,pMemberx);
			PlayerInfo[giveplayerid][pRank] = 0;
			Update(giveplayerid,pRankx);
			PlayerInfo[giveplayerid][pSkin] = 250;
			Update(giveplayerid, pSkinx);
			PlayerInfo[giveplayerid][pFPunish] = 10;
			Update(giveplayerid, pFPunishx);
			PlayerInfo[giveplayerid][pFWarns] = 0;
			Update(giveplayerid,pFWarnsx);
			format(gString, sizeof(gString), "You have been kicked with 10FP you from %s because you accumulated 3 faction warns.",FactionName[PlayerInfo[playerid][pLeader]]);
			SendClientMessage(giveplayerid, COLOR_SYN2, gString);
			SpawnPlayer(giveplayerid);
			FactionLog(PlayerInfo[playerid][pLeader], GetDatabaseID(playerid), GetDatabaseID(giveplayerid), "Was kicked out (3 warns)");
