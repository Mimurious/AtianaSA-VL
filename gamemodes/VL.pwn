		if(pa >= 0.0 && pa <= 22.5) //
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px, py+carid, pz);
		}
		if(pa >= 332.5 && pa < 365.0) //n2
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px, py + (carid / 2), pz);
		}
		if(pa >= 22.5 && pa <= 67.5) //nw
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px - (carid / 2), py + (carid / 2), pz);
		}
		if(pa >= 67.5 && pa <= 112.5) //w
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px - carid, py, pz);
		}
		if(pa >= 112.5 && pa <= 157.5) //sw
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px - (carid / 2), py - (carid / 2), pz);
		}
		if(pa >= 157.5 && pa <= 202.5) //s
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px, py - carid, pz);
		}
		if(pa >= 202.5 && pa <= 247.5)//se
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px, py - (carid / 2), pz);
		}
		if(pa >= 247.5 && pa <= 292.5)//e
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px + carid, py, pz);
		}
		if(pa >= 292.5 && pa <= 332.5)//e
		{
			GetPlayerPos(playerid, px, py, pz);
			SetPlayerPosEx(playerid, px + (carid / 2), py + (carid / 2), pz);
		}
	}
	return 1;
}

public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	if(playerid != INVALID_PLAYER_ID && IsPlayerConnected(playerid) && Desync[playerid] && (random(8) < 5)) return 0;
	if(IsPlayerConnected(hitid) && hitid != INVALID_PLAYER_ID) if(qool[hitid] == 1) return 0;
	if(weaponid == 24 && GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) {
        SetPlayerArmedWeapon(playerid, 0);
        return 0;
    }
    if(weaponid != 4 && PlayerInfo[playerid][pMission2] == 1 && !FailedInMission[playerid]) {
        if(PlayerInfo[playerid][pLanguage] == 1) PlayerTextDrawSetString(playerid, MissionSub[playerid], "~r~MA'MORIYAT SHEKAST KHORD!");
	    else PlayerTextDrawSetString(playerid, MissionSub[playerid], "~r~MISSION FAILED!");
	    PlayerTextDrawShow(playerid, MissionSub[playerid]);
	    for(new i; i < 35; i++) ApplyDynamicActorAnimation(actors[0][i], "ped", "cower", 4.1, 1, 1, 1, 1, 0);

	    SendEngMessage(playerid, COLOR_LIGHTRED, "[MISSION] The person understood your move and called the police, you can leave by /leavemission.");
	    SendPerMessage(playerid, COLOR_LIGHTRED, "[MISSION] Farde morede nazar az harekate shoma agah shode va ba police tamas gereft, az /leavemission estefade konid.");
        FailedInMission[playerid] = 1;
	    SetPlayerCriminal(playerid,999,"Trying to murder someone", 1);
    }
	if(IsWeaponWithAmmo(weaponid) && weaponid != 38)
	{
		new count = 0;
		if(weaponid != CurrentWeapon[playerid]) CurrentWeapon[playerid] = weaponid, CurrentAmmo[playerid] = GetPlayerWeaponAmmo(playerid,weaponid), count++;
		if(GetPlayerWeaponAmmo(playerid,weaponid) > CurrentAmmo[playerid] || GetPlayerWeaponAmmo(playerid,weaponid) < CurrentAmmo[playerid])
		{

			CurrentAmmo[playerid] = GetPlayerWeaponAmmo(playerid,weaponid);
			NoReloading[playerid] = 0;
			count++;
		}
		if(GetPlayerWeaponAmmo(playerid,weaponid) != 0 && GetPlayerWeaponAmmo(playerid,weaponid) == CurrentAmmo[playerid] && count == 0)
		{

			NoReloading[playerid]++;
			if(NoReloading[playerid] >= 5)
			{
				format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} %s - Possible noreload/rapidfire. (Limits reached - Desynced)", GetName(playerid));
				ABroadCast(COLOR_LIGHTRED, gString, 1);
				ResetPlayerWeapons(playerid);
				HackLog(playerid, "NoReload/RapidFire");
				NoReloading[playerid] = 0;
				CurrentWeapon[playerid] = 0;
				CurrentAmmo[playerid] = 0;
				DesyncPlayer(playerid);
				return 0;
			}
		}
	}
	new Float:x, Float:y, Float:z;
	if(GunTest[playerid] == true && GunO == 1)
	{
		for(new i = 0; i < sizeof(gobj); i++)
		{
			if(!IsValidDynamicObject(gobj[i])) continue;
			GetDynamicObjectPos(gobj[i], x, y, z);
			if ( IsPlayerAimingAt ( playerid, x, y, z, 0.25 ) )
			{
				DestroyDynamicObject(gobj[i]);
				GunScore[playerid] += 1;
				PlayerPlaySound(playerid,17802,0.0,0.0,0.0);

				if(GunScore[playerid] == 5)
				{
					ResetPlayerWeapons(playerid);
					GivePlayerWeapon(playerid,29,9999);
				}
				if(GunScore[playerid] == 10)
				{
					ResetPlayerWeapons(playerid);
					GivePlayerWeapon(playerid,31,9999);
				}
				if(GunScore[playerid] == 15)
				{
					ResetPlayerWeapons(playerid);
					GivePlayerWeapon(playerid,34,9999);
				}
				if(GunScore[playerid] == 20)
				{
					SetPlayerPosEx(playerid, 2457.6025, -1901.6090, 13.5469);
					GivePlayerCash(playerid, -50);
					SendEngMessage(playerid, COLOR_SYN2, "Congratulations! Now you have a weapon license.");
					SendPerMessage(playerid, COLOR_SYN2, "Tabrik! Shoma hamaknun mojaveze aslahe darid.");
					PlayerInfo[playerid][pGunLic] = 300;
					Update(playerid, pGunLicx);
					GunTest[playerid] = false;
					GunScore[playerid] = 0;
					GunO = 0;
					ResetPlayerWeapons(playerid);
				}
			}
		}
	}
	if(InRob[playerid] != -1 && BankOccupied == 1)
	{

		GetDynamicObjectPos(BankLaser[28], x, y, z);
		if ( IsPlayerAimingAt ( playerid, x, y, z, 1.2) )
		{
			DestroyDynamicObject(BankLaser[28]);
			CreateExplosion(x, y, z, 11, 10.0);
			CreateExplosion(x, y, z, 13, 10.0);
		}
		return 1;
	}

	return 1;
}

