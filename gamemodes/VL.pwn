				else format(gString, sizeof(gString), "");
				format(gString, sizeof(gString), "%s{FFFFFF} (%d) - Modire sathe %d%s", GetName(i), i, PlayerInfo[i][pAdmin], gString);
				SendClientMessage(playerid, COLOR_AC, gString);
				admincounter++;
			}
		}
		if(admincounter > 1)
		{
			format(gString,sizeof(gString),"Dar hale hazer %d modir hozur darand, dar surate boruze hargune moshkel az /report estefade konid.", admincounter);
			SendClientMessage(playerid, COLOR_LIGHTGREEN, gString);
		}
		else if(admincounter == 1) SendClientMessage(playerid, COLOR_LIGHTGREEN, "Dar hale hazer yek modir hozur darad, dar surate boruze hargune moshkel az /report estefade konid.");
