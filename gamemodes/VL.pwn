public OnVehicleDeath(vehicleid, killerid)
{
	if(IsValidObject(neon1[vehicleid])) DestroyObject(neon1[vehicleid]);
	if(IsValidObject(neon2[vehicleid])) DestroyObject(neon2[vehicleid]);

	SetVehicleVirtualWorld(vehicleid, 0);
	foreach(new i : Player)
	{
		if(RentVehicle[i] == vehicleid) {
			DestroyVehicleEx(RentVehicle[i], 8390);
			RentTimer[i] = 0;
			RentKM[i] = 0;
			RentVehicle[i] = INVALID_VEHICLE_ID;
		}
	}

	if(IsAnOwnableCar(vehicleid))
 	{
		foreach(new i : Player)
		{
			for(new v; v < MAX_PERSONAL_VEHICLES; v++) {
				if(PlayerInfo[i][pCarID][v] == vehicleid) {
					PlayerInfo[i][pCarHP][v] = 999.0;
					if(PlayerInfo[i][pCarIns][v] != 0)
					{
						PlayerInfo[i][pCarIns][v]--;
					}
					cUpdate(i, v, cInsurancecarx);
					cUpdate(i, v, cKMx);
					cUpdate(i, v, cHPx);
				}
			}
		}
 	}
	StopAudioStreamForPlayersInCar(vehicleid);

	if(IsAPizzaCar(vehicleid))
	{
		for(new i = 0; i < 5; i++)
		{
			if(IsValidObject(PizzaObject[vehicleid][i]))
			{
				DestroyObject(PizzaObject[vehicleid][i]);
				PizzaObject[vehicleid][i] = CreateObject(1582, 0.0, 0.0, -1000.0, 0.0, 0.0, 0.0);
			}
		}
		PizzaON[vehicleid] = 0;
	}
	new Float:Pos[4];
	GetVehiclePos(vehicleid, Pos[0], Pos[1], Pos[2]);
	if(Pos[2] <= 0) SetVehicleToRespawn(vehicleid);
	if(DMVCar[vehicleid] == 1)
	{
		foreach(new i : Player) if(DMVVeh[i]) DMVVeh[i] = 0;
		DestroyVehicleEx(vehicleid, 8435);
		DMVCar[vehicleid] = 0;
	}
	for(new i = 0; i < sizeof(VehToDestroy); i++)
	{
		if(VehToDestroy[i] == vehicleid)
		{
			DestroyVehicleEx(VehToDestroy[i], 8442);
			VehCreator[VehToDestroy[i]] = INVALID_PLAYER_ID;
			VehToDestroy[i] = INVALID_VEHICLE_ID;
		}
	}
	RadioCH[vehicleid] = 0;
	return 1;
}

