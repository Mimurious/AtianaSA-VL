stock ShowPlayerDamages(playerid, toid)
{
    new
        str[1000],
        str1[500],
        count = 0,
        name[MAX_PLAYER_NAME];

    GetPlayerName(playerid, name, sizeof(name));
    for(new id = 0; id < MAX_DAMAGES; id++)
    {
        if(DamageInfo[playerid][id][dmgDamage] != 0) count++;
    }
    if(count == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: There is no damage to display.");
    else if(count > 0)
    {
        for(new id = 0; id < MAX_DAMAGES; id++)
        {
            if(DamageInfo[playerid][id][dmgDamage] != 0)
            {
                format(str1, sizeof str1, "%d dmg from %s to %s (Kevlarhit: %d) %ds ago\n", DamageInfo[playerid][id][dmgDamage], GetWeaponNameEx(DamageInfo[playerid][id][dmgWeapon]), GetBodypartName(DamageInfo[playerid][id][dmgBodypart]), DamageInfo[playerid][id][dmgKevlarhit], gettime() - DamageInfo[playerid][id][dmgSeconds]);
                strcat(str, str1);
            }
            else if(DamageInfo[playerid][id][dmgDamage] != 0 && id >= MAX_DAMAGES)
            {
                strcat(str, "You seems taking too many damage....");
            }
        }
        Dialog_Show(toid, DAMAGE, DIALOG_STYLE_LIST, name, str, "Close", "");
    }
    return 1;
}

