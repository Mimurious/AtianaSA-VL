		ProxDetector(30.0, playerid, gString, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
	}
	return 1;
}

cmd:wanted(playerid, params[])
{
	if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "{999999}You are not a Cop.");
	if(OnDuty[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not on duty.");
	new var[1024];
	foreach(new i : Player)
	{
		if(PlayerInfo[i][pWanted] > 0)
		{
			format(gString,sizeof(gString)," {FFAF00}Suspect {F3FF02}%s(%d) {FFAF00}Wanted: {F81414}%d\n",GetName(i),i,PlayerInfo[i][pWanted]);
			strcat(var, gString);
		}
	}
    Dialog_Show(playerid, EMPTY, DIALOG_STYLE_LIST, "Wanted suspects:", var, "Close", "");
	return 1;
}

cmd:nearwanted(playerid, params[])
{
	if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "{999999}You are not a Cop.");
	if(OnDuty[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not on duty.");
	new var[1024];
	foreach(new i : Player)
	{
		if(PlayerInfo[i][pWanted] > 0 && ProxDetectorS(300.0, playerid, i))
		{
			format(gString,sizeof(gString)," {FFAF00}Suspect {F3FF02}%s(%d) {FFAF00}Wanted: {F81414}%d\n",GetName(i),i,PlayerInfo[i][pWanted]);
			strcat(var, gString);
		}
	}
    Dialog_Show(playerid, EMPTY, DIALOG_STYLE_LIST, "Wanted suspects (Near you, in distance of 300m):", var, "Close", "");
	return 1;
}

cmd:cuff(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "{999999}You are not a Cop.");
	if(OnDuty[playerid] == 0)return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not on duty.");
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/cuff <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[giveplayerid][pWanted] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: This player don't have wanted.");
		if(!ProxDetectorS(3.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(Cuffed[giveplayerid] == 1) return SendClientMessage(playerid, COLOR_ERROR, "Player is already cuffed.");

		format(gString, sizeof(gString), "* %s has been cuffed by %s, so he won't go anywhere.", GetName(giveplayerid), GetName(playerid));
		ProxDetector(30.0, playerid, gString, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);

		format(gString, sizeof(gString), "You were Cuffed by %s.", GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "You Cuffed %s.", GetName(giveplayerid));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		
		format(gString, sizeof gString, "/department I have cuffed suspect %s", GetName(giveplayerid));
		PC_EmulateCommand(playerid, gString);
		if(PlayerInfo[playerid][pLanguage] == 0)GameTextForPlayer(giveplayerid, "~r~Cuffed", 2500, 3);
		if(PlayerInfo[playerid][pLanguage] == 1)GameTextForPlayer(giveplayerid, "~r~Dastgir shodid", 2500, 3);

		TogglePlayerControllableEx(giveplayerid, 0);
		SetPlayerAttachedObject(giveplayerid,0,19418,5,-0.046999,0.042000,0.003000,176.799697,158.099868,-48.799999,1.000000,1.000000,1.000000);
		SetPlayerSpecialAction(giveplayerid, SPECIAL_ACTION_CUFFED );

		Cuffed[giveplayerid] = 1;
	}
	return 1;
}

cmd:uncuff(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "{999999}You are not a Cop.");
	if(OnDuty[playerid] == 0)return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not on duty.");
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/uncuff <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(!ProxDetectorS(3.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(Cuffed[giveplayerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Player is not cuffed.");

		format(gString, sizeof(gString), "You were Uncuffed by %s.", GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "You Uncuffed %s.", GetName(giveplayerid));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		if(PlayerInfo[playerid][pLanguage] == 0)GameTextForPlayer(giveplayerid, "~g~Uncuffed", 2500, 3);
		if(PlayerInfo[playerid][pLanguage] == 1)GameTextForPlayer(giveplayerid, "~r~Azad shodid", 2500, 3);
		TogglePlayerControllableEx(giveplayerid, 1);

		for(new i;i<10;i++) RemovePlayerAttachedObject(giveplayerid,i);
		SetPlayerSpecialAction(giveplayerid,SPECIAL_ACTION_NONE);

		Cuffed[giveplayerid] = 0;
	}
	return 1;
}

cmd:clear(playerid, params[])
{
	new giveplayerid, splayer[25];
	if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "{999999}You are not a Cop.");
	if(OnDuty[playerid] == 0)return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not on duty.");
	if(sscanf(params,"s[25]", splayer)) return SendSyntaxMessage(playerid, "/clear <Player ID/Name>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(DebugMode == false) if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot clear yourself.");
		if(PlayerInfo[playerid][pRank] < 3) return NotEnoughPermission(playerid);
		if(!IsACopCar(GetPlayerVehicleID(playerid))) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in a Police Vehicle.");

		format(gString, sizeof(gString), "You cleared the Records and Wanted Points of %s.", GetName(giveplayerid));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Officer %s has cleared your Records and Wanted Points.", GetName(playerid));
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Officer %s has cleared %s Records and Wanted Points.", GetName(playerid), GetName(giveplayerid));
		SendFactionMessage(1, COLOR_RADIO, gString);

		SetPlayerWantedLevel(giveplayerid, 0);
		PlayerInfo[giveplayerid][pWanted] = 0;
		Update(giveplayerid,pWantedx);
		ClearCrimes(giveplayerid);

		PlayerInfo[giveplayerid][pWantedLost] = 900;
		Update(giveplayerid,pWantedLostx);

		PlayerTextDrawHide(giveplayerid, WantedTime[giveplayerid]);
	}
	return 1;
}

