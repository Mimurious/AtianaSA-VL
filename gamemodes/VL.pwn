			SendPerMessage(i, COLOR_CC, gString);
		}

		format(gString,sizeof(gString),"Your score is %d and you have %d kills and %d deaths.",(KillCW[i]-DeathCW[i]),KillCW[i],DeathCW[i]);
		SendEngMessage(i, COLOR_CC, gString);
		format(gString,sizeof(gString),"Score shoma %d bood va shoma %d kills va %d deaths dashtid.",(KillCW[i]-DeathCW[i]),KillCW[i],DeathCW[i]);
		SendPerMessage(i, COLOR_CC, gString);

		new clan = PlayerInfo[i][pCLeader] + PlayerInfo[i][pCMember];
		if(ClanWarS1 > ClanWarS2 && clan == ClanWarT1)
		{
			GivePlayerCash(i, 500);
			PlayerInfo[i][pDrugs] += 2;
			Update(i, pDrugsx);
			PlayerInfo[i][pMats] += 200;
			Update(i, pMatsx);
		}
		if(ClanWarS1 < ClanWarS2 && clan == ClanWarT2)
		{
			GivePlayerCash(i, 500);
			PlayerInfo[i][pDrugs] += 2;
			Update(i, pDrugsx);
			PlayerInfo[i][pMats] += 200;
			Update(i, pMatsx);
