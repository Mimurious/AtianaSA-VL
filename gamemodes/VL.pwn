CMD:tradeveh(playerid, params[])
{
	new id,price,idcar1=-1,idcar2=-1, splayer[MAX_PLAYER_NAME], giveplayerid;
	if(PlayerInfo[playerid][pPinLogged] == 0 && strlen(PlayerInfo[playerid][pPin]) != 1) return SendEngMessage(playerid, COLOR_YELLOW2, PINEN), SendPerMessage(playerid, COLOR_YELLOW2, PINFA);
	if(sscanf(params,"s[25]i", splayer, price)) return SendSyntaxMessage(playerid, "/tradeveh <Player ID/Name> <Difference ($)>");
	if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
	else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
	else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
	else giveplayerid = GetPlayers(splayer);
	if(giveplayerid == playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You can not trade a vehicle with yourself.");
	new sendername[30],giveplayer[30];
	if(MoneyACheck(playerid, price, 1, 50000000)) return 1;
    new vid = GetPlayerVehicleID(playerid),vid2 = GetPlayerVehicleID(id);
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in your vehicle.");
	if(!IsPlayerInAnyVehicle(id)) return SendClientMessage(playerid, COLOR_ERROR, "Error: Player is not in their personal vehicle.");
	for(new vv; vv < PlayerInfo[playerid][pCarSlots]; vv++)
	{
		if(vid == PlayerInfo[playerid][pCarID][vv])
		{
			idcar1 = vv;
		}
	}
	if(idcar1 == -1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in your vehicle.");
    for(new vv; vv < PlayerInfo[id][pCarSlots]; vv++)
	{
		if(vid2 == PlayerInfo[id][pCarID][vv])
		{
			idcar2 = vv;
		}
	}
	if(idcar2 == -1) return SendClientMessage(playerid, COLOR_ERROR, "Error: Player is not in their personal vehicle.");
	if(GetPlayerCash(playerid) < price) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money.");
	if(PlayerInfo[playerid][pLevel] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have level 3.");
	if(PlayerInfo[id][pLevel] < 5) return SendClientMessage(playerid, COLOR_ERROR, "Error: That player doesn't have level 3.");
    GetPlayerName(playerid, sendername, sizeof(sendername));
	GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
    if(IsPlayerInRangeOfPlayer(playerid, giveplayerid, 5.0))
    {
		new string[500];
		tradecars[id] = PlayerInfo[playerid][pCarID][idcar1];
		tradecars2[id] = PlayerInfo[id][pCarID][idcar2];
		tradetype[id] = PlayerInfo[playerid][pCar][idcar1];
		tradetype2[id] = PlayerInfo[id][pCar][idcar2];
		tradeofferer[id] = playerid;
		tradediff[id] = price;
        format(string, sizeof(string), "{B8FFDB}* You are giving your (%s) to %s in exchange for their (%s) [odometer: %.0f, colors: %d, %d] + $%s difference.",VehicleNames[PlayerInfo[playerid][pCarModel][idcar1] - 400], giveplayer, VehicleNames[PlayerInfo[id][pCarModel][idcar2] - 400],PlayerInfo[id][pCarKM][idcar2],PlayerInfo[id][pCarColor1][idcar2],PlayerInfo[id][pCarColor2][idcar2],FormatNumber(price));
		SendClientMessage(playerid, -1, string);
		format(string, sizeof(string), "{B8FFDB}* %s offers you his (%s) [odometer: %.0f, colors: %d, %d] + $%s in exchange for your (%s).",sendername,VehicleNames[PlayerInfo[playerid][pCarModel][idcar1] - 400],PlayerInfo[playerid][pCarKM][idcar1],PlayerInfo[playerid][pCarColor1][idcar1],PlayerInfo[playerid][pCarColor2][idcar1],FormatNumber(price),VehicleNames[PlayerInfo[id][pCarModel][idcar2] - 400]);
		SendClientMessage(id, -1, string);
		format(string, sizeof(string), "{B8FFDB}* Type (/acc)ept tradeveh %d to accept this offer",playerid);
		SendClientMessage(id, -1, string);
	}
	else return SendClientMessage(playerid, COLOR_ERROR, "Error: Player is not near you.");
	return 1;
}

