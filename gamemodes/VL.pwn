					{
						SetPlayerRaceCheckpointEx(playerid, 0, DMVCheck[DCheck[playerid]][0], DMVCheck[DCheck[playerid]][1], DMVCheck[DCheck[playerid]][2], DMVCheck[DCheck[playerid]+1][0], DMVCheck[DCheck[playerid]+1][1], DMVCheck[DCheck[playerid]+1][2], 3.0);
					}
					else
					{
						SetPlayerRaceCheckpointEx(playerid, 1, DMVCheck[DCheck[playerid]][0], DMVCheck[DCheck[playerid]][1], DMVCheck[DCheck[playerid]][2], DMVCheck[DCheck[playerid]][0], DMVCheck[DCheck[playerid]][1], DMVCheck[DCheck[playerid]][2], 3.0);
					}
