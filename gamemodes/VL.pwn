				GivePlayerCash(playerid, -objectInfo[id][objMoney]);
				Update(playerid, pCashx);
				format(gString, sizeof(gString), "You have bought accessory %d for %s$.", objectInfo[id][objModel], FormatNumber(objectInfo[id][objMoney]));
				SendClientMessage(playerid, COLOR_YELLOW, gString);
			}
		}
		case 1: {
			if(GetPlayerCash(playerid) < objectInfo[id][objMoney]) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money!");
			GivePlayerCash(playerid, -objectInfo[id][objMoney]);
			Update(playerid, pCashx);
			format(gString, sizeof(gString), "You have bought accessory %d for %s$.", objectInfo[id][objModel], FormatNumber(objectInfo[id][objMoney]));
