function Find(playerid, findid)
{
	if(UseFind[playerid] == 1)
	{
		new Float:X,Float:Y,Float:Z;

		FindPoints[playerid] --;
		format(gString, sizeof(gString), "%d", FindPoints[playerid]);
		GameTextForPlayer(playerid, gString, 1000, 6);
		DisablePlayerCheckpoint(playerid);

		if(FindPoints[playerid] <= 0)
		{
			CheckPoint[playerid] = 0;
			CCP[playerid] = false;
			KillTimer(FindTimer[playerid]);
			UseFind[playerid] = 0;
			Finding[playerid] = INVALID_PLAYER_ID;
			format(gString,sizeof(gString),"Marker unset - time exceeded.", GetName(findid));
			SendClientMessage(playerid,COLOR_SYN2,gString);
			return 1;
		}
		if(BizzEntered[findid] != 0)
		{
			new i = BizzEntered[findid];
			SetPlayerCheckpointEx(playerid,BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ], 3.0);
		}
		else if(HouseEntered[findid] != 0)
		{
			new i = HouseEntered[findid];
			SetPlayerCheckpointEx(playerid,HouseInfo[i][hEntranceX], HouseInfo[i][hEntranceY], HouseInfo[i][hEntranceZ], 3.0);
		}
		else if(IsInHQ(findid))
		{
			GetHQ(findid, X,Y,Z);
			SetPlayerCheckpointEx(playerid,X,Y,Z, 3.0);
		}
		else
		{
			GetPlayerPos(findid, X,Y,Z);
			SetPlayerCheckpointEx(playerid,X,Y,Z, 3.0);
		}
		CheckPoint[playerid] = 1995;
	}
	else
	{
		CheckPoint[playerid] = 0;
		CCP[playerid] = false;
		DisablePlayerCheckpoint(playerid);
		KillTimer(FindTimer[playerid]);
		return 1;
	}
	return 1;
}

function GameModeInitExitFunc() {
	foreach(new i : Player)
	{
		if(IsPlayerConnected(i) && gLogged[i] == 1)
		{
			DisablePlayerCheckpoint(i);
			CCP[i] = false;
		    SetPlayerCameraPos(i, 1460.0, -1324.0, 287.2);
		    SetPlayerCameraLookAt(i,1374.5, -1291.1, 239.0);
			gLogged[i] = 0;
		}
	}
	SetTimer("GameModeExitFunc", 4000, 0);
	return 1;
}

function GameModeExitFunc() {
	GameModeExit();
	return 1;
}

