		format(gString, sizeof(gString), "Shoma niaz darid %d mahmuleye digar peyda konid.", remainpickups);
		SendPerMessage(playerid, COLOR_SYN2, gString);
	}
	else
	{
	    format(gString, sizeof(gString), "%s finished 'Back in business' mission. Reward: $30,000, 2 levels and 3 premium points.", GetName(playerid));
		SendEngMessageToAll(COLOR_YELLOW2, gString);
		format(gString, sizeof(gString), "%s ma'moriyate 'Back in business' ra tamam kard. Javayeze daryafti: $30,000, 2 sath va 3 emtiyaze vizhe.", GetName(playerid));
