function ValidateRequest(playerid, response_code, data[])
{
	if(PlayerInfo[playerid][pRegistred] == 1) return 0;
    if(response_code == 200)
    {
        if(PlayerInfo[playerid][pLanguage] == 1) {
			SendClientMessage(playerid, -1, "SERVER: Code ta'idiye hesabe karbariye shoma ersal shod. be mahze daryafte aan, code daryafti ra vared konid.");
        	Dialog_Show(playerid, VERIFY, DIALOG_STYLE_INPUT, "{FFFF66}SERVER: {FFFFFF}Ta'ide hesabe karbari", "{FFFFFF}Code daryafti ra vared karde va ruye \"Sabt\" click konid", "Sabt", "Bastan");
		}
		else {
			SendClientMessage(playerid, -1, "SERVER: Verification code sent, please enter the received code as soon as you get it.");
        	Dialog_Show(playerid, VERIFY, DIALOG_STYLE_INPUT, "{FFFF66}SERVER: {FFFFFF}Verification", "{FFFFFF}Enter the received code to your phone then hit \"Submit\"", "Submit", "Close");
		}
	}
    else if(response_code == 403)
    {
        if(PlayerInfo[playerid][pLanguage] == 1) {
    		SendClientMessage(playerid, COLOR_ERROR, "KHATA: In shomareye telephone dar hale estefade tavasote hesabhaye karbarie ziyadi ast, mojadadan talash konid...");
     		Dialog_Show(playerid, VERIFYX, DIALOG_STYLE_INPUT, "{FFFF66}SERVER: {FFFFFF}Ta'ide hesabe karbari", "{FFFFFF}Shomareye telephone khod ra {FF0000}(bedune 0e ebtedayi){FFFFFF} jahate daryafte code ta'idiye vared karde va ruye \"Sabt\" click konid.\n(Mesal: 9121234567)", "Sabt", "Bastan");
		}
		else {
		    SendClientMessage(playerid, COLOR_ERROR, "ERROR: This phone number is used by too many other accounts, try again...");
			Dialog_Show(playerid, VERIFYX, DIALOG_STYLE_INPUT, "{FFFF66}SERVER: {FFFFFF}Verification", "{FFFFFF}Enter your phone number {FF0000}(without 0 at the first){FFFFFF} to get the verification code then hit \"Submit\"\n(e.g.: 9121234567)", "Submit", "Close");
		}
	}
    else
    {
        format(gString, sizeof gString, "sa.atiana.ir/verify.php?phone=%s&code=%d", sPhone[playerid], sCode[playerid]);
		HTTP(playerid, HTTP_GET, gString, "", "ValidateRequest");
        if(PlayerInfo[playerid][pLanguage] == 0) format(gString, sizeof(gString), "ERROR (%d), trying to re-execute the verification program...", response_code);
        else format(gString, sizeof(gString), "KHATA (%d), talash baraye ejraye mojadade barnameye ta'ide hesabe karbari...", response_code);
        SendClientMessage(playerid, COLOR_LIGHTRED, gString);
    }
    return 1;
}

