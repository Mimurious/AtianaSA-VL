function OnPlayerLoginIn(playerid, password[])
{
	if(IsPlayerConnected(playerid))
	{
		gQuery[0] = (EOS);
		mysql_format(Database, gQuery, sizeof(gQuery), "SELECT * FROM `users` WHERE `Name`='%e' AND `Password`='%e'", GetName(playerid), password);
		mysql_tquery(Database, gQuery, "WhenPlayerLogin", "i", playerid);
	}
	return 1;
}

