cmd:giverank(playerid, params[])
{
	new giveplayerid, splayer[25], rank;
	if(PlayerInfo[playerid][pLeader] == 0) return NotEnoughPermission(playerid);
	if(PlayerInfo[playerid][pAdmin] == 6) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[25]i", splayer, rank)) return SendSyntaxMessage(playerid, "/giverank <Player ID/Name> <Rank (1-6)>");
	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[giveplayerid][pLeader] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot change rank of a leader.");
		if(PlayerInfo[giveplayerid][pMember] != PlayerInfo[playerid][pLeader]) return SendClientMessage(playerid, COLOR_ERROR, "Player is not in your faction.");
		if(rank < 1 || rank > 6) return SendClientMessage(playerid, COLOR_ERROR, "Rank must be between 1 and 6.");
		if(PlayerInfo[playerid][pLeader] == 3 && (rank < 1 || rank > 2)) return SendClientMessage(playerid, COLOR_ERROR, "Rank must be 1 or 2.");

		if(rank >= PlayerInfo[giveplayerid][pRank])
		{
			format(gString, sizeof(gString), "You promoted %s to rank %d.",GetName(giveplayerid), rank);
			SendClientMessage(playerid, COLOR_SYN3, gString);
			format(gString, sizeof(gString), "Leader %s promoted you to rank %d.",GetName(playerid), rank);
			SendClientMessage(giveplayerid, COLOR_SYN3, gString);
		}
		else
		{
			format(gString, sizeof(gString), "You demoted %s to rank %d.",GetName(giveplayerid), rank);
			SendClientMessage(playerid, COLOR_SYN3, gString);
			format(gString, sizeof(gString), "Leader %s demoted you to rank %d.",GetName(playerid), rank);
			SendClientMessage(giveplayerid, COLOR_SYN3, gString);
		}

		new str[24];
		format(str, sizeof(str), "Promoted to rank %d", rank);
		FactionLog(PlayerInfo[playerid][pLeader], GetDatabaseID(playerid), GetDatabaseID(giveplayerid), str);

		PlayerInfo[giveplayerid][pRank] = rank;
		Update(giveplayerid,pRankx);
	}
	return 1;
}

