cmd:makeadminoff(playerid, params[])
{
	new giveplayer[24], lvl;
	if(PlayerInfo[playerid][pAdmin] < 7) return NotEnoughPermission(playerid);
	if(sscanf(params,"s[24]i", giveplayer, lvl)) return SendSyntaxMessage(playerid, "/makeadminoff <Player Name> <Admin Level (1-5)>");
	if(!ExistName(giveplayer)) return SendClientMessage(playerid, COLOR_ERROR, "Player doesn't exist.");
	if(lvl > 5) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot use this command for high levels! Contact Server Manager.");
	if(strmatch(giveplayer, "Mohammad")) return SendClientMessage(playerid, COLOR_ERROR, "Why should I listen to you when you want to use this command on owner?");
	if(GetPlayerID(giveplayer) != INVALID_PLAYER_ID) return SendClientMessage(playerid, COLOR_ERROR, "Player is connected, use /makeadmin instead.");
	gQuery[0] = (EOS);
	mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `AdminLevel`='%d' WHERE `Name` = '%s'", lvl, giveplayer);
	mysql_query(Database, gQuery);

	format(gString, sizeof(gString), "%s has offline promoted %s to a level %d admin.", GetName(playerid), giveplayer, lvl >= 6 ? 6 : lvl);
	CMDRaport(gString, 1, 0);
	format(gString, sizeof(gString), "You have offline promoted %s to a level %d admin.", giveplayer, lvl);
	SendClientMessage(playerid, COLOR_SYN3, gString);

	return 1;
}

