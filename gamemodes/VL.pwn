Dialog:BUY_ACCS(playerid, response, listitem, inputtext[])
{
	if(!response) return 1;
	new id = ObjectSlot[playerid][ObjectID[playerid]];
	//if(attachInfo[playerid][aModel][ObjectType[playerid]] != 0) SendClientMessage(playerid, COLOR_GREY, "The previous accesory lost.");
	switch(listitem) {
		case 0: {
			if(objectInfo[id][objPremium] != 0) {
				if(PlayerInfo[playerid][pPremiumPoints] < objectInfo[id][objPremium]) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough premium points!");
				PlayerInfo[playerid][pPremiumPoints] -= objectInfo[id][objPremium];
