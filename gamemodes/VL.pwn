		    if(PlayerInfo[playerid][pMission2] != 0) return SendEngMessage(playerid, COLOR_ERROR, "Error: This mission is unavailable now."), SendPerMessage(playerid, COLOR_ERROR, "Khata: In ma'moriyat hamaknoon dar dastras nist.");
