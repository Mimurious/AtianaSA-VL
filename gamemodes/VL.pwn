cmd:resetturfs(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 5) return NotEnoughPermission(playerid);
	if(PlayerInfo[playerid][pAdmin] == 6) return NotEnoughPermission(playerid);
	new dif = AllZones / 4;
	for(new zone = 1; zone <= AllZones + 1; zone++)
	{
			if(zone <= dif) ZoneInfo[zone][zTeam] = 8;
			else if(zone <= dif * 2) ZoneInfo[zone][zTeam] = 9;
			else if(zone <= dif * 3) ZoneInfo[zone][zTeam] = 10;
			else if(zone <= dif * 4) ZoneInfo[zone][zTeam] = 11;
			zUpdate(zone,zTeamx);

			GetZoneName((ZoneInfo[zone][zMaxX] / 2) + (ZoneInfo[zone][zMinX] / 2) , (ZoneInfo[zone][zMaxY] / 2) + (ZoneInfo[zone][zMinY] / 2), ZoneInfo[zone][zName], 256);
			zUpdate(zone, zNamex);

			ZoneInfo[zone][zHours] = 0;
			zUpdate(zone, zHoursx);

			HideZoneForAll(Zone[zone]);
			ShowZoneForAllEx(Zone[zone], GetZoneColor(ZoneInfo[zone][zTeam]));

			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
	}
	format(gString, sizeof(gString), "%s has reseted all of server turfs.",GetName(playerid));
	CMDRaport(gString, 1, 0);
	return 1;
}

