public OnPlayerEnterRaceCheckpoint(playerid)
{
	RCP[playerid] = false;
    PlayerPlaySound(playerid, 1056, 0.0, 0.0, 0.0);
	if(RaceCheckPoint[playerid] && InRace[playerid] && IsPlayerInVehicle(playerid, RaceVehicle[playerid]))
	{
		if(RaceCheckPoint[playerid] < sizeof (RaceCheckpoints) - 2)
			SetPlayerRaceCheckpointEx(playerid, 0, RaceCheckpoints[RaceCheckPoint[playerid] - 1][0], RaceCheckpoints[RaceCheckPoint[playerid] - 1][1], RaceCheckpoints[RaceCheckPoint[playerid] - 1][2], RaceCheckpoints[RaceCheckPoint[playerid]][0], RaceCheckpoints[RaceCheckPoint[playerid]][1], RaceCheckpoints[RaceCheckPoint[playerid]][2], 3.5);

		else if(RaceCheckPoint[playerid] == sizeof (RaceCheckpoints) - 2)
			SetPlayerRaceCheckpointEx(playerid, 1, RaceCheckpoints[RaceCheckPoint[playerid] - 1][0], RaceCheckpoints[RaceCheckPoint[playerid] - 1][1], RaceCheckpoints[RaceCheckPoint[playerid] - 1][2], 0.0, 0.0, 0.0, 3.5);

		else if(RaceCheckPoint[playerid] == sizeof (RaceCheckpoints) - 1)
		{
			new
			prize = random(10);
			KillTimer(racetimer);

			switch(prize)
			{
				case 0:
				{
					if(PlayerInfo[playerid][pLanguage] == 0)
					{
						format(gString, sizeof (gString), "The race has been finished. %s is our winner. He won 1 experience point.", GetName(playerid));
					}
					if(PlayerInfo[playerid][pLanguage] == 1)
					{
						format(gString, sizeof (gString), "Mosabeghe tamam shod. Barande: %s. +1 Emtiyaze tajrobe.", GetName(playerid));
					}
					PlayerInfo[playerid][pRespect] ++;
					Update(playerid, pRespectx);
				}
				case 1 .. 9:
				{
					if(PlayerInfo[playerid][pLanguage] == 0)
					{
						format(gString, sizeof (gString), "The race has been finished. %s is our winner. He won $250.", GetName(playerid));
					}
					if(PlayerInfo[playerid][pLanguage] == 1)
					{
						format(gString, sizeof (gString), "Mosabeghe tamam shod. Barande: %s. +$250.", GetName(playerid));
					}
					GivePlayerCash(playerid, 250);
				}
			}

			foreach (Player, i)
			{
				if(InRace[i] == true)
				{
					SendEngMessage(i, COLOR_YELLOW, gString),SendPerMessage(i, COLOR_YELLOW, gString);
					SetPlayerPosEx(i, BizzInfo[25][bEntranceX], BizzInfo[25][bEntranceY], BizzInfo[25][bEntranceZ]);
					SetPlayerInterior(i,0);
					SetPlayerVirtualWorld(i,0);
					ResetPlayerWeapons(i);
					InRace[i] = false;
					RaceCheckPoint[i] = 0;
					RaceVehicle[i] = 0;
					TogglePlayerControllableEx(i, 1);
					DisablePlayerRaceCheckpoint(i);
				}
			}

			for(new j = 0; j < sizeof (race); j++)
			{
				SetVehicleToRespawn(race[j]);

				Fuel[race[j]] = 100;
				SetVehicleVirtualWorld(race[j], 1337);
			}

			RaceStarted = false;
			RacePlayers = 0;
			RaceCountdown = 0;
		}

		RaceCheckPoint[playerid] ++;
	}
	if(DrivingTest[playerid] == 1)
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			new carid = GetPlayerVehicleID(playerid);
			if(IsADMVCar(carid))
			{
				DisablePlayerRaceCheckpoint(playerid);
				if(DCheck[playerid] == 16)
				{
					if(DMVVeh[playerid]) DestroyVehicleEx(DMVVeh[playerid], 10140);
					RadioCH[DMVVeh[playerid]] = 0;
					DMVCar[DMVVeh[playerid]] = 0;
					DMVVeh[playerid] = 0;

					GivePlayerCash(playerid, -20);
					if(PlayerInfo[playerid][pTutorial] == 1) {
					    SendEngMessage(playerid, COLOR_SYN, "TUTORIAL: Congratulations! Now you have a driving license.");
						SendPerMessage(playerid, COLOR_SYN, "TUTORIAL: Tabrik! Shoma hamaknun mojaveze ranandegi darid.");
					}
					else {
						SendEngMessage(playerid, COLOR_SYN2, "Congratulations! Now you have a driving license.");
						SendPerMessage(playerid, COLOR_SYN2, "Tabrik! Shoma hamaknun mojaveze ranandegi darid.");
					}
					DCheck[playerid] = 0;
					DrivingTest[playerid] = 0;
					PlayerInfo[playerid][pDrivingLic] = 300;
					Update(playerid, pDrivingLicx);
					SetPlayerPosEx(playerid, -1973.1566,-1019.6376,32.1767);
					SetPlayerFacingAngle(playerid, 45);
					PlayerTextDrawHide(playerid, DMVText[playerid]);
				}
				else
				{
					DCheck[playerid] ++;
					format(gString, sizeof(gString), "CHECKPOINT: ~y~%d/16", DCheck[playerid]);
					PlayerTextDrawSetString(playerid, DMVText[playerid], gString);
