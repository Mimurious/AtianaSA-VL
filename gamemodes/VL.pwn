	    SendClientMessage(playerid, COLOR_SYN2, text);
	}
	else if(strcmp("*", symbol, true) == 0)
	{
	    rezultat = numar1 * numar2;
	    format(text, sizeof(text), "[CALCULATOR] %.2f * %.2f = %.2f", numar1, numar2, rezultat);
	    SendClientMessage(playerid, COLOR_SYN2, text);
	}
	else if(strcmp("/", symbol, true) == 0)
	{
	    rezultat = numar1 / numar2;
	    format(text, sizeof(text), "[CALCULATOR] %.2f / %.2f = %.2f", numar1, numar2, rezultat);
	    SendClientMessage(playerid, COLOR_SYN2, text);
	}
	return 1;
}

