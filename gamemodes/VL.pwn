function PayDayCheck()
{
	foreach(new i : Player)
	{
		if(gLogged[i] == 0) continue;
		if(chdet[i] > 0) chdet[i]--;


		GetPlayerPos(i,PPos[i][0],PPos[i][1],PPos[i][2]);
		if(IsPlayerInRangeOfPoint(i,3,PPos[i][0],PPos[i][1],PPos[i][2]) && !AFK[i] && PlayerInfo[i][pAdmin] != 8)
	    {
	    	AFKMins[i]++;
	    	IsAFK[i] = 1;
    	 	mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = 2 WHERE `ID` = %d", PlayerInfo[i][pSQLID]);
			if(IsAFK[i] && AFKMins[i] > 2) mysql_query(Database, gQuery);

	    	new wantedlevel = GetPlayerWantedLevel(i);
	    	if(!wantedlevel) {
		    	if(AFKMins[i] == 30)
				{
					format(gString, sizeof(gString), "> %s has been kicked by AdmBot, reason: Being idle without sleeping for more than 30 minutes.", GetName(i));
					SendEngMessageToAll(COLOR_LIGHTRED, gString);
					format(gString, sizeof(gString), "> %s tavasote AdmBot kick shod, dalil: Dur az dastras budan bedune khabidan baraye bish az 30 daghighe.", GetName(i));
					SendPerMessageToAll(COLOR_LIGHTRED, gString);
					Kick(i);
					mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = 0 WHERE `ID` = %d", PlayerInfo[i][pSQLID]);
					mysql_query(Database, gQuery);
				}
				else if(AFKMins[i] == 29)
				{
					SendEngMessage(i, COLOR_LIGHTRED, "You will be kicked in a minute due to being idle without sleeping.");
			    	SendPerMessage(i, COLOR_LIGHTRED, "Shoma ta yek daghigheye digar be dalile dur az dastras budan bedune khab kick khahid shod.");
				}
				else if(AFKMins[i] == 28)
			    {
			    	SendEngMessage(i, COLOR_LIGHTRED, "You will be kicked in 2 minutes due to being idle without sleeping.");
			    	SendPerMessage(i, COLOR_LIGHTRED, "Shoma ta 2 daghigheye digar be dalile dur az dastras budan bedune khab kick khahid shod.");
			    }
			}
			else {
			    if(AFKMins[i] == 10)
				{
					format(gString, sizeof(gString), "> %s has been kicked by AdmBot, reason: Being idle while having wanted levels for more than 10 minutes.", GetName(i));
					SendEngMessageToAll(COLOR_LIGHTRED, gString);
					format(gString, sizeof(gString), "> %s tavasote AdmBot kick shod, dalil: Dur az dastras budan hengame tahte taghib budan baraye bish az 10 daghighe.", GetName(i));
					SendPerMessageToAll(COLOR_LIGHTRED, gString);
					mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = 0 WHERE `ID` = %d", PlayerInfo[i][pSQLID]);
					mysql_query(Database, gQuery);
					Kick(i);
				}
				else if(AFKMins[i] == 9)
				{
					SendEngMessage(i, COLOR_LIGHTRED, "You will be kicked in a minute due to being idle while having wanted levels.");
			    	SendPerMessage(i, COLOR_LIGHTRED, "Shoma ta yek daghigheye digar be dalile dur az dastras budan hengame tahte taghib budan kick khahid shod.");
				}
				else if(AFKMins[i] == 8)
			    {
			    	SendEngMessage(i, COLOR_LIGHTRED, "You will be kicked in 2 minutes due to being idle while having wanted levels.");
			    	SendPerMessage(i, COLOR_LIGHTRED, "Shoma ta 2 daghigheye digar be dalile dur az dastras budan hengame tahte taghib budan kick khahid shod.");
			    }
			}
	    }
	    else
	    {
	    	AFKMins[i] = 0;
	    	IsAFK[i] = 0;
	    	mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = 1 WHERE `ID` = %d", PlayerInfo[i][pSQLID]);
			mysql_query(Database, gQuery);
	    }

		new level = PlayerInfo[i][pLevel];
		if(level >= 1 && level <= 5) { PlayerInfo[i][pPayCheck] += 1; }
		else if(level >= 6 && level <= 10) { PlayerInfo[i][pPayCheck] += 2; }
		else if(level >= 11 && level <= 15) { PlayerInfo[i][pPayCheck] += 3; }
		else if(level >= 16 && level <= 20) { PlayerInfo[i][pPayCheck] += 4; }
		else if(level >= 21 && level <= 25) { PlayerInfo[i][pPayCheck] += 5; }
		else if(level >= 26 && level <= 30) { PlayerInfo[i][pPayCheck] += 6; }
		else if(level >= 31 && level <= 40) { PlayerInfo[i][pPayCheck] += 7; }
		else if(level >= 41 && level <= 50) { PlayerInfo[i][pPayCheck] += 8; }
		else if(level >= 51 && level <= 60) { PlayerInfo[i][pPayCheck] += 9; }
		else if(level >= 61 && level <= 70) { PlayerInfo[i][pPayCheck] += 10; }
		else if(level >= 71 && level <= 80) { PlayerInfo[i][pPayCheck] += 11; }
		else if(level >= 81 && level <= 90) { PlayerInfo[i][pPayCheck] += 12; }
		else if(level >= 91 && level <= 100) { PlayerInfo[i][pPayCheck] += 13; }
		else if(level >= 101) { PlayerInfo[i][pPayCheck] += 14; }
		Update(i,pPayCheckx);

		PlayerInfo[i][pPayDay] += 1;
		Update(i, pPayDayx);

		if(PlayerInfo[i][pContractTime] > 0)
		{
			PlayerInfo[i][pContractTime] --;
			Update(i, pContractTimex);
		}
		if(PlayerInfo[i][pMoneyBoost] > 0)
		{
			PlayerInfo[i][pMoneyBoost] --;
			Update(i, pMoneyBoostx);
		}

		new Float: health;
		GetPlayerHealthEx(i, health);
		if(AFK[i] == 0) SetPlayerHealthEx(i, health - 1);
	}
	return 1;
}

function DateDiff(y1,m1,d1,y2,m2,d2)
{
	new ok = 1;
	new countd = 0;
	while(ok)
	{
		new nyear, nmonth, nday;
		CalcDate2(y1, m1, d1, nyear, nmonth, nday, countd);
		if(nyear == y2 && nmonth == m2 && nday == d2) ok = 0;
		countd ++;
	}
	return countd-1;
}

