cmd:levelup(playerid, params[])
{
	new lvlrespect, lvlcost;

	lvlcost = PlayerInfo[playerid][pLevel] * levelcost;
	if(PlayerInfo[playerid][pLevel] == 1) lvlrespect = 2;
	else lvlrespect = PlayerInfo[playerid][pLevel] * levelrespect - 2;

	if(GetPlayerCash(playerid) < lvlcost) {
		format(gString, sizeof gString, "Error: You need $%s more to do this action.", FormatNumber(lvlcost - GetPlayerCash(playerid)));
		SendEngMessage(playerid, COLOR_ERROR, gString);

		format(gString, sizeof gString, "Khata: Baraye anjame in a'mal shoma niyaz be $%s poole bishtar darid.", FormatNumber(lvlcost - GetPlayerCash(playerid)));
		return SendPerMessage(playerid, COLOR_ERROR, gString);
	}
	if(PlayerInfo[playerid][pRespect] < lvlrespect) {
		format(gString, sizeof gString, "Error: You need %s more experience points to do this action.", FormatNumber(lvlrespect - PlayerInfo[playerid][pRespect]));
		SendEngMessage(playerid, COLOR_ERROR, gString);
		format(gString, sizeof gString, "Khata: Baraye anjame in a'mal shoma niyaz be %s emtiyaze tajrobeye bishtar darid.", FormatNumber(lvlrespect - PlayerInfo[playerid][pRespect]));
		return SendPerMessage(playerid, COLOR_ERROR, gString);
 	}
	new confirm[64];
	sscanf(params,"s[64]", confirm);
	if(strmatch(confirm,"confirm"))
	{
	    PlayerInfo[playerid][pRespect] = 0;
		Update(playerid,pRespectx);

		PlayerInfo[playerid][pLevel]++;
		Update(playerid,pLevelx);

		format(gString, sizeof(gString), "Congratulations, you are now level {FF0000}%d{FFFFFF}.", PlayerInfo[playerid][pLevel]);
		SendEngMessage(playerid, -1, gString);

		format(gString, sizeof(gString), "Tabrik, shoma hamaknun sathe {FF0000}%d{FFFFFF} hastid.", PlayerInfo[playerid][pLevel]);
		SendPerMessage(playerid, -1, gString);

		if(PlayerInfo[playerid][pReferral] > 0)
		{
			new bonusr =  lvlrespect * 25/100;
			new bonusm = lvlcost * 2;

			gQuery[0] = (EOS);
			mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `TReferralRespect` = `TReferralRespect` + %d, `TReferralMoney` = `TReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
			mysql_query(Database, gQuery);

			gQuery[0] = (EOS);
			mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `ReferralRespect` = `ReferralRespect` + %d, `ReferralMoney` = `ReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
			mysql_query(Database, gQuery);
		}

		if(PlayerInfo[playerid][pLanguage] == 0)format(gString, sizeof(gString), "~g~Level Up~n~~w~Current level: %d", PlayerInfo[playerid][pLevel]);
		if(PlayerInfo[playerid][pLanguage] == 1)format(gString, sizeof(gString), "~g~Afzayeshe Sath~n~~w~Sathe konuni: %d", PlayerInfo[playerid][pLevel]);
		GameTextForPlayer(playerid, gString, 5000, 1);
		GivePlayerCash(playerid, -lvlcost);
	}
	else if(PlayerInfo[playerid][pPremiumAccount] > gettime() || PlayerInfo[playerid][pPremiumAccount] == -1)
	{
		PlayerInfo[playerid][pRespect] -= lvlrespect;
		Update(playerid,pRespectx);

		PlayerInfo[playerid][pLevel]++;
		Update(playerid,pLevelx);

		format(gString, sizeof(gString), "Congratulations, you are now level {FF0000}%d{FFFFFF}.", PlayerInfo[playerid][pLevel]);
		SendEngMessage(playerid, -1, gString);

		format(gString, sizeof(gString), "Tabrik, shoma hamaknun sathe {FF0000}%d{FFFFFF} hastid.", PlayerInfo[playerid][pLevel]);
		SendPerMessage(playerid, -1, gString);

		if(PlayerInfo[playerid][pReferral] > 0)
		{
			new bonusr =  lvlrespect * 25/100;
			new bonusm = lvlcost * 2;

			gQuery[0] = (EOS);
			mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `TReferralRespect` = `TReferralRespect` + %d, `TReferralMoney` = `TReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
			mysql_query(Database, gQuery);

			gQuery[0] = (EOS);
			mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `ReferralRespect` = `ReferralRespect` + %d, `ReferralMoney` = `ReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
			mysql_query(Database, gQuery);
		}

		if(PlayerInfo[playerid][pLanguage] == 0)format(gString, sizeof(gString), "~g~Level Up~n~~w~Current level: %d", PlayerInfo[playerid][pLevel]);
		if(PlayerInfo[playerid][pLanguage] == 1)format(gString, sizeof(gString), "~g~Afzayeshe Sath~n~~w~Sathe konuni: %d", PlayerInfo[playerid][pLevel]);
		GameTextForPlayer(playerid, gString, 5000, 1);
		GivePlayerCash(playerid, -lvlcost);
	}
	else if((PlayerInfo[playerid][pPremiumAccount] < gettime() && PlayerInfo[playerid][pPremiumAccount] != -1))
	{
	    if(PlayerInfo[playerid][pRespect] > lvlrespect) {
	        SendEngMessage(playerid, COLOR_SYN, "Your account is not premium and you'll lose your extra experience points.");
	        SendEngMessage(playerid, COLOR_SYN, "Use '/levelup confirm' to ignore this warning and level up.");

	        SendPerMessage(playerid, COLOR_SYN, "Hesabe karbariye shoma vizhe nist va shoma emtiyazate tajrobeye ezafiye khod ra az dast khahid dad.");
	        SendPerMessage(playerid, COLOR_SYN, "Baraye nadide gereftane in ekhtar va afzayeshe sath, az '/levelup confirm' estefade konid.");
	    }
	    else if(PlayerInfo[playerid][pRespect] == lvlrespect) {
		    PlayerInfo[playerid][pRespect] = 0;
			Update(playerid,pRespectx);

			PlayerInfo[playerid][pLevel]++;
			Update(playerid,pLevelx);

			format(gString, sizeof(gString), "Congratulations, you are now level {FF0000}%d{FFFFFF}.", PlayerInfo[playerid][pLevel]);
			SendEngMessage(playerid, -1, gString);

			format(gString, sizeof(gString), "Tabrik, shoma hamaknun sathe {FF0000}%d{FFFFFF} hastid.", PlayerInfo[playerid][pLevel]);
			SendPerMessage(playerid, -1, gString);

			if(PlayerInfo[playerid][pReferral] > 0)
			{
				new bonusr =  lvlrespect * 25/100;
				new bonusm = lvlcost * 2;

				gQuery[0] = (EOS);
				mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `TReferralRespect` = `TReferralRespect` + %d, `TReferralMoney` = `TReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
				mysql_query(Database, gQuery);

				gQuery[0] = (EOS);
				mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `ReferralRespect` = `ReferralRespect` + %d, `ReferralMoney` = `ReferralMoney` + %d WHERE `ID`=%d", bonusr, bonusm, PlayerInfo[playerid][pReferral]);
				mysql_query(Database, gQuery);
			}
			if(PlayerInfo[playerid][pLanguage] == 0)format(gString, sizeof(gString), "~g~Level Up~n~~w~Current level: %d", PlayerInfo[playerid][pLevel]);
			if(PlayerInfo[playerid][pLanguage] == 1)format(gString, sizeof(gString), "~g~Afzayeshe Sath~n~~w~Sathe konuni: %d", PlayerInfo[playerid][pLevel]);
			GameTextForPlayer(playerid, gString, 5000, 1);
			GivePlayerCash(playerid, -lvlcost);
	    }
	}
	return 1;
}

cmd:admins(playerid, params[])
{
	new admincounter = 0;
	if(PlayerInfo[playerid][pLanguage] == 0)
	{
		SendClientMessage(playerid, COLOR_LIGHTGREEN, "|____ Online Admins: ____|");
		foreach(new i : Player)
		{
			if(PlayerInfo[i][pAdmin] >= 1 && PlayerInfo[i][pAdmin] <= 5) {
				if(IsAFK[i] == 1 && AFKMins[i] > 2) format(gString, sizeof(gString), " {CECECE}(Idle [%dm])", AFKMins[i]);
				else if(IsPlayerPaused(i) && AFK[i]) format(gString, sizeof(gString), " {909090}(Alt-Tabbed and sleep)");
				else if(IsPlayerPaused(i)) format(gString, sizeof(gString), " {CECECE}(Alt-Tabbed)");
				else if(AFK[i]) format(gString, sizeof(gString), " {909090}(Sleep)");
				else if(HaveReport(i)) format(gString, sizeof(gString), " {7CFC00}(Busy)");
				else format(gString, sizeof(gString), "");
				format(gString, sizeof(gString), "%s{FFFFFF} (%d) - Admin level %d%s", GetName(i), i, PlayerInfo[i][pAdmin], gString);
				SendClientMessage(playerid, COLOR_AC, gString);
				admincounter++;
			}
		}
		if(admincounter > 1)
		{
			format(gString,sizeof(gString),"There are %d admins online at the moment, use /report if you have a problem.", admincounter);
			SendClientMessage(playerid, COLOR_LIGHTGREEN, gString);
		}
		else if(admincounter == 1) SendClientMessage(playerid, COLOR_LIGHTGREEN, "There is an admin online at the moment, use /report if you have a problem.");
		else SendClientMessage(playerid, COLOR_LIGHTGREEN, "There is no admin online at the moment, use /support if you have a problem.");
	}
	else if(PlayerInfo[playerid][pLanguage] == 1)
	{
		SendClientMessage(playerid, COLOR_LIGHTGREEN, "|____ Modirane Online: ____|");
		foreach(new i : Player)
		{
			if(PlayerInfo[i][pAdmin] >= 1 && PlayerInfo[i][pAdmin] <= 5) {
				if(IsAFK[i] == 1 && AFKMins[i] > 2) format(gString, sizeof(gString), " {CECECE}(Dur az dastras [%dm])", AFKMins[i]);
				else if(IsPlayerPaused(i) && AFK[i]) format(gString, sizeof(gString), " {909090}(Kharej az bazi va khab)");
