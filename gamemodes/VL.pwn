		LoadCar(playerid);
		PlayerInfo[playerid][pCarLocX][idcar] = x;
		PlayerInfo[playerid][pCarLocY][idcar] = y;
		PlayerInfo[playerid][pCarLocZ][idcar] = z;
		PlayerInfo[playerid][pCarAngle][idcar] = a;
