{
	new id;
	if(IsAGroveCar(vehicleid)) id = 8;
	else if(IsATitanCar(vehicleid)) id = 9;
	else if(IsAMexicanCar(vehicleid)) id = 10;
	else if(IsARifaCar(vehicleid)) id = 11;
