function cUpdate(playerid, id, type) {
	gQuery[0] = EOS;
	switch(type)
	{
		case cLocationxx:
	    {
	        mysql_format(Database,gQuery,sizeof(gQuery),"UPDATE `cars` SET `Locationx`='%f' WHERE `ID`='%d'",PlayerInfo[playerid][pCarLocX][id],PlayerInfo[playerid][pCar][id]);
			mysql_tquery(Database,gQuery,"","");
	    }
	    case cLocationyx:
	    {
	        mysql_format(Database,gQuery,sizeof(gQuery),"UPDATE `cars` SET `Locationy`='%f' WHERE `ID`='%d'",PlayerInfo[playerid][pCarLocY][id],PlayerInfo[playerid][pCar][id]);
			mysql_tquery(Database,gQuery,"","");
	    }
	    case cLocationzx:
	    {
	        mysql_format(Database,gQuery,sizeof(gQuery),"UPDATE `cars` SET `Locationz`='%f' WHERE `ID`='%d'",PlayerInfo[playerid][pCarLocZ][id],PlayerInfo[playerid][pCar][id]);
			mysql_tquery(Database,gQuery,"","");
	    }
		case cAnglex:
	    {
	        mysql_format(Database,gQuery,sizeof(gQuery),"UPDATE `cars` SET `Angle`='%f' WHERE `ID`='%d'",PlayerInfo[playerid][pCarAngle][id],PlayerInfo[playerid][pCar][id]);
			mysql_tquery(Database,gQuery,"","");
	    }
		case cLocationx2x:
	    {
	        mysql_format(Database,gQuery,sizeof(gQuery),"UPDATE `cars` SET `Locationx2`='%f' WHERE `ID`='%d'",PlayerInfo[playerid][pCarLLocX][id],PlayerInfo[playerid][pCar][id]);
			mysql_tquery(Database,gQuery,"","");
	    }
	    case cLocationy2x:
	    {
