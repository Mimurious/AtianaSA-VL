stock GetPlayerSpeedEx(playerid, bool:kmh)
{
	new Float:Vx,Float:Vy,Float:Vz,Float:rtn;
	if(IsPlayerInAnyVehicle(playerid)) GetVehicleVelocity(GetPlayerVehicleID(playerid),Vx,Vy,Vz); else GetPlayerVelocity(playerid,Vx,Vy,Vz);
	rtn = floatsqroot(floatabs(floatpower(Vx + Vy + Vz,2)));
	return kmh?floatround(rtn * 100 * 1.61):floatround(rtn * 100);
}

public OnPlayerUpdate(playerid)
{
	if(playerid != INVALID_PLAYER_ID && IsPlayerConnected(playerid)) if(Desync[playerid] && (random(8) < 5)) return 0;

	if(qool[playerid]) {
		SetPlayerHealthEx(playerid, 999999);
		if(IsPlayerInAnyVehicle(playerid))
		{
			RepairVehicle(GetPlayerVehicleID(playerid));
			SetVehicleHealth(GetPlayerVehicleID(playerid), 999.0);
		}
	}

	static
        Float: X, Float: Y, Float: Z
    ;

    if( IsPlayerInAnyVehicle( playerid ) )
    {
        GetVehicleVelocity( GetPlayerVehicleID( playerid ), X, Y, Z );
        if( X > 3.0 || Y > 3.0 || Z > 3.0 || X < -3.0 || Y < -3.0 || Z < -3.0 )
        {
            if(Desync[playerid]) return 0;
    		SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due getting out of sync or using mods/abnormal clients.");
            format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} Vehicle swinging detected on %s (%d) and got kicked.", GetName(playerid), playerid);
			ABroadCast(COLOR_LIGHTRED, gString, 1);
			Desync[playerid] = true;
			format(aString, sizeof(aString), "* %s has been kicked from the server (%s).", GetName(playerid), "Vehicle swinging");
			HBroadCast(COLOR_YELLOW, aString, 1);
			Kick(playerid);
        }
    }

	if(IsPlayerInAnyVehicle(playerid) && !Desync[playerid]) {
		ChangeVehicleColor(GetPlayerVehicleID(playerid), vcolor1[GetPlayerVehicleID(playerid)], vcolor2[GetPlayerVehicleID(playerid)]);
	    new Float: vh;
	    GetVehicleHealth(GetPlayerVehicleID(playerid), vh);
	    if(vh == 1000)
	    {
	        SetVehicleHealth(GetPlayerVehicleID(playerid), 0);
	        SetTimerEx("OnVehicleDeath", 5000, 0, "i", GetPlayerVehicleID(playerid), playerid);
	    }
	}

	new animname[24], animlib[24];
    GetAnimationName(GetPlayerAnimationIndex(playerid), animlib, sizeof(animlib), animname, sizeof(animname));
    if (!strcmp(animname, "SWIM_CRAWL", true) && !IsPlayerInAnyVehicle(playerid))
    {
        new
            Float:velocityX,
            Float:velocityY,
            Float:velocityZ,
            Float:speed;

        GetPlayerVelocity(playerid, velocityX, velocityY, velocityZ);
        speed = floatsqroot((velocityX * velocityX) + (velocityY * velocityY) + (velocityZ * velocityZ) * 100);
        if (floatround(speed, floatround_round) >= 3)
        {
            if(Desync[playerid]) return 0;
    		SendClientMessage(playerid, COLOR_LIGHTRED, "You have been kicked due getting out of sync or using mods/abnormal clients.");
            format(gString, sizeof(gString), "[SERVER-WARN]{FFFFFF} Flyhack detected on %s (%d) and got kicked.", GetName(playerid), playerid);
			ABroadCast(COLOR_LIGHTRED, gString, 1);
			Desync[playerid] = true;
			format(aString, sizeof(aString), "* %s has been kicked from the server (%s).", GetName(playerid), "Flyhack");
			HBroadCast(COLOR_YELLOW, aString, 1);
			Kick(playerid);
        }
    }

	new keyo, Float:speed = GetPlayerSpeedEx(playerid, false);
	if(keyo & KEY_FIRE && speed > 6.0 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && AntiCheatGetSpecialAction(playerid) != SPECIAL_ACTION_DUCK && IsRunning(AntiCheatGetWeapon(playerid)))
	{
		SetPlayerArmedWeapon(playerid, 0);
	}

	if(AntiCheatGetSpecialAction(playerid) == SPECIAL_ACTION_ENTER_VEHICLE && gBugPlayerData[playerid][gbug_playerenteringveh] == 0)
	{
		gBugPlayerData[playerid][gbug_pentervtick] = GetTickCount();
		GetPlayerPos(playerid, gBugPlayerData[playerid][gbug_playerpos][0], gBugPlayerData[playerid][gbug_playerpos][1], gBugPlayerData[playerid][gbug_playerpos][2]);
		gBugPlayerData[playerid][gbug_playerenteringveh] = 1;
	}

	if(gBugPlayerData[playerid][gbug_playerenteringveh] == 0) return 1;

	if(AntiCheatGetSpecialAction(playerid) != SPECIAL_ACTION_ENTER_VEHICLE && gBugPlayerData[playerid][gbug_playerenteringveh] != 3 && GetTickCount()-gBugPlayerData[playerid][gbug_pentervtick] > 321)
	{
		{
			gBugPlayerData[playerid][gbug_playerenteringveh] = 0;

		}
		return 1;
	}

	new Float:x, Float:y, Float:z;
	GetPlayerVelocity(playerid, x, y, z);

	if(gBugPlayerData[playerid][gbug_playerenteringveh] == 3 && AntiCheatGetSpecialAction(playerid) != SPECIAL_ACTION_ENTER_VEHICLE)
	{
		gBugPlayerData[playerid][gbug_playerenteringveh] = 0;

		return 1;
	}

	if(x == 0 && y == 0 && z == 0)
	{
		if(gBugPlayerData[playerid][gbug_playerenteringveh] != 3)
		{
			if(AntiCheatGetSpecialAction(playerid) == SPECIAL_ACTION_ENTER_VEHICLE && GetTickCount()-gBugPlayerData[playerid][gbug_pentervtick] > 321)
			{
				if(gBugPlayerData[playerid][gbug_playerenteringveh] == 2)
				{
					gBugPlayerData[playerid][gbug_playerenteringveh] = 3;

				}
				else
				{
					gBugPlayerData[playerid][gbug_playerenteringveh] = 2;

					gBugPlayerData[playerid][gbug_pentervtick] = GetTickCount();
				}
			}
			else if(GetTickCount()-gBugPlayerData[playerid][gbug_pentervtick] > 321)
			{
				gBugPlayerData[playerid][gbug_playerenteringveh] = 0;

			}
		}
	}
	new drunk2 = GetPlayerDrunkLevel(playerid);
	if(drunk2 < 100)
	{
		SetPlayerDrunkLevel(playerid,2000);
	}
	else
	{
		if(DLlast[playerid] != drunk2)
		{
			new fps = DLlast[playerid] - drunk2;

			if((fps > 0) && (fps < 200))
				FPS2[playerid] = fps;
			DLlast[playerid] = drunk2;
		}
	}
	return 1;
}

function PlayerToPoint(Float:radi, playerid, Float:x, Float:y, Float:z)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:oldposx, Float:oldposy, Float:oldposz;
		new Float:tempposx, Float:tempposy, Float:tempposz;
		GetPlayerPos(playerid, oldposx, oldposy, oldposz);
		tempposx = (oldposx -x);
		tempposy = (oldposy -y);
		tempposz = (oldposz -z);

		if(((tempposx < radi) && (tempposx > -radi)) && ((tempposy < radi) && (tempposy > -radi)) && ((tempposz < radi) && (tempposz > -radi)))
		{
			return 1;
		}
	}
	return 0;
}

stock ShowPlayerFriends(playerid)
{
	new
	szDialog[1024],
	szDialog2[1024],
	szQuery[256],
	szTitle[256],
	szString[256],
	x,
	onf;

	format(szQuery, sizeof(szQuery), "SELECT * FROM `friends` WHERE `AddBy` = '%d'", PlayerInfo[playerid][pSQLID]);
	new Cache: result = mysql_query(Database, szQuery);
	cache_get_row_count(q_rc);
	for(new i, j = q_rc; i != j; ++i)
	{
		cache_get_value_name(i, "friendName", szString);
		format(Selected[playerid][x], MAX_PLAYER_NAME, szString);
		new userID = GetPlayerID( szString );
		if(userID != INVALID_PLAYER_ID)
		{
		    if(AFK[userID]) {
				onf++;
				if(x == 0) format(szDialog, sizeof(szDialog), "Add Friend...\n{FFFF00}%s (%d){FFFFFF}\n", szString, userID);
				else format(szDialog, sizeof(szDialog), "{FFFF00}%s (%d){FFFFFF}\n", szString, userID);
				strcat(szDialog2, szDialog);
			}
			else {
				onf++;
				if(x == 0) format(szDialog, sizeof(szDialog), "Add Friend...\n{13E300}%s (%d){FFFFFF}\n", szString, userID);
				else format(szDialog, sizeof(szDialog), "{13E300}%s (%d){FFFFFF}\n", szString, userID);
				strcat(szDialog2, szDialog);
			}
		}
		else
		{
			if(x == 0) format(szDialog, sizeof(szDialog), "Add Friend...\n{FF0000}%s{FFFFFF}\n", szString);
			else format(szDialog, sizeof(szDialog), "{FF0000}%s{FFFFFF}\n", szString);
			strcat(szDialog2, szDialog);
		}
		x++;
	}
	if(x == 0)
	{
		format(szDialog, sizeof(szDialog), "Add Friend...");
		strcat(szDialog2, szDialog);
	}
	cache_delete(result);
	format(szTitle, sizeof(szTitle), "Online Friends: %d", onf);
	Dialog_Show(playerid, FRIENDS, DIALOG_STYLE_LIST, szTitle, szDialog2, "Select", "Back");
	return 1;
}

