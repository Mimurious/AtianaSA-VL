		if(SpectatorID[giveplayerid] == playerid) return SendEngMessage(playerid, COLOR_ERROR, "Player is not near you."), SendPerMessage(playerid, COLOR_ERROR, "In bazikon dar nazdikie shoma nist.");
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendEngMessage(playerid, COLOR_ERROR, "Player is not near you."), SendPerMessage(playerid, COLOR_ERROR, "In bazikon dar nazdikie shoma nist.");
		if(playerid == giveplayerid) return SendEngMessage(playerid, COLOR_ERROR, "Error: You cannot whisper yourself."), SendPerMessage(playerid, COLOR_ERROR, "Shoma nemitavanid be khodetan whisper dahid.");
		if(TOG[3][giveplayerid] == 0) return SendEngMessage(playerid, COLOR_ERROR, "Error: This player has toggeled his whisper messages off."), SendPerMessage(playerid, COLOR_ERROR, "Khata: In bazikon payam haye shakhsiye khod ra gheyre fa'al karde ast.");
