function StartRace()
{
	new
	str[4],
	soundid = 1056,
	p[7],
	veh;

	RaceCountdown --;

	if(RaceCountdown)
	{
		str[0] = RaceCountdown + 48;
		str[1] = EOS;
	}
	else
	{
		strcat((str[0] = EOS, str), "GO!");
		soundid = 1057;
		RaceStarted = true;
	}

	foreach(new i : Player)
	{
		if(!InRace[i])
			continue;

		veh = GetPlayerVehicleID(i);

		GameTextForPlayer(i, str, 1500, 3);
		PlayerPlaySound(i, soundid, 0.0, 0.0, 0.0);

		RaceCheckPoint[i] = 1;

		if(!RaceCountdown)
		{
			if(veh != INVALID_VEHICLE_ID)
			{
				GetVehicleParamsEx(veh, p[0], p[1], p[2], p[3], p[4], p[5], p[6]);
				SetVehicleParamsEx(veh, 1, p[1], p[2], p[3], p[4], p[5], p[6]);
			}
			TogglePlayerControllableEx(i, 1);
			SetPlayerRaceCheckpointEx(i, 0, RaceCheckpoints[RaceCheckPoint[i] - 1][0], RaceCheckpoints[RaceCheckPoint[i] - 1][1], RaceCheckpoints[RaceCheckPoint[i] - 1][2], RaceCheckpoints[RaceCheckPoint[i]][0], RaceCheckpoints[RaceCheckPoint[i]][1], RaceCheckpoints[RaceCheckPoint[i]][2], 3.5);
		}
	}

	if(RaceCountdown)
		SetTimer("StartRace", 1000, false);

	return 1;
}

function PrepareRace()
{
	if(RacePlayers >= 2)
	{
		format(gString, sizeof(gString), "NR AdmBot: Dar in lahze %d bazikon dar Racing Arena hastand.", RacePlayers);
		SendPerMessageToAll(COLOR_NEWS, gString);
		format(gString, sizeof(gString), "Khabarnegar AdmBot: There are %d racers in the Racing Arena at the moment.", RacePlayers);
		SendEngMessageToAll(COLOR_NEWS, gString);

		foreach(new i : Player)
		{
			if(!InRace[i])
				continue;

			SendEngMessage(i, COLOR_YELLOW, "Get ready, the race will start soon!"), SendPerMessage(i, COLOR_YELLOW, "Amade! Mosabeghe be zoodi shoro khahad shod!");
		}
		RaceCountdown = 4;
		SetTimer("StartRace", 1000, false);
	}
	else
	{
		foreach(Player,i)
		{
			if(!InRace[i])
				continue;
			SendEngMessage(i,COLOR_YELLOW,"The game cannot start because there are not enough players. (Minimum 2 players.)"); SendPerMessage(i,COLOR_YELLOW,"Bazikonane kafi baraye shoroe mosabeghe vojod nadarad (Hadeaghal 2 nafar)");
			SetPlayerPosEx(i, BizzInfo[25][bEntranceX], BizzInfo[25][bEntranceY], BizzInfo[25][bEntranceZ]);
			SetPlayerInterior(i,0);
			SetPlayerVirtualWorld(i,0);
			ResetPlayerWeapons(i);
			InRace[i] = false;
			RaceCheckPoint[i] = 0;
			RaceVehicle[i] = 0;
			TogglePlayerControllableEx(i, 1);
			DisablePlayerRaceCheckpoint(i);
		}
		for(new j = 0; j < sizeof (race); j++)
		{
			SetVehicleToRespawn(race[j]);

			SetVehicleVirtualWorld(race[j], 1337);
			Fuel[race[j]] = 100;
		}
		RaceStarted = false;
		RacePlayers = 0;
	}
	return 1;
}
