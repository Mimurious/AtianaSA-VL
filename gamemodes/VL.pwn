cmd:topclans(playerid, params[])
{
	SendClientMessage(playerid, COLOR_GREY, "|___________ Top 5 Clans ___________|");
	new aux, aux2, Clan[100], bool: cont = true, Wars[100];
	for(new i = 1; i <= TotalClans; i++) Wars[i] = ClanInfo[i][cWWars], Clan[i] = ClanInfo[i][cID];
	while (cont)
	{
		cont = false;
		for(new i = 1; i <= TotalClans; i++)
		{
			for(new j = i; j <= TotalClans; j++) if(Wars[i] < Wars[j]) aux = Wars[i], Wars[i] = Wars[j] , Wars[j] = aux, aux2 = Clan[i], Clan[i] = Clan[j], Clan[j] = aux2, cont = true;
		}
	}
	for(new i = 1; i <= 5; i++)
	{
		format(gString, sizeof(gString), "Clan %s: Won %d wars.",ClanInfo[Clan[i]][cName],Wars[i]);
		SendClientMessage(playerid, COLOR_GRAD2, gString);
	}
	return 1;
}

