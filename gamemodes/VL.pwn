		    if(objectInfo[id][objPremium] != 0) format(string2, sizeof(string2), "Emtiyaze vizhe\t %s\n", FormatNumber(objectInfo[id][objPremium])), strcat(string, string2);
		    if(objectInfo[id][objMoney] != 0) format(string2, sizeof(string2), "Pool\t $%s\n", FormatNumber(objectInfo[id][objMoney])), strcat(string, string2);
		    Dialog_Show(playerid, BUY_ACCS, DIALOG_STYLE_TABLIST_HEADERS, "SERVER: Lavazem", string, "Entekhab", "Laghv");
		}
	}

	if(clickedid == AccsTD[5]) show_accs_dialog(playerid);

	if(clickedid == AccsTD[6]) {
		SetPlayerFacingAngle(playerid, OldFacing[playerid]);
	}
	if(clickedid == AccsTD[7]) {
		new Float: Angle;
		GetPlayerFacingAngle(playerid, Angle);
		SetPlayerFacingAngle(playerid, Angle-10);
	}
	if(clickedid == AccsTD[8]) {
		new Float: Angle;
		GetPlayerFacingAngle(playerid, Angle);
		SetPlayerFacingAngle(playerid, Angle+10);
	}

	if(clickedid == AccsTD[10]) {
		if(ObjectID[playerid] == 0) return 1;
		SetPVarInt(playerid, "Edited", 0);
		ObjectID[playerid] --;
		update_accs(playerid);
	}
	if(clickedid == AccsTD[11]) {
		if(ObjectID[playerid] == count_objects(ObjectType[playerid])-1) return 1;
		SetPVarInt(playerid, "Edited", 0);
		ObjectID[playerid] ++;
		update_accs(playerid);
	}
	if(clickedid == AccsTD[12]) EditAttachedObject(playerid, ObjectType[playerid]);
	
    if(clickedid == Text: INVALID_TEXT_DRAW)
    {
        if(InDealer[playerid] == 1)
        {
            TogglePlayerControllableEx(playerid, 1);
			SetCameraBehindPlayer(playerid);
			SetPlayerVirtualWorld(playerid,0);
			CancelSelectTextDraw(playerid);
			HideDSTD(playerid);
			InDealer[playerid] = 0;
			SetPlayerPosEx(playerid,-1657.8622,1210.2476,7.2500);
			SetPlayerInterior(playerid, 0);
			DestroyVehicleEx(VehicleDealer[playerid], 11721);
        }
        if(PinStep[playerid] == 1)
        {
            HidePinTextDraws(playerid);
            PinStep[playerid] = 0;
        }
        if(Selecting_Textdraw[playerid] != -1)
        {
            SelectTextdraw(playerid, Selecting_Textdraw[playerid]);
        }
        if(GetPVarInt(playerid, "accs") == 1) exit_accs(playerid);
        return 0;
    }
    return 0;
}

