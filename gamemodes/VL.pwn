			if(PlayerInfo[playerid][pLanguage] == 1)GameTextForPlayer(playerid, "~w~Be Khane Khosh Amadid!!!", 5000, 3);

			SendClientMessage(playerid, COLOR_SYN2, "Congratulations, You can enter and exit here anytime.");
