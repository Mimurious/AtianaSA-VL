	{
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(PlayerInfo[giveplayerid][pWanted] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: This player don't have wanted.");
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		new vid = GetPlayerVehicleID(playerid);
		if(GetPlayerState(playerid) != 2 && !IsACopCar(vid))
		{
			SendClientMessage(playerid, COLOR_ERROR, "Error: You are not driver of a Police Vehicle.");
			return 1;
		}
		if(GetMaxSeats(vid) > 1)
		{
			new s = 1;
			new f = 0;
			while(f == 0 && s < GetMaxSeats(vid))
			{
				if(!IsSeatTaken(vid, s)){ PutPlayerInVehicleEx(giveplayerid, vid, s); f = 1; }
				s++;
			}
			if(f == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: This vehicle doesn't have seat empty to drag a player in.");
		}
		else return SendClientMessage(playerid, COLOR_ERROR, "Error: This vehicle doesn't have seat empty to drag a player in.");
