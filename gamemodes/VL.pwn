			ApplyDynamicActorAnimation(actorid, "ped", "cower", 4.1, 1, 1, 1, 1, 0);

			if(PlayerInfo[playerid][pLanguage] == 1) PlayerTextDrawSetString(playerid, MissionSub[playerid], "~r~MA'MORIYAT SHEKAST KHORD!");
		    else PlayerTextDrawSetString(playerid, MissionSub[playerid], "~r~MISSION FAILED!");
		    PlayerTextDrawShow(playerid, MissionSub[playerid]);
		    FailedInMission[playerid] = 1;

		    SendEngMessage(playerid, COLOR_LIGHTRED, "[MISSION] You attacked a wrong person and he/she called the police, you can leave by /leavemission.");
		    SendPerMessage(playerid, COLOR_LIGHTRED, "[MISSION] Shoma farde eshtebahi ra morede hadaf gharar dade va ou ba police tamas gereft, az /leavemission estefade konid.");

		    SetPlayerCriminal(playerid,999,"Trying to murder someone", 1);
		}
	}
}

function DestroyMissionActors(playerid)
{
	if(IsValidDynamicActor(actors[playerid][0])) {
	    DestroyDynamicActor(actors[playerid][0]);
	    DestroyDynamicActor(actors[playerid][1]);
	    DestroyDynamicActor(actors[playerid][2]);
	    DestroyDynamicActor(actors[playerid][3]);
	    DestroyDynamicActor(actors[playerid][4]);
	    for(new i; i < sizeof mactor; i++) DestroyDynamicActor(actors[playerid][5 + i]);
	    assignedcont[playerid] = 0;
	}
}

public OnGameModeInit()
{
	Code_ExTimer_Begin(GameModeInit);
	MapAndreas_Init(MAP_ANDREAS_MODE_FULL);
	SendRconCommand("hostname [LOADING...]");
	SendRconCommand("weburl [LOADING...]");
	SetGameModeText("[LOADING...]");
	SendRconCommand("mapname [LOADING...]");
	SendRconCommand("language [LOADING...]");

	MySQL_Init();

	mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = '0' WHERE `Status` != '0'");
	mysql_query(Database, gQuery);
	AntiDeAMX();
	LoadSystems();
	LoadActors();
	LoadEvents();
	LoadTimers();
	TimeLoad();
	load_objects_accesory();
	CreateObjects();
	CreateVehicles();
	CreatePickups();
	CreateTextLabels();
	CreateGlobalTextdraws();
	CreateGlobalObjects();
	//for(new i = 0; i < sizeof(SeatTaken); i++) SeatTaken[i] = INVALID_PLAYER_ID;

	DisableInteriorEnterExits();
	ShowPlayerMarkers(2);
	LimitPlayerMarkerRadius(600.0);
	EnableStuntBonusForAll(0);
	ManualVehicleEngineAndLights();
	UsePlayerPedAnims();

	pong[0] = CreatePongGame(826.6961, -1.1594, 1006.1797, 40, 180, 3, 53);
	pong[1] = CreatePongGame(831.5591, -1.1702, 1006.1797, 40, 180, 3, 53);

	ShowNameTags(0);

	ClanZone = CreateZone(2077, 978, 2339, 1188);
	CreateZoneBorders(ClanZone);


	SetVehicleSurfable(430, true);
	SetVehicleSurfable(446, true);
	SetVehicleSurfable(452, true);
	SetVehicleSurfable(453, true);
	SetVehicleSurfable(454, true);
	SetVehicleSurfable(472, true);
	SetVehicleSurfable(473, true);
	SetVehicleSurfable(484, true);
	SetVehicleSurfable(493, true);
	SetVehicleSurfable(595, true);

	for(new i = 0; i < sizeof(ReportInfo); i++) ResetReport(i);
	for(new i = 0; i < sizeof(HelpInfo); i++) ResetHelp(i);
	for(new i = 0; i < MAX_VEHICLES; i++) {
		VehCreator[i] = INVALID_PLAYER_ID;
        VehToDestroy[i] = INVALID_VEHICLE_ID;
	}

	mysql_format(Database, gQuery, sizeof(gQuery), "SELECT * FROM `settings`");
	new Cache:result = mysql_query(Database, gQuery);
	if(cache_num_rows())
	{
		new hostname[144], gamemode_text[144], gamemode_version[144], gamemode_map[144], gamemode_lang[144], url[144];
		cache_get_value_name(0, "Hostname", hostname);
		cache_get_value_name(0, "ModeText", gamemode_text);
		cache_get_value_name(0, "CurrentVersion", gamemode_version);
		cache_get_value_name(0, "MapName", gamemode_map);
		cache_get_value_name(0, "Language", gamemode_lang);
		cache_get_value_name(0, "URL", url);
		format(gString, sizeof(gString), "hostname %s", hostname);
		SendRconCommand(gString);
		format(gString, sizeof(gString), "%s v%s", gamemode_text, gamemode_version);
		SetGameModeText(gString);
		format(gString, sizeof(gString), "mapname %s", gamemode_map);
		SendRconCommand(gString);
		format(gString, sizeof(gString), "language %s", gamemode_lang);
		SendRconCommand(gString);
		format(gString, sizeof(gString), "weburl %s", url);
		SendRconCommand(gString);
	}
	cache_delete(result);
	
	for(new i = 1; i < MAX_VEHICLES; i++)
	{
		if(i == INVALID_VEHICLE_ID) continue;
		SetVehicleToRespawn(i);
	}

	printf("* Server started. [Took %dms to run.]", Code_ExTimer_End(GameModeInit));
	return 1;
}

function TimeLoad()
{
	new unused;
	gettime(LastPayDayH, unused, unused);
 	SetWorldTime(LastPayDayH);
	switch(random(5))
	{
		case 0: if(LastPayDayH == 18) HappyDayHour = 19;
		case 1: if(LastPayDayH == 19) HappyDayHour = 20;
		case 2: if(LastPayDayH == 20) HappyDayHour = 21;
		case 3: if(LastPayDayH == 21) HappyDayHour = 22;
		case 4:	if(LastPayDayH == 22) HappyDayHour = 18;
	}
}

function LoadTimers()
{
	moneytimer = SetTimer("MoneyTimer", 1000, true);
	othertimer = SetTimer("OtherTimer", 1000, true);
	checkcartimer = SetTimer("CheckCar", 150, true);
	checkfueltimer = SetTimer("CheckFuel", 20000, true);
	productiontimer = SetTimer("ProductTimer", 300000, true);
	custompickups = SetTimer("CustomPickups", 1000, true);
	paydaytimer = SetTimer("PayDayCheck", 60000, true);

	nametagtimer = SetTimer("UpdateNametag", 500, true);
	radartimer = SetTimer("RadarCheck", 500, true);
	clocktimer = SetTimer("ClockUpdate", 100, true);
}

function UnloadTimers()
{
	KillTimer(moneytimer);
	KillTimer(othertimer);
	KillTimer(productiontimer);
	KillTimer(checkcartimer);
	KillTimer(checkfueltimer);
	KillTimer(custompickups);
	KillTimer(paydaytimer);

	KillTimer(radartimer);
	KillTimer(clocktimer);
	KillTimer(nametagtimer);
}

