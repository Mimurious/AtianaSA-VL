cmd:throwback(playerid, params[])
{
	if (IsPlayerConnected(playerid))
	{
		if (PlayerFishes[playerid][0] < 1 && PlayerFishes[playerid][1] < 1 && PlayerFishes[playerid][2] < 1 && PlayerFishes[playerid][3] < 1 && PlayerFishes[playerid][4] < 1)
			return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have any fish to release.");

		new fish;

		if (sscanf(params, "i", fish))
			return SendSyntaxMessage(playerid, "/throwback <FishID>");

		if (fish <= 5 && fish >= 1)
		{
			if (PlayerFishes[playerid][fish - 1] < 1)
				return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have any fish in that slot.");

			PlayerFishes[playerid][fish - 1] = 0;

			new
			string[100];

			format(string, sizeof(string), "* %s throws a fish back in the water.", GetName(playerid));
			MeAction(playerid, string);
		}
		else return SendSyntaxMessage(playerid, "/throwback <FishID>");
	}
	return 1;
}

