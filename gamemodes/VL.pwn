		Update(giveplayerid,pFWarnsx);
		InTraining[playerid] = 0;
		OnDuty[giveplayerid] = 0;
		PlayerInfo[giveplayerid][pFPunish] = punish;
		Update(giveplayerid, pFPunishx);
		format(gString, sizeof(gString), "You kicked %s from your faction with %d faction punish.",GetName(giveplayerid), punish);
		SendClientMessage(playerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Leader %s kicked you from %s with %d faction punish.",GetName(playerid),FactionName[PlayerInfo[playerid][pLeader]], punish);
		SendClientMessage(giveplayerid, COLOR_SYN2, gString);
		format(gString, sizeof(gString), "Left the faction (with %d FP)", punish);
		FactionLog(PlayerInfo[playerid][pLeader], GetDatabaseID(playerid), GetDatabaseID(giveplayerid), gString);
		PlayerInfo[giveplayerid][pSpawnChange] = 1;
		Update(giveplayerid, pSpawnChangex);
		SetPlayerSkin(giveplayerid, 250);
		PlayerInfo[giveplayerid][pSkin] = 250;
		Update(giveplayerid, pSkinx);
		TeamColor(giveplayerid);
		new skins[100], cnt;
		for(new i = 1; i <= pskins; i++)
		{
			if(SkinInfo[i][sOwner] != PlayerInfo[giveplayerid][pSQLID]) continue;
			skins[cnt] = SkinInfo[i][sModel];
			cnt++;
		}
		ShowModelSelectionMenuEx(giveplayerid, skins, cnt, "Clothes Selection", 66669);
	}
	return 1;
}

