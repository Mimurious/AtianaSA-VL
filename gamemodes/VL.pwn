        strmid(CarPlate[playerid][idcar], inputtext, 0, strlen(inputtext), 999);
		SendClientMessage(playerid, COLOR_SYN3, "The license plate will be changed in the next respawn of the vehicle.");
		cUpdate(playerid, idcar, cLicensex);

