function LoadHouses()
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	Code_ExTimer_Begin(LoadHouses);
	if(rows)
	{
		new tmp[128];
		new id = 0;
		for(new i = 0; i < rows; i++)
		{
			id++;
			cache_get_value_name(i, "EntranceX", tmp);			HouseInfo[id][hEntranceX] = floatstr(tmp);
			cache_get_value_name(i, "EntranceY", tmp);			HouseInfo[id][hEntranceY] = floatstr(tmp);
			cache_get_value_name(i, "EntranceZ", tmp);			HouseInfo[id][hEntranceZ] = floatstr(tmp);
			cache_get_value_name(i, "ExitX", tmp);				HouseInfo[id][hExitX] = floatstr(tmp);
			cache_get_value_name(i, "ExitY", tmp);				HouseInfo[id][hExitY] = floatstr(tmp);
			cache_get_value_name(i, "ExitZ", tmp);				HouseInfo[id][hExitZ] = floatstr(tmp);
			cache_get_value_name(i, "Owner", tmp);				strmid(HouseInfo[id][hOwner], tmp, 0, MAX_PLAYER_NAME, MAX_PLAYER_NAME);
			cache_get_value_name(i, "Value", tmp);				HouseInfo[id][hValue] = strval(tmp);
			cache_get_value_name(i, "Interior", tmp);			HouseInfo[id][hInt] = strval(tmp);
			cache_get_value_name(i, "Lock", tmp);				HouseInfo[id][hLock] = strval(tmp);
			cache_get_value_name(i, "Owned", tmp);				HouseInfo[id][hOwned] = strval(tmp);
			cache_get_value_name(i, "Rent", tmp);				HouseInfo[id][hRent] = strval(tmp);
			cache_get_value_name(i, "Rentable", tmp);			HouseInfo[id][hRentable] = strval(tmp);
			cache_get_value_name(i, "Safe", tmp);				HouseInfo[id][hSafe] = strval(tmp);
			cache_get_value_name(i, "Level", tmp);				HouseInfo[id][hLevel] = strval(tmp);
			cache_get_value_name(i, "World", tmp);				HouseInfo[id][hWorld] = strval(tmp);
			cache_get_value_name(i, "SellPrice", tmp);			HouseInfo[id][hSellPrice] = strval(tmp);

			Total_House_Created ++;
			new o = HouseInfo[id][hOwned];
			new r = HouseInfo[id][hRentable];

			new lString[128];
			if(o) format(gString, sizeof(gString), "{FFFFFF}%s{2ECC71}'s House (#%d)\n",HouseInfo[id][hOwner], id);
			else format(gString, sizeof(gString), "House #%d\n",id);

			format(lString, sizeof(lString), "This house is for sale!\n");
			if(!o) strins(gString, lString, strlen(gString));
			format(lString, sizeof(lString), "Price: {FFFFFF}$%s\n", FormatNumber(HouseInfo[id][hValue]));
			if(!o) strins(gString, lString, strlen(gString));
			format(lString, sizeof(lString), "{2ECC71}Level: {FFFFFF}%d\n", HouseInfo[id][hLevel]);
			strins(gString, lString, strlen(gString));
   			if(HouseInfo[id][hRent] != 0) format(lString, sizeof(lString), "{2ECC71}Rent:{FFFFFF} $%s\n", FormatNumber(HouseInfo[id][hRent]));
   			else format(lString, sizeof(lString), "{2ECC71}Rent:{FFFFFF} Free\n", FormatNumber(HouseInfo[id][hRent]));
			if(o&&r) strins(gString, lString, strlen(gString));
			format(lString, sizeof(lString), "{2ECC71}To rent a room type /rentroom.");
			if(o&&r) strins(gString, lString, strlen(gString));
			format(lString, sizeof(lString), "{2ECC71}To buy this house type /buyhouse.");
			if(!o) strins(gString, lString, strlen(gString));

			if(!o) HouseInfo[id][hPickup] = CreateDynamicPickup(1273, 1, HouseInfo[id][hEntranceX], HouseInfo[id][hEntranceY], HouseInfo[id][hEntranceZ], -1, -1, -1, 15.0);
			if(!o) HouseInfo[id][hMapIcon] = CreateDynamicMapIcon(HouseInfo[id][hEntranceX], HouseInfo[id][hEntranceY], HouseInfo[id][hEntranceZ], 31, -1, -1, -1, -1, 30.0);
			if(o) HouseInfo[id][hPickup] = CreateDynamicPickup(1272, 1, HouseInfo[id][hEntranceX], HouseInfo[id][hEntranceY], HouseInfo[id][hEntranceZ], -1, -1, -1, 15.0);
			if(o) HouseInfo[id][hMapIcon] = CreateDynamicMapIcon(HouseInfo[id][hEntranceX], HouseInfo[id][hEntranceY], HouseInfo[id][hEntranceZ], 32, -1, -1, -1, -1, 30.0);

			HouseInfo[id][hTextInfo] = CreateDynamic3DTextLabel(gString,0x2ECC71FF,HouseInfo[id][hEntranceX], HouseInfo[id][hEntranceY], HouseInfo[id][hEntranceZ]+0.5, 20, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, -1, -1, -1, 15.0);

			AllHouses++;
		}
	}
	printf("* %d houses loaded. [Took %dms to load.]", rows, Code_ExTimer_End(LoadHouses));
	return 1;
}

