cmd:sleep(playerid, params[])
{
	if(AFK[playerid] == 0)
	{
	    if(HouseEntered[playerid] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in your own/rented house.");
		if(HouseEntered[playerid] != PlayerInfo[playerid][pHouseKey] && HouseEntered[playerid] != PlayerInfo[playerid][pRentKey]) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are not in your own/rented house.");
		if(IsPlayerInRangeOfPoint(playerid, 3, HouseInfo[HouseEntered[playerid]][hExitX],HouseInfo[HouseEntered[playerid]][hExitY],HouseInfo[HouseEntered[playerid]][hExitZ])) return SendClientMessage(playerid, COLOR_ERROR, "Error: You are too close to the exit door of the house.");
		if(PlayerInfo[playerid][pWanted] > 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot sleep while you have wanted.");
        if(PlayerInfo[playerid][pPayDay] < 5 && (PlayerInfo[playerid][pPremiumAccount] < gettime() && PlayerInfo[playerid][pPremiumAccount] != -1)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to play for at least 5 minutes for this hour before trying to sleep.");
		AFK[playerid] = 1;
		TogglePlayerControllableEx(playerid, false);
		SetPlayerColor(playerid, 0x909090FF);
		ApplyAnimation(playerid,"CRACK","crckidle2",4.1,0,1,1,1,1);
		DespawnCar(playerid);
		SendEngMessage(playerid, COLOR_PURPLE, "Now you can stay AFK without being kicked. Use /sleep again when you want to come back to the game.");
		SendEngMessage(playerid, COLOR_PURPLE, "Minimizing the game (alt-tab) can cause some bugs. It is recommended to use the ESC before minimizing the game.");
		SendEngMessage(playerid, COLOR_PURPLE, "All of your personal vehicles have been despawned, you can spawn and use them any time that you come back to the game.");
		
		SendPerMessage(playerid, COLOR_PURPLE, "Ham aknun shoma mitavanid bedune kick shodan AFK konid. Baraye bazgasht be bazi dar har zaman az /sleep estefade konid.");
		SendPerMessage(playerid, COLOR_PURPLE, "Minimize kardane bazi (alt-tab) momken ast moshkelati ra ijad konad. Behtar ast qabl az an az ESC estefade konid.");
		SendPerMessage(playerid, COLOR_PURPLE, "Tamamiye vasayele naghliyeye shoma napadid shodand, bad az bargashtanetan mitavanid aan ha ra mojadadan zaher konid.");

		mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = '2' WHERE `ID` = %d", PlayerInfo[playerid][pSQLID]);
		mysql_query(Database, gQuery);

		UpdateNt(playerid);
	}
	else
	{
		AFK[playerid] = 0;
		TogglePlayerControllableEx(playerid, true);
		ApplyAnimation(playerid,"SUNBATHE", "Lay_Bac_out",4.1,0,0,0,0,1);
		TeamColor(playerid);
		SetPlayerHealthEx(playerid, 100);
		SendEngMessage(playerid, COLOR_PURPLE, "Welcome back! You can sleep any time again.");
		SendPerMessage(playerid, COLOR_PURPLE, "Mojadadan khosh amadid! Dar har zamani ke bekhahid mitavanid dobare bekhabid.");

		mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Status` = 1 WHERE `ID` = %d", PlayerInfo[playerid][pSQLID]);
		mysql_query(Database, gQuery);

		UpdateNt(playerid);
	}
	return 1;
}

cmd:housewithdraw(playerid, params[])
{
	new money;
	if(PlayerInfo[playerid][pHouseKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't own a house.");
	new house = PlayerInfo[playerid][pHouseKey];
	if(sscanf(params,"i", money)) return SendSyntaxMessage(playerid, "/housewithdraw <Amount ($+)>"), format(gString, sizeof(gString), "You have $%s in house safe.", FormatNumber(HouseInfo[house][hSafe])), SendClientMessage(playerid, COLOR_GRAD3, gString);
	{
		if(HouseEntered[playerid] != house) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be in your house.");
		if(money > HouseInfo[house][hSafe] || money < 1) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money in house safe.");
		GivePlayerCash(playerid,money);
		HouseInfo[house][hSafe]=HouseInfo[house][hSafe]-money;
		hUpdate(house, hSafex);
		format(gString, sizeof(gString), "  You have withdrawn $%s from your house safe, Total: $%s ", FormatNumber(money),FormatNumber(HouseInfo[house][hSafe]));
		SendClientMessage(playerid, COLOR_YELLOW, gString);
		PPWithdrawLog(playerid, house, money, 2);
	}
	return 1;
}

cmd:sellhouseto(playerid, params[])
{
	new giveplayerid, splayer[25], price;
	if(PlayerInfo[playerid][pPinLogged] == 0 && strlen(PlayerInfo[playerid][pPin]) != 1) return SendEngMessage(playerid, COLOR_YELLOW2, PINEN), SendPerMessage(playerid, COLOR_YELLOW2, PINFA);
	if(PlayerInfo[playerid][pHouseKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't own a house.");
	if(sscanf(params,"s[25]i", splayer,price)) return SendSyntaxMessage(playerid, "/sellhouseto <Player ID/Name> <Price ($+)>");
	{
		new house = PlayerInfo[playerid][pHouseKey];
		if(!IsPlayerInRangeOfPoint(playerid, 3.0, HouseInfo[house][hEntranceX], HouseInfo[house][hEntranceY], HouseInfo[house][hEntranceZ])) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be near your house.");
		if(GetPlayers(splayer) == INVALID_PLAYER_ID) return InvalidPlayer(playerid);
		else if(GetPlayers(splayer) == 1000) return ToManyResults(playerid);
		else if(GetPlayers(splayer) == 1001) return ShowResults(playerid, splayer);
		else giveplayerid = GetPlayers(splayer);
		if(!ProxDetectorS(5.0, playerid, giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player is not near you.");
		if(PlayerInfo[giveplayerid][pHouseKey] != 0) return SendClientMessage(playerid, COLOR_ERROR, "Player already own a house.");
		if(MoneyACheck(playerid, price, 1, 10000000)) return 1;
		if(price > GetPlayerCash(giveplayerid)) return SendClientMessage(playerid, COLOR_ERROR, "Player don't have enough money.");
		if(PlayerInfo[giveplayerid][pLevel] < HouseInfo[house][hLevel]) return SendClientMessage(playerid, COLOR_ERROR, "Player level is not high enough.");

		HousePrice[giveplayerid][playerid] = price;
