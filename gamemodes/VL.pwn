Dialog:FRIENDS1(playerid, response, listitem, inputtext[])
{
	new string[128];
	if(!response) return 1;
	new id;
	if(sscanf(inputtext, "u", id))
	{
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}That player is not online!");
	}
	else
	{
		if(id == playerid) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}You cannot add yourself to the list!");
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}That player is offline!");
		new test, friends;
		new result[256];
		format(string, sizeof(string), "SELECT * FROM `friends` WHERE `AddBy`='%d'", PlayerInfo[playerid][pSQLID]);
		new Cache: db = mysql_query (Database, string);
		cache_get_row_count(q_rc);
		for ( new i, j = q_rc; i != j; ++i )
		{
			friends++;
			cache_get_value_name( i, "friendID", result ); test = strval( result );
			if(test == PlayerInfo[id][pSQLID]) return SendClientMessage(playerid, COLOR_WHITE, "This player is already in your list!");
		}
		if(friends == 25 && (PlayerInfo[playerid][pPremiumAccount] < gettime() && PlayerInfo[playerid][pPremiumAccount] != -1)) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}* You need a premium account to have more than 25 friends!");
		if(friends == 50) return SendClientMessage(playerid, COLOR_WHITE, "{FFB870}* You have too many friends!");
		cache_delete( db );
		format(string, sizeof(string), "{FFB870}You've added %s (%d) in your friendlist.", GetName(id), id);
		SendClientMessage(playerid, COLOR_WHITE, string);
		format(string, sizeof(string), "{FFB870}%s (%d) added you in their friendlist.", GetName(playerid), playerid);
		SendClientMessage(id, COLOR_WHITE, string);
		format(string, sizeof(string), "INSERT INTO `friends` (`friendID`, `friendName`, `AddBy`) VALUES ('%d', '%s', '%d')", PlayerInfo[id][pSQLID], PlayerInfo[id][pUsername], PlayerInfo[playerid][pSQLID]);
		mysql_tquery(Database, string, "", "");
	}
	return 1;
}

