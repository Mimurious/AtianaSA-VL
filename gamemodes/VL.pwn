cmd:findbiz(playerid, params[])
{
	if(!gLogged[playerid])
		return SendEngMessage(playerid, COLOR_ERROR, "Error: You are not logged in."), SendPerMessage(playerid, COLOR_ERROR, "Lotfan baraye estefade az dastoorate server login konid.");

	new id;

	if(CheckPoint[playerid] != 0) return SendClientMessage(playerid, COLOR_GOLD, "Shoma yek checkpointe fa'al darid. Az '/cancel checkpoint' baraye cancel kardane an estefade konid.");

	if(sscanf(params, "i", id))
		return Dialog_Show(playerid, GPS5, DIALOG_STYLE_INPUT, "Businesses", "Please enter the business ID to locate:", "Checkpoint", "Cancel");

	if(id < 1 || id > businesses_count)
	{
		SendEngMessage(playerid, COLOR_ERROR, "Error: Invalid biz id.");
		SendPerMessage(playerid, COLOR_ERROR, "Error: Invalid biz id.");
	}

	SetPlayerCheckpointEx(playerid, BizzInfo[id][bEntranceX], BizzInfo[id][bEntranceY], BizzInfo[id][bEntranceZ], 5.0);
	CheckPoint[playerid] = 1;

	SendEngMessage(playerid, COLOR_SYN2, "A checkpoint has been set on your map.");
	SendPerMessage(playerid, COLOR_SYN2, "Yek checkpoint ruye naghsheye shoma gharar dade shod.");

	return 1;
}

cmd:findhouse(playerid, params[])
{
	if(!gLogged[playerid])
		return SendEngMessage(playerid, COLOR_ERROR, "Error: You are not logged in."), SendPerMessage(playerid, COLOR_ERROR, "Lotfan baraye estefade az dastoorate server login konid.");

	new id;

	if(CheckPoint[playerid] != 0) return SendClientMessage(playerid, COLOR_GOLD, "Shoma yek checkpointe fa'al darid. Az '/cancel checkpoint' baraye cancel kardane an estefade konid.");

	if(sscanf(params, "i", id))
		return Dialog_Show(playerid, GPS4, DIALOG_STYLE_INPUT, "Houses", "Please enter the house ID to locate:", "Checkpoint", "Cancel");

	if(id < 1 || id > sizeof(HouseInfo) - 1) return SendClientMessage(playerid, COLOR_ERROR, "Invalid house id.");
