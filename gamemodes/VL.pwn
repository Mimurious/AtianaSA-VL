Dialog:REPORT(playerid, response, listitem, inputtext[])
{
    if (!response) SendEngMessage(playerid, COLOR_WHITE,"{FFFF45}You have canceled your report request."), SendPerMessage(playerid, COLOR_WHITE,"{FFFF45}Shoma darkhaste gozareshe khod ra laghv kardid.");

	if(response)
	{
		ReportInfo[reportid][rPlayer] = playerid;
