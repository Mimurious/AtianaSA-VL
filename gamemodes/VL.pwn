cmd:rc(playerid, params[])
{
	new msg[128];
	if(sscanf(params,"s[128]",msg)) return SendSyntaxMessage(playerid, "/rc <Text>");
	{
		if(DebugMode == false) if(ReportChat[playerid] == -1) return SendClientMessage(playerid, COLOR_ERROR, "No active help conversation.");
		new string2[128];

		if(ReportChat[playerid] != -1)
		{
			if(PlayerInfo[playerid][pAdmin]) format(gString, sizeof(gString), "<RC> %s %s: %s", GetStaffN(playerid, 1),  GetName(playerid), msg);
			else format(gString, sizeof(gString), "<RC> %s : %s", GetName(playerid), msg);
			if(strlen(gString) > 120)
			{
				strmid(string2, gString, 110, 256);
				strdel(gString, 110, 128);

				format(gString,128,"%s...",gString);
				SendClientMessage(ReportChat[playerid], COLOR_YELLOW, gString);
				SendClientMessage(playerid, COLOR_YELLOW, gString);

				format(string2,128,"...%s",string2);
				SendClientMessage(ReportChat[playerid], COLOR_YELLOW, string2);
				SendClientMessage(playerid, COLOR_YELLOW, string2);

			}
			else
			{
				SendClientMessage(ReportChat[playerid], COLOR_YELLOW, gString);
				SendClientMessage(playerid, COLOR_YELLOW, gString);
			}
		}
	}
	return 1;
}

