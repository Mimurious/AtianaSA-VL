Ban_GetLongIP(const ip[])
{
	new len = strlen(ip);
	if (!(len > 0 && len < 17))
	{
		return 0;
	}

	new count;
	new pos;
	new dest[3];
	new val[4];
	for (new i; i < len; i++)
	{
		if (ip[i] == '.' || i == len)
		{
			strmid(dest, ip, pos, i);
			pos = (i + 1);

			val[count] = strval(dest);
			if (!(0 <= val[count] <= 255))
			{
				return 0;
			}

			count++;
			if (count > 3)
			{
				return 0;
			}
		}
	}

	if (count != 3)
	{
		return 0;
	}
	return ((val[0] * 16777216) + (val[1] * 65536) + (val[2] * 256) + (val[3]));
}

