forward UpdateNametag();
public UpdateNametag()
{
	foreach(new i : Player) UpdateNt(i);
}

function UpdateNt(i)
{
	if(IsPlayerConnected(i))
	{
		new nametag[256], playername[MAX_PLAYER_NAME], Float:armour;
		GetPlayerArmourEx(i, armour);
		GetPlayerName(i, playername, sizeof(playername));
		new id = PlayerInfo[i][pCMember] + PlayerInfo[i][pCLeader];
		new nString[144];
		if(id != 0) {
			if(ClanInfo[id][cTagType] == 0)
			{
				format(nString, sizeof(nString),"{%s}%s{%06x}%s", ClanInfo[id][cColor], ClanInfo[id][cTag], GetPlayerColor(i) >>> 8, PlayerInfo[i][pUsername]);
			}
			else if(ClanInfo[id][cTagType] == 1)
			{
				format(nString, sizeof(nString),"{%06x}%s{%s}%s", GetPlayerColor(i) >>> 8, PlayerInfo[i][pUsername], ClanInfo[id][cColor], ClanInfo[id][cTag]);
			}
		}
		else {
			format(nString, sizeof(nString),"{%06x}%s", GetPlayerColor(i) >>> 8, PlayerInfo[i][pUsername]);
		}

		if(armour > 1.0)
		{
			format(nametag, sizeof(nametag), "%s {FFFFFF}(%i)%s\n{FFFFFF}%s\n{FF0000}%s%s", nString, i, (AFK[i]) ? " {909090}[SLEEP]" : IsPlayerPaused(i) ? " {CECECE}[ALT-TABBED]" : "", GetArmorDots(i), GetHealthDots(i), GetWantedStars(i));
		}
		else
		{
			format(nametag, sizeof(nametag), "%s {FFFFFF}(%i)%s\n{FF0000}%s%s", nString, i, (AFK[i]) ? " {909090}[SLEEP]" : IsPlayerPaused(i) ? " {CECECE}[ALT-TABBED]" : "", GetHealthDots(i), GetWantedStars(i));
		}
