				GetVehiclePos(vehicleid, px, py, pz);
				SetVehiclePos(vehicleid, px - (carid / 2), py - (carid / 2), pz);
			}
			if(pa >= 157.5 && pa <= 202.5) //s
			{
				GetVehiclePos(vehicleid, px, py, pz);
				SetVehiclePos(vehicleid, px, py - carid, pz);
