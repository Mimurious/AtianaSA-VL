		CreateDynamicPickup(1275, 1, JobInfo[i][jPosx], JobInfo[i][jPosy], JobInfo[i][jPosz], -1, -1, -1, 10.0);
	}
}

function LoadSafes()
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	Code_ExTimer_Begin(LoadSafes);
	if(rows)
	{
		new tmp[128];
		new id = 0;
		for(new i = 0; i < rows; i++)
		{
			id++;
			cache_get_value_name(i, "PosX", tmp), 		SafeInfo[id][sPosX] = floatstr(tmp);
			cache_get_value_name(i, "PosY", tmp), 		SafeInfo[id][sPosY] = floatstr(tmp);
			cache_get_value_name(i, "PosZ", tmp), 		SafeInfo[id][sPosZ] = floatstr(tmp);
			cache_get_value_name(i, "Money", tmp), 		SafeInfo[id][sMoney] = strval(tmp);
			cache_get_value_name(i, "Drugs", tmp), 		SafeInfo[id][sDrugs] = strval(tmp);
			cache_get_value_name(i, "Materials", tmp), 	SafeInfo[id][sMaterials] = strval(tmp);
			cache_get_value_name(i, "News", tmp);		strmid(SafeInfo[id][sNews], tmp, false, strlen(tmp), 128);
            cache_get_value_name(i, "Slot", tmp), 	SafeInfo[id][sSlot] = strval(tmp);
            cache_get_value_name(i, "MinLvl", tmp), 	SafeInfo[id][sMinLvl] = strval(tmp);
            cache_get_value_name(i, "Opened", tmp), 	SafeInfo[id][sOpened] = strval(tmp);
            cache_get_value_name(i, "EndDate", tmp), 	SafeInfo[id][sEndDate] = strval(tmp);

			CreateDynamic3DTextLabel("Faction SAFE\nType /safe to acces it.", -1, SafeInfo[id][sPosX], SafeInfo[id][sPosY], SafeInfo[id][sPosZ], 20.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, -1, -1, -1, 10.0);
			CreateDynamicPickup(1239, 1, SafeInfo[id][sPosX], SafeInfo[id][sPosY], SafeInfo[id][sPosZ], -1, -1, -1, 10.0);
		}
	}
	printf("* %d faction safes loaded. [Took %dms to load.]", rows, Code_ExTimer_End(LoadSafes));
	return 1;
}

function IsNearSafe(playerid)
{
	new faction = PlayerInfo[playerid][pMember] + PlayerInfo[playerid][pLeader];
	if(IsPlayerInRangeOfPoint(playerid, 2.0, SafeInfo[faction][sPosX], SafeInfo[faction][sPosY], SafeInfo[faction][sPosZ])) return 1;
	return 0;
}

function LoadZones()
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	Code_ExTimer_Begin(LoadZones);
	if(rows)
	{
		new tmp[128];
		new id = 0;
		for(new i = 0; i < rows; i++)
		{
			id++;
			cache_get_value_name(i, "MinX", tmp), ZoneInfo[id][zMinX] = floatstr(tmp);
			cache_get_value_name(i, "MinY", tmp), ZoneInfo[id][zMinY] = floatstr(tmp);
			cache_get_value_name(i, "MaxX", tmp), ZoneInfo[id][zMaxX] = floatstr(tmp);
			cache_get_value_name(i, "MaxY", tmp), ZoneInfo[id][zMaxY] = floatstr(tmp);
			cache_get_value_name(i, "Team", tmp), ZoneInfo[id][zTeam] = strval(tmp);
			cache_get_value_name(i, "Hours", tmp), ZoneInfo[id][zHours] = strval(tmp);
			cache_get_value_name(i, "Name", tmp);		strmid(ZoneInfo[id][zName], tmp, false, strlen(tmp), 32);

			Zone[id] = CreateZone(ZoneInfo[id][zMinX], ZoneInfo[id][zMinY], ZoneInfo[id][zMaxX], ZoneInfo[id][zMaxY]);

			CreateZoneBorders(Zone[id]);
			CreateZoneNumber(Zone[id], id, 1.3);

			TakeON[id] = 0;
			ZoneAreaTurf[i] = CreateDynamicRectangle(ZoneInfo[id][zMinX], ZoneInfo[id][zMinY], ZoneInfo[id][zMaxX], ZoneInfo[id][zMaxY], -1, -1, -1);

			AllZones++;
		}
	}
	printf("* %d zones loaded. [Took %dms to load.]", AllZones, Code_ExTimer_End(LoadZones));
	return 1;
}

