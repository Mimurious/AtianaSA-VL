		new string2[128];
		format(gString, sizeof(gString), "[Report: #%d] %s (%d): %s", reportid, GetName(playerid), playerid, ReportInfo[reportid][rReason]);
		if(strlen(gString) > 120)
		{
			strmid(string2, gString, 110, 256);
