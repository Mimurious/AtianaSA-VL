cmd:missions(playerid, params[])
{
    if(GetPlayerWantedLevel(playerid) > 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot play missions while you are a wanted suspect.");
	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 263.6465,2895.4883,10.5314))
	{
		if(CheckPoint[playerid] != 0) return SendClientMessage(playerid, COLOR_GOLD, "Shoma yek checkpointe fa'al darid. Az '/cancel checkpoint' baraye cancel kardane an estefade konid.");
		CheckPoint[playerid] = 1;
		SetPlayerCheckpointEx(playerid, 263.6465,2895.4883,10.5314, 5.0);
		SendEngMessage(playerid, COLOR_ERROR, "Error: You are not near Cesar Vialpando.");
		return SendPerMessage(playerid, COLOR_ERROR, "Khata: Shoma nazdike Cesar Vialpando nistid.");
	}
	if(IsPlayerInRangeOfPoint(playerid, 3.0, 263.6465,2895.4883,10.5314))
	{
		if(PlayerInfo[playerid][pMission1] == 1 || PlayerInfo[playerid][pMission2] == 1) return SendEngMessage(playerid, COLOR_ERROR, "Error: You are already in a mission."), SendPerMessage(playerid, COLOR_ERROR, "Khata: Shoma dar hale hazer dar yek ma'moriyat hastid.");

		format(gString, sizeof gString, "Mission name\tStatus\n");
	 	if(PlayerInfo[playerid][pMission1] == 0) format(gString, sizeof gString,"%sBack in business\t{00FF00}Available", gString);
		else if(PlayerInfo[playerid][pMission1] == 2) format(gString, sizeof gString,"%sBack in business\t{0000FF}Completed", gString);

		if(PlayerInfo[playerid][pMission2] == 0) format(gString, sizeof gString,"%s\nAssassination\t{00FF00}Available", gString);
		else if(PlayerInfo[playerid][pMission2] == 2) format(gString, sizeof gString,"%s\nAssassination\t{0000FF}Completed", gString);


        Dialog_Show(playerid, MISSIONS, DIALOG_STYLE_TABLIST_HEADERS,"Missions List:",gString,"Select", "Cancel");
	}
    return 1;
}

cmd:leavemission(playerid, params[])
{
	if(PlayerInfo[playerid][pMission1] != 1 && PlayerInfo[playerid][pMission2] != 1) return SendEngMessage(playerid, COLOR_ERROR, "Error: You are not in any mission."), SendPerMessage(playerid, COLOR_ERROR, "Khata: Shoma dar hale hazer dar hich ma'moriyati nistid.");

    if(PlayerInfo[playerid][pMission1] == 1) {
		PlayerInfo[playerid][pMission1] = 0;
		Update(playerid, pMission1x);

		SendEngMessage(playerid, COLOR_SYN, "You left the mission sucessfully.");
		SendPerMessage(playerid, COLOR_SYN, "Shoma ba movafaghiyat az ma'moriyat kharej shodid.");

		PlayerTextDrawHide(playerid, MissionSub[playerid]);
        TogglePlayerControllable(playerid, 1);
	}
	else if(PlayerInfo[playerid][pMission2] == 1) {
		PlayerInfo[playerid][pMission2] = 0;
		Update(playerid, pMission2x);
		DestroyMissionActors(playerid);
		SetPlayerVirtualWorld(playerid, 0);
	    SetPlayerFacingAngle(playerid, 0);
	    SetPlayerInterior(playerid, 0);
        SetCameraBehindPlayer(playerid);
        SetPlayerPosEx(playerid, posyx[playerid][0], posyx[playerid][1], posyx[playerid][2]);
        PlayerTextDrawHide(playerid, MissionSub[playerid]);
        TogglePlayerControllable(playerid, 1);
        ResetPlayerWeapons(playerid);
        FailedInMission[playerid] = 0;
        KillTimer(PlayerTimers[playerid][4]);

		SendEngMessage(playerid, COLOR_SYN, "You left the mission sucessfully.");
		SendPerMessage(playerid, COLOR_SYN, "Shoma ba movafaghiyat az ma'moriyat kharej shodid.");
	}

	return 1;
}

Dialog:MISSIONS(playerid, response, listitem, inputtext[])
{
	if(response)
	{
        if(listitem == 0) {
            if(PlayerInfo[playerid][pMission1] != 0) return SendEngMessage(playerid, COLOR_ERROR, "Error: This mission is unavailable now."), SendPerMessage(playerid, COLOR_ERROR, "Khata: In ma'moriyat hamaknoon dar dastras nist.");
			Dialog_Show(playerid, BACKINBIZ, DIALOG_STYLE_MSGBOX,"'Back in business' mission","{A9C4E4}In this mission you need to find some bundles around the map.\n\nDifficulty: {FF0000}Hard\n{A9C4E4}Price: {00FF00}30,000$, 2 levels & 3 premium points","Start", "Back");
		}
