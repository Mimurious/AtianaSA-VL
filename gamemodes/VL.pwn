cmd:sellbiz(playerid, params[])
{
	if(PlayerInfo[playerid][pPinLogged] == 0 && strlen(PlayerInfo[playerid][pPin]) != 1) return SendEngMessage(playerid, COLOR_YELLOW2, PINEN), SendPerMessage(playerid, COLOR_YELLOW2, PINFA);
	if(PlayerInfo[playerid][pBizzKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't own a business.");
	new bizz = PlayerInfo[playerid][pBizzKey];
	new moneyback = (3*BizzInfo[bizz][bPrice])/6 + BizzInfo[bizz][bSafe];
	format(gString,sizeof(gString),"Do you want to sell your business for: $%s",FormatNumber(moneyback));
	if(PlayerInfo[playerid][pLanguage] == 0)Dialog_Show(playerid, sellbiztostate, DIALOG_STYLE_MSGBOX, "Sell Business:", gString, "Sell", "Cancel");
	format(gString,sizeof(gString),"Aya shoma motmaen hastid ke mikhahid tejarate khod ra be $%s befroshid?",FormatNumber(moneyback));
	if(PlayerInfo[playerid][pLanguage] == 1)Dialog_Show(playerid, sellbiztostate, DIALOG_STYLE_MSGBOX, "Foroshe Tejarat:", gString, "Forosh", "Laghv");
	return 1;
}

cmd:bizfee(playerid, params[])
{
	new fee;
	if(PlayerInfo[playerid][pBizzKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't own a business.");
	if(sscanf(params,"i", fee)) return SendSyntaxMessage(playerid, "/bizfee <Amount ($+)>");
	{
		new bizz = PlayerInfo[playerid][pBizzKey];
		if(MoneyACheck(playerid, fee, 1, 10)) return 1;

		BizzInfo[bizz][bEntranceCost] = fee;
		bUpdate(bizz, bEntranceCostx);
		format(gString, sizeof(gString), "Bizz fee set to $%s.", FormatNumber(BizzInfo[bizz][bEntranceCost]));
		SendClientMessage(playerid, COLOR_GRAD3, gString);
		BizzUpdate(bizz);
	}
	return 1;
}

cmd:bizdesc(playerid, params[])
{
	new fee[65];
	if(PlayerInfo[playerid][pBizzKey] == 0) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't own a business.");
	if(sscanf(params,"s[65]", fee)) return SendSyntaxMessage(playerid, "/bizdesc <Description>");
	{
		new bizz = PlayerInfo[playerid][pBizzKey];
		BizzInfo[bizz][bDiscription] = fee;
