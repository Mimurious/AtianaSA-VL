	if(weaponid == 49 || weaponid == 50 || weaponid == 51 || weaponid == 53 || weaponid == 54 || GetPlayerState(damagedid) == PLAYER_STATE_DRIVER) InDmEvent[playerid] = 0;
	if(GetPlayerState(playerid) != PLAYER_STATE_PASSENGER && GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return 0;
