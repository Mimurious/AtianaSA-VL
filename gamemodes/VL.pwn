cmd:transfer(playerid, params[])
{
	new splayer[25], money;
	if(PlayerInfo[playerid][pPinLogged] == 0 && strlen(PlayerInfo[playerid][pPin]) != 1) return SendEngMessage(playerid, COLOR_YELLOW2, PINEN), SendPerMessage(playerid, COLOR_YELLOW2, PINFA);
	if(BizzEntered[playerid] != 2 && BizzEntered[playerid] != 32 && BizzEntered[playerid] != 33) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need to be in Bank to transfer money.");
	if(sscanf(params,"s[25]i", splayer, money)) return SendSyntaxMessage(playerid, "/transfer <Player Name> <Amount ($+)>"), SendClientMessage(playerid, COLOR_SYN, "- Notice! The player name must be his/her full name and the transfered money will be unrefundable if you made a mistake.");
	{
	    if(!ExistName(splayer)) return SendClientMessage(playerid, COLOR_ERROR, "Error: Player doesn't exist, you need to use a full name.");
		if(GetPlayerID(splayer) == playerid) return SendClientMessage(playerid, COLOR_ERROR, "Error: You cannot transfer money yourself.");
		if(PlayerInfo[playerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_ERROR, "Error: You need be level 3 to transfer money.");
		if(MoneyACheck(playerid, money, 1, 50000000)) return 1;
		new tax = money/100;
		if(tax > 10000) tax = 10000;
		if(PlayerInfo[playerid][pAccount] < (money+tax)) return SendClientMessage(playerid, COLOR_ERROR, "Error: You don't have enough money in bank.");

		format(gString, sizeof(gString), "You have transfered $%s to %s with a tax of $%s.", FormatNumber(money), GetPlayerNameBySQLID(GetSQLIDByPlayerName(splayer)), FormatNumber(tax));
		SendClientMessage(playerid, COLOR_SYN2, gString);
		if(GetPlayerID(splayer) != INVALID_PLAYER_ID) {
			format(gString, sizeof(gString), "You have recieved $%s from %s.", FormatNumber(money), GetName(playerid));
			SendClientMessage(GetPlayerID(splayer), COLOR_SYN2, gString);
			PlayerInfo[GetPlayerID(splayer)][pAccount] += money;
			Update(GetPlayerID(splayer),pAccountx);
			PlayerInfo[playerid][pAccount] -= (money + tax);
			Update(playerid,pAccountx);
		}
		else {
			mysql_format(Database, gQuery, sizeof(gQuery), "UPDATE `users` SET `Account`=`Account` + %d WHERE `Name` = '%s'", money, splayer);
			mysql_query(Database, gQuery);
			PlayerInfo[playerid][pAccount] -= (money + tax);
			Update(playerid,pAccountx);
		}

		BizzInfo[2][bSafe] += tax;
		bUpdate(2, bSafex);
		BizzPayLog(playerid, 2, tax);

		format(gString, sizeof(gString), "%s [user:%d] transfered $%s to %s [user:%d].", GetName(playerid), PlayerInfo[playerid][pSQLID], FormatNumber(money), GetPlayerNameBySQLID(GetSQLIDByPlayerName(splayer)), GetSQLIDByPlayerName(splayer));
		ABroadCast(COLOR_YELLOW, gString, 1);

  		TransferLog(playerid, splayer, money);
	}
	return 1;
}

